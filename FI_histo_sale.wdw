   -
     name : CAParAnnée
     procedure_id : 2226135678854013297
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // CAParAnnée (<AnnE>)
      //
      // Paramètres :
      // 	AnnE : <indiquez ici le rôle de AnnE>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE CAParAnnée(AnnE)
      
      sChdebut , sChFin est une chaîne
      dAteDeb, dDateFin sont des dates
      sChdebut = ChaîneConstruit("0101%1",AnnE)
      dAteDeb = ChaîneVersDate(sChdebut,"AAAAMMJJ")
      
      grSupprimeSérie(GRF_histo)
      grSupprimeTout(GRF_histo)
      grTitre(GRF_histo,ChaîneConstruit("Vente de l'année %1",AnnE))
      colorSerie()
      POUR nDeb = 1 À 12
      	dAteDeb..Jour = "01"
      	dAteDeb..Mois = nDeb
      	dAteDeb..Année = AnnE
      	dDateFin = DernierJourDuMois(dAteDeb)
      	sChFin = DateVersMoisEnLettre(dAteDeb)
      	
      	REQ_Select_Ca_année.Param_date_order_start	= dAteDeb
      	REQ_Select_Ca_année.Param_date_order_end	= dDateFin
      	REQ_Select_Ca_année.Param_IDSociety			= gnID_societe
      	soit nrc = HExécuteRequête(REQ_Select_Ca_année)
      	si pas nrc ALORS
      		HErreurInfo(hErrComplet)
      	sinon
      		pour tout REQ_Select_Ca_année
      			grAjouteDonnée(GRF_histo,1,REQ_Select_Ca_année.la_somme_Total_TTC)
      			grAjouteDonnée(GRF_histo,2,REQ_Select_Ca_année.la_somme_S_total_HT)
      			grAjouteDonnée(GRF_histo,3,REQ_Select_Ca_année.la_somme_Discount)
      			grAjouteDonnée(GRF_histo,4,REQ_Select_Ca_année.la_somme_Taxe)
      		FIN
      	FIN
      	
      	REQ_SELECT_SaleMethodePaymentUsed.Param_IDSociety	= gnID_societe
      	nrc= HExécuteRequête(REQ_SELECT_SaleMethodePaymentUsed)
      	SI pas nrc ALORS
      		HErreurInfo(hErrComplet)
      	sinon
      //		taBleIDPayment est un tableau associatif d'entiers sur 8 octets
      			soit nNumserie = 5
      			pour tout  REQ_SELECT_SaleMethodePaymentUsed
      //				taBleIDPayment[REQ_SELECT_SaleMethodePaymentUsed.Journal] = REQ_SELECT_SaleMethodePaymentUsed.IDMoyen_paiement
      				REQ_Select_payment_methode_sale.Param_date_order_start	= dAteDeb
      				REQ_Select_payment_methode_sale.Param_date_order_end	= dDateFin
      				REQ_Select_payment_methode_sale.Param_IDMoyen_paiement	= REQ_SELECT_SaleMethodePaymentUsed.IDMoyen_paiement
      				REQ_Select_payment_methode_sale.Param_IDSociety			= gnID_societe
      				nrc = HExécuteRequête(REQ_Select_payment_methode_sale)
      				SI PAS nrc ALORS
      					HErreurInfo(hErrComplet)
      				SINON
      					POUR TOUT REQ_Select_payment_methode_sale 
      						grAjouteDonnée(GRF_histo,nNumserie,nDeb,REQ_Select_payment_methode_sale.la_somme_Payed)
      					FIN
      				FIN
      				grEtiquetteSérie(GRF_histo,nNumserie,REQ_SELECT_SaleMethodePaymentUsed.Journal)
      				nNumserie++
      			FIN
      	FIN
      	
      //	grEtiquetteSérie(GRF_histo,1,"C A TTC")
      //	grEtiquetteSérie(GRF_histo,2,"C A HT")
      //	grEtiquetteSérie(GRF_histo,3,"Remise")
      //	grEtiquetteSérie(GRF_histo,4,"Taxe")
      	grEtiquetteCatégorie(GRF_histo,nDeb,sChFin)
      FIN
      
      grEtiquetteSérie(GRF_histo,1,"C A TTC")
      grEtiquetteSérie(GRF_histo,2,"C A HT")
      grEtiquetteSérie(GRF_histo,3,"Remise")
      grEtiquetteSérie(GRF_histo,4,"Taxe")
      
      grTypeSérie(GRF_histo, 1, grHistoEmpilé)
      grTypeSérie(GRF_histo, 2, grHistoEmpilé)
      grTypeSérie(GRF_histo, 3, grHistoEmpilé)
      grTypeSérie(GRF_histo, 4, grHistoEmpilé)
      
      grDessine(GRF_histo)
     type : 458752
