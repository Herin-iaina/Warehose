     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_uom ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_uom()
      
      gSql_select_uom est une requête SQL = 
      [
      	SELECT <§cs:6§>
      	unit_of_mesur.IDunit_of_mesur,<§cs:6§>
      	unit_of_mesur.Name,<§cs:6§>
      	unit_of_mesur.ratio<§cs:6§>
      	FROM<§cs:6§>
      	unit_of_mesur<§cs:6§>
      	WHERE<§cs:6§>
      	unit_of_mesur.IDSociety = {Param_societID} <§cs:6§>
      ]
      
      gSql_select_uom.Param_societID = gnID_societe
      soit nrc = HExécuteRequêteSQL(gSql_select_uom )
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	ListeSupprimeTout(COMBO_uom_puchase)
      	
      	POUR TOUT gSql_select_uom
      		soit nIndcs = TableAjouteLigne(COMBO_uom_puchase)
      		COMBO_uom_puchase[nIndcs].COL_IDuom_p	= gSql_select_uom.IDunit_of_mesur
      		COMBO_uom_puchase[nIndcs].COL_uom		= gSql_select_uom.Name
      		COMBO_uom_puchase[nIndcs].COL_ratio_p	= gSql_select_uom.ratio
      	FIN
      FIN
