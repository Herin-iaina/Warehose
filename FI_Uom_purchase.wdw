           code : |1-
            SAI_SansNom1 = COMBO_uom_puchase.COL_uom
            
            gIndcs = FEN_Pruchase_order.TABLE_purchase_order_line.Select()
            FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_RatioUoM	= COMBO_uom_puchase.COL_ratio_p
            FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_ID_uom		= COMBO_uom_puchase.COL_IDuom_p
            
            //calcul le prix unitaire HT
            soit rPrix_compute = 0.00
            soit ratio_fix = FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_RatioUomPuchase
            soit ratio_var = COMBO_uom_puchase.COL_ratio_p
            soit prix_p    = FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Prix_p
            rPrix_compute = prix_p *  ratio_var / ratio_fix
            FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Prix_HT = rPrix_compute
            
            // calcul de sous total
            soit Prix_unit_price = FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Prix_HT
            soit Quantite        = FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Quantité
            soit discount_p		 = FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Remise / 100
            soit subtotal_line   = Prix_unit_price * (1- discount_p) * Quantite
            FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Sous_total_HT = subtotal_line
            
            FEN_Pruchase_order.Compute_Purchase_Price_HT()
            FEN_Pruchase_order.Compute_subtotal_line()
            FEN_Pruchase_order.Compute_taxe()
            FEN_Pruchase_order.Compute_total()
