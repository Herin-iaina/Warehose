  procedures :
   -
     name : Echo_lot
     procedure_id : 2208330969985485754
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Echo_lot ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_lot()
      
      
      sql_data est une requête SQL = 
      [
      	SELECT 
      	Available_quantity.IDAvailable_quantity AS IDAvailable_quantity,	
      	Available_quantity.Qantity AS Qantity,	
      	Available_quantity.IDArticle AS IDArticle,
      	Available_quantity.IDLot AS id_lot,	
      	Article.name_product AS name_product,
      	Article.Tracability as 	Tracability,
      	unit_of_mesur.Name AS Name_uom,	
      	Lot.Name AS Lot_Name
      	FROM 
      	(
      	(
      	Available_quantity
      	INNER JOIN
      	Article
      	ON Available_quantity.IDArticle = Article.IDArticle
      	)
      	INNER JOIN
      	unit_of_mesur
      	ON Article.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur
      	)
      	LEFT OUTER JOIN
      	Lot
      	ON Available_quantity.IDLot = Lot.IDLot
      	WHERE 
      	(
      	Article.IDArticle = {Id_Produit}
      	AND
      	Available_quantity.IDWarehouse = {Param_IdStock}
      	AND Available_quantity.Qantity > 0
      	)
      	ORDER BY  Lot.Expire_date DESC
      ]
      sql_data.Id_Produit		= COL_ID_art
      sql_data.Param_IdStock	= COL_IDWarehouse
      
      soit nrc = HExécuteRequêteSQL(sql_data,COL_ProcéduresGlobales.MaConnexion,hRequêteDéfaut+hRequêteSansCorrection)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON si HNbEnr(sql_data) > 0
      	ListeSupprimeTout(COMBO_Lot)
      	soit Tracability  est un booléen
      	POUR TOUT sql_data
      		soit nIndcs = TableAjouteLigne(COMBO_Lot)
      		COMBO_Lot[nIndcs].COL_ID_lot = sql_data.id_lot
      		COMBO_Lot[nIndcs].COL_Lot	 = sql_data.Lot_Name
      		COMBO_Lot[nIndcs].COL_Qte	 = sql_data.Qantity
      		COMBO_Lot[nIndcs].COL_ID_stock	= sql_data.IDAvailable_quantity
      		Tracability 			= sql_data.Tracability
      	FIN
      	BTN_categorie..Visible = Tracability
      	COMBO_Lot = 1
      	select_lot()
      FIN
     type : 458752
