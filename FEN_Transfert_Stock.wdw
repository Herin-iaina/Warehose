     name : Echo_warehouse
     procedure_id : 2207986032512722332
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_warehouse ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_warehouse()
      
      //REQ_select_warehouse_user.Param_IDSociety = gnID_societe
      //soit nrc = HExécuteRequête(REQ_select_warehouse_user)
      //SI PAS nrc ALORS
      //	HErreurInfo(hErrComplet)
      //SINON
      //	ListeAffiche(COMBO_warehouse)
      //	ListeAffiche(COMBO_destination)
      //FIN
      
      sqlWarehouse est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Warehouse.IDWarehouse AS IDWarehouse_,	<§cs:3§>
      	Warehouse.Name AS Name_,	<§cs:3§>
      	Warehouse.IDSociety AS IDSociety_,	<§cs:3§>
      	Warehouse.Type_Empacement AS Type_Empacement_<§cs:3§>
      FROM <§cs:3§>
      	Warehouse<§cs:3§>
      WHERE <§cs:3§>
      	Warehouse.IDSociety = {Param_IDSociety}<§cs:3§>
      	AND	Warehouse.Type_Empacement = 1<§cs:3§>
      ]
      sqlWarehouse.Param_IDSociety = gnID_societe
      soit Nrc = HExécuteRequêteSQL(sqlWarehouse)
      SI PAS Nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	ListeSupprimeTout(COMBO_warehouse)
      	ListeSupprimeTout(COMBO_destination)
      	pour TOUT sqlWarehouse 
      		soit indcsW = TableAjouteLigne(COMBO_warehouse)
      		COMBO_warehouse[indcsW].COL_IDWarehouse		= sqlWarehouse.IDWarehouse_
      		COMBO_warehouse[indcsW].COL_Name			= sqlWarehouse.Name_
      		soit indcsD = TableAjouteLigne(COMBO_destination)
      		COMBO_destination[indcsD].COL_IDWarehouse	= sqlWarehouse.IDWarehouse_
      		COMBO_destination[indcsD].COL_Name			= sqlWarehouse.Name_
      	FIN
      	
      FIN
     type : 458752
   -
     name : Echo_article
     procedure_id : 2208279632225060721
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Echo_article ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	COMBO_Article : <indiquez ici le rôle de COMBO_Article>
      PROCEDURE Echo_article()
      
      sql_article est une requête SQL = 
      [
      	SELECT DISTINCT<§cs:3§>
      	Article.name_product AS name_product,<§cs:3§>
      	Article.IDArticle AS ID_articl<§cs:3§>
      	FROM <§cs:3§>
      	(<§cs:3§>
      	Available_quantity<§cs:3§>
      	INNER JOIN<§cs:3§>
      	Article<§cs:3§>
      	ON Available_quantity.IDArticle = Article.IDArticle<§cs:3§>
      	)<§cs:3§>
      	<§cs:3§>
      	WHERE <§cs:3§>
      	(<§cs:3§>
      	Available_quantity.IDWarehouse = {Param_IdStock}<§cs:3§>
      	AND Available_quantity.Qantity > 0<§cs:3§>
      	)<§cs:3§>
      	GROUP BY<§cs:3§>
      	name_product,<§cs:3§>
      	ID_articl<§cs:3§>
      ]
      sql_article.Param_IdStock = COMBO_warehouse.COL_IDWarehouse
      soit nrc= HExécuteRequêteSQL(sql_article)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	ListeSupprimeTout(TABLE_transfert.COMBO_Article)
      	POUR TOUT sql_article
      		soit nIndcs = TableAjouteLigne(TABLE_transfert.COMBO_Article) 
      		TABLE_transfert.COMBO_Article[nIndcs].COL_Article		= sql_article.name_product
      		TABLE_transfert.COMBO_Article[nIndcs].COL_ID_art		= sql_article.ID_articl
      	FIN	
      FIN
      HLibèreRequête(sql_article)
     type : 458752
