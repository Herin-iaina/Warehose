  -
    name : BTN_Valider
    identifier : 0x2ea44c992e0aea50
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         si COMBO_warehouse.COL_IDWarehouse <=0 _ou_ COMBO_destination.COL_IDWarehouse <= 0 ALORS
         	ToastSupprimeTout()
         	ToastAffiche("Veillez remplir les emplacement source et destination ",toastCourt,cvMilieu,chCentre,BleuFoncé)
         	retour
         FIN
         
         soit nOccrs = TABLE_transfert..Occurrence
         
         si nOccrs > 0 ALORS
         	//Ajout de transfert
         	sql_transfert_detail est une requête SQL = 
         [
         	insert into <§cs:3§>
         	Transfert_internal<§cs:3§>
         	(date_move,Description,IDWarehouse,IDdestination,IDSociety,IDUser)<§cs:3§>
         	values<§cs:3§>
         	({dttransf},{Desc},{Idsource},{ID_destination},{SocietID},{UserID})<§cs:3§>
         ]	
         	soit dhdate est une dateheure
         	dhdate..PartieDate = DateDuJour()
         	dhdate..PartieHeure = Maintenant()
         	sql_transfert_detail.dttransf	= dhdate
         	sql_transfert_detail.Desc		= "Transfert interne du " + dhdate..PartieDate
         	sql_transfert_detail.Idsource	= COMBO_warehouse.COL_IDWarehouse
         	sql_transfert_detail.ID_destination	= COMBO_destination.COL_IDWarehouse
         	sql_transfert_detail.SocietID	= gnID_societe
         	sql_transfert_detail.UserID		= gnActif_user_id
         	soit nrc = HExécuteRequêteSQL(sql_transfert_detail)
         	si pas nrc ALORS
         		HErreurInfo(hErrComplet)
         	FIN
         	HLibèreRequête(sql_transfert_detail)
         	HLitDernier(Transfert_internal,IDTransfert_internal)
         	soit IDTranfert = Transfert_internal.IDTransfert_internal
         	
         	//Mise à jour de quantité de l'emplacement source
         	Sql_Select_Qte_Source est une requête SQL = 
         [
         	select <§cs:3§>
         	Available_quantity.Qantity<§cs:3§>
         	from<§cs:3§>
         	Available_quantity<§cs:3§>
         	where<§cs:3§>
         	Available_quantity.IDAvailable_quantity = {ID_Avalaible}<§cs:3§>
         ]
         	Sql_Upadte_Source est une requête SQL = 
         [
         	Update <§cs:3§>
         	Available_quantity<§cs:3§>
         	set<§cs:3§>
         	Available_quantity.Qantity = {Source_qte}<§cs:3§>
         	WHERE<§cs:3§>
         	Available_quantity.IDAvailable_quantity = {ID_Avalaible}<§cs:3§>
         ]
         	//ajout de mouvement de stock
         	Qsl_add_stock_move est une requête SQL = 
         	[
         			INSERT INTO<§cs:3§>
         			Stock_move<§cs:3§>
         			(date_move,Ref_move,Qantity,IDAvailable_quantity,IDArticle,IDSource,IDestination,Qantity_initial,Ecart,IDUser,IDTransfert_internal)<§cs:3§>
         			VALUES<§cs:3§>
         			({Date_Mvt},{ref},{Qte},{id_Available},{Id_product},{SourceID},{DestinationID},{Qte_init},{Ecart},{user},{ID_Transf})<§cs:3§>
         	]
         	
         	//Mise à jour de la quantité de l'emplacement destination
         	Sql_Select_Qte_destination est une requête SQL = 
         	[
         	SELECT <§cs:3§>
         	Available_quantity.Qantity As Quantite,<§cs:3§>
         	Available_quantity.IDAvailable_quantity as IDAvailable_quantity<§cs:3§>
         	FROM<§cs:3§>
         	Available_quantity<§cs:3§>
         	WHERE<§cs:3§>
         	Available_quantity.IDArticle = {ID_Art}<§cs:3§>
         	and<§cs:3§>
         	Available_quantity.IDLot	= {ID_lot}<§cs:3§>
         	and<§cs:3§>
         	Available_quantity.IDWarehouse = {StockID}<§cs:3§>
         	]
         	//Ajout de la quantité de l'emplacement de destination
         	SQL_add_Quanity_dest est une requête SQL = 
         [
         	INSERT INTO Available_quantity<§cs:3§>
         	( <§cs:3§>
         		IDArticle,	<§cs:3§>
         		IDLot,	<§cs:3§>
         		IDWarehouse,	<§cs:3§>
         		Qantity<§cs:3§>
         	)<§cs:3§>
         VALUES<§cs:3§>
         	( <§cs:3§>
         		{Param_IDArticle},	<§cs:3§>
         		{Param_IDLot},	<§cs:3§>
         		{Param_IDWarehouse},	<§cs:3§>
         		{Param_Qantity}<§cs:3§>
         	 )<§cs:3§>
         ]
         
         	POUR TOUTE LIGNE DE TABLE_transfert
         		
         		soit rQte_source_init = 0.00
         		soit rQte_source_final = 0.00
         		
         		//Mise à jour de quantité de l'emplacement source
         		Sql_Select_Qte_Source.ID_Avalaible	= TABLE_transfert.COL_Id_available
         		nrc = HExécuteRequêteSQL(Sql_Select_Qte_Source,COL_ProcéduresGlobales.MaConnexion,hRequêteDéfaut+hRequêteSansCorrection)
         		si pas nrc ALORS
         			HErreurInfo(hErrComplet)
         		sinon 
         			pour TOUT Sql_Select_Qte_Source 
         				rQte_source_init = Sql_Select_Qte_Source.Qantity
         			FIN
         			
         			rQte_source_final = rQte_source_init - TABLE_transfert.COL_Quantité_à_transferer
         			
         			//Mise à jour
         			Sql_Upadte_Source.ID_Avalaible	= TABLE_transfert.COL_Id_available
         			Sql_Upadte_Source.Source_qte	= rQte_source_final
         			nrc = HExécuteRequêteSQL(Sql_Upadte_Source)
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			fin
         			HLibèreRequête(Sql_Upadte_Source)
         			// mouvement
         			Qsl_add_stock_move.Date_Mvt		= dhdate
         			Qsl_add_stock_move.ref			= "Transfert interne du " + dhdate..PartieDate
         			Qsl_add_stock_move.Qte			= TABLE_transfert.COL_Quantité_à_transferer
         			Qsl_add_stock_move.id_Available	= TABLE_transfert.COL_Id_available
         			Qsl_add_stock_move.Id_product	= TABLE_transfert.COL_Id_article
         			Qsl_add_stock_move.SourceID		= COMBO_warehouse.COL_IDWarehouse
         			Qsl_add_stock_move.DestinationID= COMBO_destination.COL_IDWarehouse
         			Qsl_add_stock_move.Qte_init		= rQte_source_init
         			Qsl_add_stock_move.Ecart		= COL_Quantité_à_transferer * -1
         			Qsl_add_stock_move.user			= gnActif_user_id
         			Qsl_add_stock_move.ID_Transf	= IDTranfert
         			nrc = HExécuteRequêteSQL(Qsl_add_stock_move)
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			FIN
         			HLibèreRequête(Qsl_add_stock_move)
         		FIN		
         		HLibèreRequête(Sql_Select_Qte_Source)
         	
         		// destination
         		soit rQte_init_dest  = 0.00
         		soit rQte_final_dest = 0.00
         		Soit rID_Available_dest = 0.00
         		Sql_Select_Qte_destination.ID_Art = TABLE_transfert.COL_Id_article
         		Sql_Select_Qte_destination.ID_lot = TABLE_transfert.COL_ID_lot
         		Sql_Select_Qte_destination.StockID= COMBO_destination.COL_IDWarehouse
         		nrc = HExécuteRequêteSQL(Sql_Select_Qte_destination)
         		SI PAS nrc ALORS
         			HErreurInfo(hErrComplet)
         		sinon si HNbEnr(Sql_Select_Qte_destination) > 0
         			//Update
         			pour tout Sql_Select_Qte_destination
         				rQte_init_dest 		= Sql_Select_Qte_destination.Quantite
         				rID_Available_dest	= Sql_Select_Qte_destination.IDAvailable_quantity
         			FIN
         			rQte_final_dest = COL_Quantité_à_transferer + rQte_init_dest
         			Sql_Upadte_Source.ID_Avalaible	= rID_Available_dest
         			Sql_Upadte_Source.Source_qte	= rQte_final_dest
         			nrc = HExécuteRequêteSQL(Sql_Upadte_Source)
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			FIN
         			HLibèreRequête(Sql_Upadte_Source)
         			
         			// mouvement
         			Qsl_add_stock_move.Date_Mvt		= dhdate
         			Qsl_add_stock_move.ref			= "Transfert interne du " + dhdate..PartieDate
         			Qsl_add_stock_move.Qte			= TABLE_transfert.COL_Quantité_à_transferer
         			Qsl_add_stock_move.id_Available	= rID_Available_dest
         			Qsl_add_stock_move.Id_product	= TABLE_transfert.COL_Id_article
         			Qsl_add_stock_move.SourceID		= COMBO_warehouse.COL_IDWarehouse
         			Qsl_add_stock_move.DestinationID= COMBO_destination.COL_IDWarehouse
         			Qsl_add_stock_move.Qte_init		= rQte_init_dest
         			Qsl_add_stock_move.Ecart		= COL_Quantité_à_transferer
         			Qsl_add_stock_move.user			= gnActif_user_id
         			Qsl_add_stock_move.ID_Transf	= IDTranfert
         			nrc = HExécuteRequêteSQL(Qsl_add_stock_move)
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			FIN
         			HLibèreRequête(Qsl_add_stock_move)
         			
         		SINON SI HNbEnr(Sql_Select_Qte_destination) = 0
         			//Ajout
         			SQL_add_Quanity_dest.Param_IDArticle	= TABLE_transfert.COL_Id_article
         			SQL_add_Quanity_dest.Param_IDLot		= TABLE_transfert.COL_ID_lot
         			SQL_add_Quanity_dest.Param_IDWarehouse	= COMBO_destination.COL_IDWarehouse
         			SQL_add_Quanity_dest.Param_Qantity		= COL_Quantité_à_transferer
         			nrc = HExécuteRequêteSQL(SQL_add_Quanity_dest)
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			FIN
         			HLibèreRequête(SQL_add_Quanity_dest)
         			
         			HLitDernier(Available_quantity,IDAvailable_quantity)
         			rID_Available_dest = Available_quantity.IDAvailable_quantity
         			
         			// mouvement
         			Qsl_add_stock_move.Date_Mvt		= dhdate
         			Qsl_add_stock_move.ref			= "Transfert interne du " + dhdate..PartieDate
         			Qsl_add_stock_move.Qte			= TABLE_transfert.COL_Quantité_à_transferer
         			Qsl_add_stock_move.id_Available	= rID_Available_dest
         			Qsl_add_stock_move.Id_product	= TABLE_transfert.COL_Id_article
         			Qsl_add_stock_move.SourceID		= COMBO_warehouse.COL_IDWarehouse
         			Qsl_add_stock_move.DestinationID= COMBO_destination.COL_IDWarehouse
         			Qsl_add_stock_move.Qte_init		= rQte_init_dest
         			Qsl_add_stock_move.Ecart		= COL_Quantité_à_transferer
         			Qsl_add_stock_move.user			= gnActif_user_id
         			Qsl_add_stock_move.ID_Transf	= IDTranfert
         			nrc = HExécuteRequêteSQL(Qsl_add_stock_move)
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			FIN
         			HLibèreRequête(Qsl_add_stock_move)
         			
         		FIN
         		HLibèreRequête(Sql_Select_Qte_destination)
         	fin
         	
         FIN
         ToastSupprimeTout()
         ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
         FEN_Transfert_Stock..Grisé = Vrai
         Close_window()
