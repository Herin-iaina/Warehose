   -
     name : Echo_article
     procedure_id : 2215007869879354362
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Echo_article ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	COMBO_Article : <indiquez ici le rôle de COMBO_Article>
      PROCEDURE Echo_article()
      
      sql_article est une requête SQL = 
      [
      	SELECT DISTINCT<§cs:3§>
      	Article.name_product AS name_product,<§cs:3§>
      	Article.IDArticle AS ID_article,<§cs:3§>
      	article.Type_product as Type_product,<§cs:3§>
      	unit_of_mesur.Name as UOM<§cs:3§>
      	FROM <§cs:3§>
      	Article<§cs:3§>
      	inner join<§cs:3§>
      	unit_of_mesur<§cs:3§>
      	on Article.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur<§cs:3§>
      	WHERE <§cs:3§>
      	(<§cs:3§>
      	article.IDSociety = {SocietID}<§cs:3§>
      	and Article.Type_product not like 'Stockable'<§cs:3§>
      	AND Article.Stat = 1<§cs:3§>
      	)<§cs:3§>
      ]
      
      sql_articleStockable est une requête SQL = 
      [
      	SELECT DISTINCT<§cs:3§>
      	Article.name_product AS name_product,<§cs:3§>
      	Article.IDArticle AS ID_article,<§cs:3§>
      	Article.Type_product AS Type_product,<§cs:3§>
      	unit_of_mesur.Name AS UOM<§cs:3§>
      	FROM <§cs:3§>
      	(<§cs:3§>
      	Available_quantity<§cs:3§>
      	INNER JOIN<§cs:3§>
      	Article<§cs:3§>
      	ON Available_quantity.IDArticle = Article.IDArticle<§cs:3§>
      	)<§cs:3§>
      	INNER JOIN<§cs:3§>
      	unit_of_mesur<§cs:3§>
      	ON Article.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur<§cs:3§>
      	WHERE <§cs:3§>
      	(<§cs:3§>
      	Available_quantity.IDWarehouse = {Param_IdStock}<§cs:3§>
      	AND Available_quantity.Qantity > 0<§cs:3§>
      	)<§cs:3§>
      	GROUP BY<§cs:3§>
      	name_product,<§cs:3§>
      	ID_article,<§cs:3§>
      	Type_product,<§cs:3§>
      	UOM<§cs:3§>
      ]
      
      
      sql_article.SocietID = gnID_societe
      soit nrc = HExécuteRequêteSQL(sql_article)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	ListeSupprimeTout(TABLE_Consommation.COMBO_Article)
      	POUR TOUT sql_article
      		soit nIndcs = TableAjouteLigne(TABLE_Consommation.COMBO_Article) 
      		TABLE_Consommation.COMBO_Article[nIndcs].COL_Article  = sql_article.name_product
      		TABLE_Consommation.COMBO_Article[nIndcs].COL_ID_art	  = sql_article.ID_article
      		TABLE_Consommation.COMBO_Article[nIndcs].col_Type	  = sql_article.Type_product
      		TABLE_Consommation.COMBO_Article[nIndcs].col_UOM	  = sql_article.UOM
      	FIN
      FIN
      HLibèreRequête(sql_article)
      
      sql_articleStockable.Param_IdStock = COMBO_warehouse.COL_IDWarehouse
      nrc = HExécuteRequêteSQL(sql_articleStockable)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	POUR TOUT sql_articleStockable 
      		soit nIndcs = TableAjouteLigne(TABLE_Consommation.COMBO_Article) 
      		TABLE_Consommation.COMBO_Article[nIndcs].COL_Article  = sql_articleStockable.name_product
      		TABLE_Consommation.COMBO_Article[nIndcs].COL_ID_art	  = sql_articleStockable.ID_article
      		TABLE_Consommation.COMBO_Article[nIndcs].col_Type	  = sql_articleStockable.Type_product
      		TABLE_Consommation.COMBO_Article[nIndcs].col_UOM	  = sql_articleStockable.UOM
      	FIN
      FIN
      HLibèreRequête(sql_articleStockable)
     type : 458752
