    name : BTN_Valider
    identifier : 0x2ebd49ec093f83fa
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : BQAAAAUAAABk4BtHoaEtU748TiwdZ3pwSfMCvdC/uL6RXHzzl7bP/1krveCPkXfSWpG/U14tmQlOEfb/EOUdvQgMl8ggWRTfXKpRl7HetL+H18BDlitSkFSIS2r4XPuF0EBXne/WBvolM498a69zlVo0k0J8XKtdg4ChuSUhsP40Gj23O6Z4dghhdF2UWoCycMSwXsPSy9bhY7+NdiB3
        code : |1+
         si COMBO_warehouse.COL_IDWarehouse <=0 ALORS
         	ToastSupprimeTout()
         	ToastAffiche("Veillez remplir l'emplacement source",toastCourt,cvMilieu,chCentre,BleuFoncé)
         	ComboOuvre(COMBO_warehouse)
         	retour
         FIN
         Sablier(Vrai)
         soit nOccrs = TABLE_Consommation..Occurrence
         
         si nOccrs > 0 ALORS 
         	
         	
         	//Ajout de conso
         	sql_conso_detail est une requête SQL = 
         [
         	insert into <§cs:3§>
         	Consommation<§cs:3§>
         	(Description,Create_date,IDSociety,IDUser,IDWarehouse)<§cs:3§>
         	values<§cs:3§>
         	({Desc},{dateConso},{SocietID},{UserID},{IdStock})<§cs:3§>
         ]	
         	soit dhdate est une dateheure
         	dhdate..PartieDate = DateDuJour()
         	dhdate..PartieHeure = Maintenant()
         	sql_conso_detail.dateConso	= dhdate
         	sql_conso_detail.Desc		= (SAI_desc ~="" ? "Consommation du " + dhdate..PartieDate sinon SAI_desc)
         	sql_conso_detail.SocietID	= gnID_societe
         	sql_conso_detail.UserID		= gnActif_user_id
         	sql_conso_detail.IdStock	= COMBO_warehouse.COL_IDWarehouse
         	soit nrc = HExécuteRequêteSQL(sql_conso_detail)
         	si pas nrc ALORS
         		HErreurInfo(hErrComplet)
         	FIN
         	HLibèreRequête(sql_conso_detail)
         	HLitDernier(Consommation,IDConsommation)
         	soit IDConso = Consommation.IDConsommation
         	
         	//Mise à jour de quantité de l'emplacement source
         	Sql_Select_Qte_Source est une requête SQL = 
         [
         	select <§cs:3§>
         	Available_quantity.Qantity<§cs:3§>
         	from<§cs:3§>
         	Available_quantity<§cs:3§>
         	where<§cs:3§>
         	Available_quantity.IDAvailable_quantity = {ID_Avalaible}<§cs:3§>
         ]
         	Sql_Upadte_Source est une requête SQL = 
         [
         	Update <§cs:3§>
         	Available_quantity<§cs:3§>
         	set<§cs:3§>
         	Available_quantity.Qantity = {Source_qte}<§cs:3§>
         	WHERE<§cs:3§>
         	Available_quantity.IDAvailable_quantity = {ID_Avalaible}<§cs:3§>
         ]
         	//ajout de mouvement de stock
         	Qsl_add_stock_move est une requête SQL = 
         	[
         			INSERT INTO<§cs:3§>
         			Stock_move<§cs:3§>
         			(date_move,Ref_move,Qantity,IDAvailable_quantity,IDArticle,IDSource,IDestination,Qantity_initial,Ecart,IDUser,IDConsommation)<§cs:3§>
         			VALUES<§cs:3§>
         			({Date_Mvt},{ref},{Qte},{id_Available},{Id_product},{SourceID},{DestinationID},{Qte_init},{Ecart},{user},{ID_conso})<§cs:3§>
         	]
         	
         	soit XId_destination est un entier sur 8 octets
         	// sélection emplacement de destination
         	sql_destination est une requête SQL = 
         	[
         	SELECT <§cs:3§>
         	Warehouse.IDWarehouse<§cs:3§>
         	FROM<§cs:3§>
         	Warehouse<§cs:3§>
         	WHERE<§cs:3§>
         	Warehouse.Type_Empacement = 2<§cs:3§>
         	AND<§cs:3§>
         	Warehouse.IDSociety = {SocietID}<§cs:3§>
         	]
         	sql_destination.SocietID = gnID_Ware_House
         	nrc = HExécuteRequêteSQL(sql_destination)
         	SI PAS nrc ALORS
         		HErreurInfo(hErrComplet)
         	SINON SI HNbEnr(sql_destination) > 0
         		POUR TOUT sql_destination
         			XId_destination = sql_destination.IDWarehouse
         		FIN
         	SINON SI HNbEnr(sql_destination) = 0
         		gSql_codes_Warehouse est une Requête SQL = 
         		[
         	INSERT INTO<§cs:3§>
         	Warehouse<§cs:3§>
         	(Name,Type_Empacement,Removal_strategy,IDSociety)<§cs:3§>
         	VALUES <§cs:3§>
         	('Stock client',2,'FIFO',{ID_societe})<§cs:3§>
         		]
         		gSql_codes_Warehouse.ID_societe = gnID_societe
         		
         		nrc = HExécuteRequêteSQL(gSql_codes_Warehouse)
         		SI PAS nrc ALORS
         			HErreurInfo(hErrComplet)
         		FIN
         		HLitDernier(Warehouse,IDWarehouse)
         		XId_destination = Warehouse.IDWarehouse
         		
         	FIN
         
         	POUR TOUTE LIGNE DE TABLE_Consommation
         		
         		soit rQte_source_init = 0.00
         		soit rQte_source_final = 0.00
         		
         		//Mise à jour de quantité de l'emplacement source
         		Sql_Select_Qte_Source.ID_Avalaible	= TABLE_Consommation.COL_Id_available
         		nrc = HExécuteRequêteSQL(Sql_Select_Qte_Source)
         		si pas nrc ALORS
         			HErreurInfo(hErrComplet)
         		sinon
         			pour tout Sql_Select_Qte_Source
         				rQte_source_init = Sql_Select_Qte_Source.Qantity
         			FIN
         			
         			rQte_source_final = rQte_source_init - TABLE_Consommation.COL_Quantité_à_transferer<§aad:0§>
         			//Mise à jour
         			Sql_Upadte_Source.ID_Avalaible	= TABLE_Consommation.COL_Id_available
         			Sql_Upadte_Source.Source_qte	= rQte_source_final
         			nrc = HExécuteRequêteSQL(Sql_Upadte_Source)
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			fin
         			HLibèreRequête(Sql_Upadte_Source)
         			// mouvement
         			Qsl_add_stock_move.Date_Mvt		= dhdate
         			Qsl_add_stock_move.ref			= (SAI_desc ~="" ? "Consommation du " + dhdate..PartieDate SINON ChaîneConstruit("%1 [Consommation]",SAI_desc))
         			Qsl_add_stock_move.id_Available	= TABLE_Consommation.COL_Id_available
         			Qsl_add_stock_move.Id_product	= TABLE_Consommation.COL_Id_article
         			Qsl_add_stock_move.SourceID		= COMBO_warehouse.COL_IDWarehouse
         			Qsl_add_stock_move.DestinationID= XId_destination
         			Qsl_add_stock_move.Qte			= COL_Quantité_à_transferer
         			Qsl_add_stock_move.Qte_init		= rQte_source_init
         			Qsl_add_stock_move.Ecart		= COL_Quantité_à_transferer * -1
         			Qsl_add_stock_move.user			= gnActif_user_id
         			Qsl_add_stock_move.ID_Conso		= IDConso
         			nrc= HExécuteRequêteSQL(Qsl_add_stock_move)
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			FIN
         			HLibèreRequête(Qsl_add_stock_move)
         		FIN		
         		HLibèreRequête(Sql_Select_Qte_Source)			
         	fin
         	
         FIN
         Sablier(Faux)
         ToastSupprimeTout()
         ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
         FEN_Consommation..Grisé = Vrai
         Close_window()
        type : 18
      -
        type : 161
        enabled : false
