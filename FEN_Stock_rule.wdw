               gnId_article = COMBO_Article.COL_ID_art
               si gnId_article <= 0 ALORS
               	ToastSupprimeTout()
               	ToastAffiche("Veillez ajouter un article",toastCourt,cvMilieu,chCentre,BleuFoncé)
               	retour
               FIN
               
               Si gbIs_new = Vrai ALORS
               	sql_Add_stock_rule est une requête SQL = 
               [
               	insert into 
               	Replenishment_rule
               	(Name,Quantity_min,Quantity_max,Init_date,End_date,Stat,IDArticle,IDSociety)
               	values
               	({Nam_rule},{Qt_min},{Qt_max},{DateDebut},{DateFin},{statue},{ArticleID},{SocietID})
               ]
               	sql_Add_stock_rule.Nam_rule		= SAI_Name_rule
               	sql_Add_stock_rule.Qt_min		= SAI_Quantity_min
               	sql_Add_stock_rule.Qt_max		= SAI_Quantity_max
               	sql_Add_stock_rule.DateDebut	= SAI_Init_date
               	sql_Add_stock_rule.DateFin		= (SAI_End_date = 99991231 ? SAI_End_date SINON 99991231)
               	sql_Add_stock_rule.statue		= INT_Stat
               	sql_Add_stock_rule.ArticleID	= gnId_article
               	sql_Add_stock_rule.SocietID		= gnID_societe
               	soit nrc = HExécuteRequêteSQL(sql_Add_stock_rule)
               	si pas nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	HLibèreRequête(sql_Add_stock_rule)
               	HLitDernier(Replenishment_rule,IDReplenishment_rule)
               	gID_Stock_rule = Replenishment_rule.IDReplenishment_rule
               	
               sinon
               	sql_update_rule est une requête SQL = 
               [
               	update 
               	Replenishment_rule
               	set
               	Name			= {Nam_rule},
               	Quantity_min	= {Qt_min},
               	Quantity_max	= {Qt_max},
               	Init_date		= {DateDebut},
               	End_date		= {DateFin},
               	Stat			= {statue},
               	IDArticle		= {ArticleID}
               	where
               	IDReplenishment_rule = {ID_Stock_rule}
               ]
               	sql_update_rule.Nam_rule	= SAI_Name_rule
               	sql_update_rule.Qt_min		= SAI_Quantity_min
               	sql_update_rule.Qt_max		= SAI_Quantity_max
               	sql_update_rule.DateDebut	= SAI_Init_date
               	sql_update_rule.DateFin		= (SAI_End_date = 99991231 ? SAI_End_date SINON 99991231)
               	sql_update_rule.statue		= INT_Stat
               	sql_update_rule.ArticleID	= gnId_article
               	sql_update_rule.ID_Stock_rule = gID_Stock_rule
               	soit nrc = HExécuteRequêteSQL(sql_update_rule)
               	si pas nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	HLibèreRequête(sql_update_rule)
               FIN
               
               Echo_data()
