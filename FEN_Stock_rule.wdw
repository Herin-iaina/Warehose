      PROCEDURE Echo_data()
      
      sql_Stock_rule est une requête SQL = 
      [
      	SELECT 
      	IDReplenishment_rule,
      	Name,	
      	Quantity_min,	
      	Quantity_max,	
      	Init_date,	
      	End_date,	
      	Stat,	
      	IDArticle
      	FROM
      	Replenishment_rule
      	WHERE
      	IDReplenishment_rule = {ID_Stock_rule}
      ]
      sql_Stock_rule.ID_Stock_rule = gID_Stock_rule
      soit nrc = HExécuteRequêteSQL(sql_Stock_rule)
      SI PAS nrc ALORS
