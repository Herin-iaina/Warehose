     name : Echo_article_new
     procedure_id : 2209443469787845174
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Echo_article_new ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_article_new()
      
      sql_article est une requête SQL = 
      [
      	SELECT DISTINCT
      	Article.name_product AS name_product,
      	Article.IDArticle AS ID_article,
      	unit_of_mesur.Name AS UoM
      	FROM 
      	Article
      	INNER JOIN
      	unit_of_mesur
      	ON Article.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur
      	WHERE 
      	Article.Type_product = 'Stockable'
      	AND
      	Article.IDSociety = {SocietID}
      	AND Article.IDArticle NOT IN (SELECT Replenishment_rule.IDArticle FROM Replenishment_rule)
      	order by name_product asc
      ]
      sql_article.SocietID = gnID_societe
      soit nrc = HExécuteRequêteSQL(sql_article)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON	
      	POUR TOUT sql_article
      		soit nIndcs = TableAjouteLigne(COMBO_Article) 
      		COMBO_Article[nIndcs].COL_Article = sql_article.name_product
      		COMBO_Article[nIndcs].COL_ID_art  = sql_article.ID_article
      	FIN
      FIN
