   -
     code : |1-
      
      
      
      PROCEDURE fen_param_outlook(LOCAL gnIDAgenda est un entier = -1)
      
      SI gnIDAgenda <> -1 ALORS
      	
      
      	// Recherche de l'agenda
      	HLitRecherchePremier(AgendaOutlook, IDAgendaOutlook, gnIDAgenda)	
      FIN
      BTN_Nouveau..Visible = Faux
      BTN_Modifier..Visible = Faux
      // Valeur de retour par défaut
      MaFenêtre..ValeurRenvoyée = Faux
   -
     code : |1+
      ListeProfils()
      
      // Affiche le profil
      FichierVersEcran()
      
      // En mode création
      SI gnIDAgenda = -1 ALORS
      	
      	// Sélectionne automatique le seul profil disponible, ou le cas échéant le premier élément de la liste
      	ListeSelectPlus(COMBO_Agd_outlook, (COMBO_Agd_outlook..Occurrence > 2 ? 1 SINON 2))
      FIN
      
      // 
      ExécuteTraitement(COMBO_Agd_outlook, trtSélection)
      
      
      
