  procedures :
   -
     name : ListeProfils
     procedure_id : 2200145156851244377
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //ListeProfils ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ListeProfils()
      
      
      // Récupère la liste des profils Outlook existant
      sListeProfils est une chaine = Outlook_compte()
      sProfil		  est une chaine
      
      // Si la récupération des profils a échoué
      SI ErreurDétectée ALORS
      	Erreur
      	Ferme
      FIN
      
      POUR TOUTE CHAÎNE sProfil DE sListeProfils SEPAREE PAR RC
      	
      	// On vérifie si on a déjà importé ce profil
      	sql_code est une requête SQL = 
      [
      	select 
      		IDAgendaOutlook
      	from
      		AgendaOutlook
      	where
      		Profil = {account} and IDUser = {user}
      ]
      	sql_code.account = sProfil
      	sql_code.user = gnActif_user_id
      	soit nrc = HExécuteRequêteSQL(sql_code)
      	si pas nrc ALORS
      		HErreurInfo(hErrComplet)
      	sinon
      		si HNbEnr(sql_code) < 1 ALORS
      			// Ajoute le profil à la liste
      			ListeAjoute(COMBO_Agd_outlook, gLien(sProfil) + sProfil)
      		sinon
      			ListeAjoute(COMBO_Agd_outlook, gLien("") + gStylo(iGrisClair) + sProfil + " (importé)")
      		FIN
      	FIN
      	
      FIN
      
      // Si on a plusieurs profils
      SI COMBO_Agd_outlook..Occurrence > 0 ALORS	
      	ListeInsère(COMBO_Agd_outlook, gLien("") + "<Sélectionnez...>", 1)		
      SINON
      	
      	// Affiche une erreur et ferme
      	Erreur("Outlook n'est pas installé sur cet ordinateur ou bien aucun profil Outlook n'a encore été créé")
      	Ferme()
      FIN
      
