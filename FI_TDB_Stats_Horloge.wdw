            	dLigne(IMG_DessinFuseau,nRayon + (Cosinus(nAngleAiguille) * nTailleHeure), nRayon + (Sinus(nAngleAiguille) * nTailleHeure), nRayon, nRayon, nCoulHeure, nEpaisseurHeure)
            	
            	// Grande aiguille
            	nAngleAiguille = (((hHeureFuseau..Minute * 60) + (hHeureFuseau..Seconde)) / 10) - 90
            	dLigne(IMG_DessinFuseau,nRayon + (Cosinus(nAngleAiguille) * nTailleMinute), nRayon + (Sinus(nAngleAiguille) * nTailleMinute), nRayon, nRayon, nCoulMinute, nEpaisseurMinute)
            	
            	// Trotteuse
            	nAngleAiguille = (hHeureFuseau..Seconde * 6) - 90
            	dLigne(IMG_DessinFuseau,nRayon + (Cosinus(nAngleAiguille) * nTailleSeconde), nRayon + (Sinus(nAngleAiguille) * nTailleSeconde),nRayon, nRayon, nCoulSeconde, nEpaisseurSeconde)
            	
            FIN
            
            // Dessine l'horloge une première fois
            DessineHorloge()
            
            // Rappelle le dessin d'horloge chaque seconde
            TimerSys(DessineHorloge,1s)
            
