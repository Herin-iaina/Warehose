     -
       name : IMG_Dessin
       identifier : 0x2db99ac0c0e27b87
       internal_properties : 
       type : 8
       code_elements :
        type_code : 8
        p_codes :
         -
           code : |1+
            
            // Paramètres de dessin
            nRayon 				est un entier = IMG_Dessin..Largeur / 2
            
            // Couleurs des aiguilles
            nCoulSeconde 		est un entier = RGB(220, 56, 43)
            nCoulMinute 		est un entier = RGB(55, 70, 73)
            nCoulHeure 			est un entier = RGB(55, 70, 73)
            nBlancTournant 		est un entier = 20
            
            // Taille et épaisseur des aiguilles
            nTailleHeure 		est un entier = 0.70 * nRayon - nBlancTournant
            nTailleMinute 		est un entier = 0.86 * nRayon - nBlancTournant
            nTailleSeconde 		est un entier = 0.96 * nRayon - nBlancTournant
            nEpaisseurHeure 	est un entier = 5
            nEpaisseurMinute 	est un entier = 3
            nEpaisseurSeconde 	est un entier = 1
            
            // Dessin de l'horloge
            PROCÉDURE INTERNE DessineHorloge
            	
            	// Initialisation du dessin
            	dDébutDessin(IMG_Dessin,dEffacer)
            	dChangeMode(IMG_Dessin,dessinAntiAliasing)	
            	
            	// Heure à dessiner
            	hHeureLocale est une heure	= heuresys()
            	IMG_Dessin..Bulle = HeureVersChaîne(hHeureLocale,"HH:MM:SS")
            	LIB_Heure = IMG_Dessin..Bulle	
            	
            	// Petite aiguille
            	nAngleAiguille est un entier = ((( (hHeureLocale..heure modulo 12) * 60) + (hHeureLocale..Minute)) / 2) - 90
            	dLigne(IMG_Dessin,nRayon + (Cosinus(nAngleAiguille) * nTailleHeure), nRayon + (Sinus(nAngleAiguille) * nTailleHeure), nRayon, nRayon, nCoulHeure, nEpaisseurHeure)
            
            	// Grande aiguille
            	nAngleAiguille = (((hHeureLocale..Minute * 60) + (hHeureLocale..Seconde)) / 10) - 90
            	dLigne(IMG_Dessin,nRayon + (Cosinus(nAngleAiguille) * nTailleMinute), nRayon + (Sinus(nAngleAiguille) * nTailleMinute), nRayon, nRayon, nCoulMinute, nEpaisseurMinute)
            
            	// Trotteuse
            	nAngleAiguille = (hHeureLocale..Seconde * 6) - 90
            	dLigne(IMG_Dessin,nRayon + (Cosinus(nAngleAiguille) * nTailleSeconde), nRayon + (Sinus(nAngleAiguille) * nTailleSeconde),nRayon, nRayon, nCoulSeconde, nEpaisseurSeconde)
            
            FIN
            
            // Dessine l'horloge une première fois
            DessineHorloge()
            
            // Rappelle le dessin d'horloge chaque seconde
            TimerSys(DessineHorloge,1s)
