            // Africa/Abidjan
            // Africa/Accra
            // Africa/Addis_Ababa
            // Africa/Algiers
            // Africa/Asmara
            // Africa/Bamako
            // etc
            LIB_Autre_fuseau = <§$0000§>
            
            // Paramètres de dessin
            nRayon 				est un entier = IMG_DessinFuseau..Largeur / 2
            
            // Couleurs des aiguilles
            nCoulSeconde 		est un entier = RGB(220, 56, 43)
            nCoulMinute 		est un entier = RGB(55, 70, 73)
            nCoulHeure 			est un entier = RGB(55, 70, 73)
            nBlancTournant 		est un entier = 20
            
            // Taille et épaisseur des aiguilles
            nTailleHeure 		est un entier = 0.70 * nRayon - nBlancTournant
            nTailleMinute 		est un entier = 0.86 * nRayon - nBlancTournant
            nTailleSeconde 		est un entier = 0.96 * nRayon - nBlancTournant
            nEpaisseurHeure 	est un entier = 5
            nEpaisseurMinute 	est un entier = 3
            nEpaisseurSeconde 	est un entier = 1
            
            // Dessin de l'horloge
