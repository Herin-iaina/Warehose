     -
       name : IMG_Agrandir
       identifier : 0x2f1a7cadaa8b5127
       internal_properties : 
       type : 8
       code_elements :
        type_code : 8
        p_codes : []
    type : 32
    code_elements :
     type_code : 70
     p_codes :
      -
        code : |1-
         PROCEDURE MaFenêtre()
         LIB_Profil_client_par_origine = ChaîneConstruit(LIB_Profil_client_par_origine,NuméroDeMoisVersLettre(MoisEnCours()))
      -
        code : |1-
         procédure interne Actualiser()
         	sqlVenteParVendeur est une requête SQL = 
         	[
         	SELECT 
         	SUM(Total_TTC) AS total,
         	User.Name as sellor
         	FROM 
         	Sale_order
         	inner JOIN user
         	ON Sale_order.IDUser = user.IDUser 
         	WHERE
         	Sale_order.IDSociety = {IDSociete}
         	AND
         	date_order BETWEEN {AAMM} AND {AAAAMM}	
         	GROUP BY
         	USER.Name
         	]
         	dDebutMoisEncours, dfinMoisEncours sont des dates
         	dDebutMoisEncours		= PremierJourDuMois(DateDuJour())	
         	dfinMoisEncours			= (dDebutMoisEncours..Mois < 12 ? PremierJourDuMois(dDebutMoisEncours..Année,dDebutMoisEncours..Mois + 1) SINON PremierJourDeLAnnée(dDebutMoisEncours..Année + 1))
         	
         	sqlVenteParVendeur.IDSociete	= gnID_societe
         	sqlVenteParVendeur.AAMM			= dDebutMoisEncours
         	sqlVenteParVendeur.AAAAMM		= dfinMoisEncours
         	
         	HExécuteRequêteSQL(sqlVenteParVendeur)
         	soit i = 1
         	grSupprimeTout(GRF_tunnel)
         	pour tout sqlVenteParVendeur
         		grAjouteDonnée(GRF_tunnel,1,i,sqlVenteParVendeur.total)
         		grEtiquetteCatégorie(GRF_tunnel,i,sqlVenteParVendeur.sellor)
         		i ++
         	FIN
         	grDessine(GRF_tunnel)
         fin
         
         Actualiser()
         Timer(Actualiser,Hasard(6,12)*200)
