      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	gLargeur_ : <indiquez ici le rôle de gLargeur_>
      PROCEDURE top_customer()
      
      REQ_select_top_cinq_client.Param_IDSociety = gnID_societe
      soit nrc = HExécuteRequête(REQ_select_top_cinq_client)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	nbCol = HNbEnr(REQ_select_top_cinq_client)
      	ZoneRépétéeSupprimeTout(ZR_client_top_five)
      	POUR TOUT REQ_select_top_cinq_client 
      		soit nIndcs = ZoneRépétéeAjouteLigne(ZR_client_top_five)
      		ZR_client_top_five[nIndcs].LIB_client	= REQ_select_top_cinq_client.Name
      		ZR_client_top_five[nIndcs].LIB_CA		= REQ_select_top_cinq_client.la_somme_Total_TTC
      		ZR_client_top_five[nIndcs].LIB_frond..CouleurFond = tabColors[nIndcs]
      	FIN
      FIN
     type : 458752
   -
     name : top_purchase
     procedure_id : 2223159210825591739
     type_code : 14
     code : |1-
