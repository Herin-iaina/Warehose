      PROCEDURE top_purchase()
      
      REQ_select_top_syplyer.Param_IDSociety = gnID_societe
      soit nrc = HExécuteRequête(REQ_select_top_syplyer)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	nbCol = HNbEnr(REQ_select_top_syplyer)
      	ZoneRépétéeSupprimeTout(ZR_client_top_five)
      	POUR TOUT REQ_select_top_syplyer 
      		soit nIndcs = ZoneRépétéeAjouteLigne(ZR_client_top_five)
      		ZR_client_top_five[nIndcs].LIB_client	= REQ_select_top_syplyer.Name
      		ZR_client_top_five[nIndcs].LIB_CA		= REQ_select_top_syplyer.la_somme_sub_ttl_ttc
      		ZR_client_top_five[nIndcs].LIB_frond..CouleurFond = tabColors[nIndcs]
      	FIN
      FIN
