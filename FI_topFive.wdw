        code : |1+
         PROCEDURE MaFenêtre(gLargeur_, gHauteur_, gxType_op sont des numériques)
         
         tabColors est un tableau d'entiers = [RGB(75,167,3),RVB(60,192,253),RVB(243,119,78),RVB(72,135,180),RVB(118,141,156),RVB(206,116,41),]
         soit nbCol = 0.00
         
         si gxType_op = 1 ALORS
         	top_customer()
         sinon si gxType_op = 2
         	top_purchase()
         FIN
         ZR_client_top_five..HauteurLigne 		= gHauteur_
         ZR_client_top_five.LIB_frond..Hauteur 	= gHauteur_
         si nbCol > 0 ALORS
         	ZR_client_top_five..LargeurColonne = gLargeur_ / nbCol	
         	ZR_client_top_five.LIB_frond..Largeur = ZR_client_top_five..LargeurColonne
         	ZR_client_top_five.LIB_CA..Largeur = ZR_client_top_five..LargeurColonne / 2
         FIN
      -
        code : |1+
         
         
         
         
        type : 14
      -
        code : |1+
         ZR_client_top_five..HauteurLigne 		= FI_topFive..Hauteur
