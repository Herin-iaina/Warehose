         PROCEDURE MaFenêtre()
      -
        code : |1+
         procédure interne Actualiser()
         	sqlRupturestock est une requête SQL = 
         [
         	select <§cs:6§>
         	count( distinct Article.IDArticle) as nombre,<§cs:6§>
         	SUM(Available_quantity.Qantity)<§cs:6§>
         	from<§cs:6§>
         	Article<§cs:6§>
         	inner join Available_quantity<§cs:6§>
         	on Article.IDArticle = Available_quantity.IDArticle<§cs:6§>
         	where<§cs:6§>
         	Article.IDSociety = {IDSociete}<§cs:6§>
         	and<§cs:6§>
         	article.Stat = 1<§cs:6§>
         	and<§cs:6§>
         	article.Type_product like '%Stockable%'<§cs:6§>
         	having<§cs:6§>
         	SUM(Available_quantity.Qantity)  <= 0		<§cs:6§>
         ]
         	sqlRupturestock.IDSociete = gnID_societe
         	soit bNrc = HExécuteRequêteSQL(sqlRupturestock)
         	si pas bNrc ALORS
         		HErreurInfo(hErrComplet)
         	sinon
         		pour TOUT sqlRupturestock 
         			LIB_135 = sqlRupturestock.nombre
         		FIN
         	FIN
         	HLibèreRequête(sqlRupturestock)
         fin
         
         Actualiser()
         Timer(Actualiser,Hasard(6,12)*200)
