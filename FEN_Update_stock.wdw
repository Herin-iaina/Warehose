    name : BTN_Valider
    identifier : 0x2e9e58a56e20127e
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         Ecart est un numérique 
         gnQt_P, gnLot_P,gnId_Available_Qt,gnId_stock sont des entiers = 0
         
         gnId_stock 	= COMBO_warehouse_user.COL_IDWarehouse
         gnLot_P 	= (gbTracabilité = Faux ? 0 SINON COMBO_lot.COL_IDLot)
         
         si gnId_stock = 0 ALORS
         	ToastSupprimeTout()
         	ToastAffiche("Ajouter un entrepôt",toastCourt,cvMilieu,chCentre,BleuFoncé)
         	ComboOuvre(COMBO_warehouse_user)
         	retour
         FIN
         si gbTracabilité = Vrai _et_ COMBO_lot.COL_IDLot <= 0 ALORS
         	ToastSupprimeTout()
         	ToastAffiche("Cet produit devrai avoir un lot",toastCourt,cvMilieu,chCentre,BleuFoncé)
         	ComboOuvre(COMBO_lot)
         	RETOUR
         FIN
         
         Sql_select_Available est une requête SQL = 
         [
         	select 
         	IDAvailable_quantity,
         	Available_quantity.Qantity
         	from
         	Available_quantity
         	where
         	Available_quantity.IDArticle = {Id_product}
         	and
         	Available_quantity.IDWarehouse = {Id_Stok}
         	and
         	Available_quantity.IDLot = {Id_lot}
         ]
         Sql_select_Available.Id_product	= gId_Produit
         Sql_select_Available.Id_Stok	= gnId_stock
         Sql_select_Available.Id_lot		= gnLot_P
         // vérifie s'il existe de quantité 
         soit nrc = HExécuteRequêteSQL(Sql_select_Available)
         si pas nrc ALORS
         	HErreurInfo(hErrComplet)
         sinon si HNbEnr(Sql_select_Available) > 0
         	pour tout Sql_select_Available 
         		gnId_Available_Qt = Sql_select_Available.IDAvailable_quantity
         		gnQt_P			  = Sql_select_Available.Qantity
         	FIN	
         FIN
         
         HLibèreRequête(Sql_select_Available)
         
         //Ajout de mouvement de stock
         Ecart = SAI_Qte - gnQt_P
         Qsl_add_stock_move est une requête SQL = 
         [
         	INSERT INTO
         	Stock_move
         	(date_move,Ref_move,Qantity,IDAvailable_quantity,IDArticle,IDSource,IDestination,Qantity_initial,Ecart,IDUser)
         	VALUES
         	({Date_Mvt},{ref},{Qte},{id_Available},{Id_product},{SourceID},{DestinationID},{Qte_init},{Ecart},{user})
         ]
         dhDatePc est une dateheure
         dhDatePc..PartieDate = DateDuJour()
         dhDatePc..PartieHeure= Maintenant()
         Qsl_add_stock_move.Date_Mvt			= dhDatePc
         Qsl_add_stock_move.ref				= "Ajustement de stock"
         Qsl_add_stock_move.Qte				= SAI_Qte
         Qsl_add_stock_move.id_Available		= gnId_Available_Qt
         Qsl_add_stock_move.Id_product		= gId_Produit
         Qsl_add_stock_move.SourceID			= gnId_stock
         Qsl_add_stock_move.DestinationID	= gnId_stock
         Qsl_add_stock_move.Qte_init			= gnQt_P
         Qsl_add_stock_move.Ecart			= Ecart
         Qsl_add_stock_move.user				= gnActif_user_id
         nrc = HExécuteRequêteSQL(Qsl_add_stock_move)
         SI PAS nrc ALORS
         	HErreurInfo(hErrComplet)
         FIN
         
         
         
         //Mise à jour de stock
         SI gnId_Available_Qt > 0 ALORS
         
         	sql_update_Stock est une requête SQL = 
         	[
         		UPDATE 
         		Available_quantity
         		SET
         		Qantity = {Qte}
         		WHERE
         		IDAvailable_quantity = {Id_Stock}
         	]
         	sql_update_Stock.Qte		= SAI_Qte
         	sql_update_Stock.Id_Stock 	= gnId_Available_Qt
         	nrc = HExécuteRequêteSQL(sql_update_Stock)
         	SI PAS nrc ALORS
         		HErreurInfo(hErrComplet)
         	FIN
         SINON
         	SQL_add_Qte est une requête SQL = 
         	[
         		INSERT INTO 
         		Available_quantity
         		(Qantity,IDArticle,IDLot,IDWarehouse)
         		VALUES
         		({Qte},{ID_Product},{LOtID},{StokId})
         	]
         	SQL_add_Qte.Qte			= SAI_Qte
         	SQL_add_Qte.ID_Product	= gId_Produit
         	SQL_add_Qte.LOtID		= gnLot_P
         	SQL_add_Qte.StokId		= gnId_stock
         	nrc = HExécuteRequêteSQL(SQL_add_Qte)
         	SI PAS nrc ALORS
         		HErreurInfo(hErrComplet)
         	FIN
         FIN
         
         ToastSupprimeTout()
         ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
         //HLibèreRequête()
         
         SAI_Qte = 0
         DonneFocus(SAI_Qte)
        type : 18
