   -
     code : |1-
      PROCEDURE MaFenêtre(gId_article,gID_uom,gnID_lot sont des entiers sur 8 octets,gxQte_done est un numérique, gbIs_new est un booléen,indcs_op est un entier)
      
      Echo_product()
      Echo_uom()
      Echo_lot()
      
      si gbIs_new = Vrai ALORS
      	GR_Article..EnSaisie	= Vrai
      	GR_UoM..EnSaisie		= Vrai
      	GR_Qte..Visible			= Vrai
      sinon
      	GR_Article..EnSaisie	= Faux
      	GR_UoM..EnSaisie		= Faux
      	GR_Qte..Visible			= Vrai
      //	FEN_operation_edit..Hauteur = FEN_operation_edit..Hauteur - 85
      	 SAI_Qty = gxQte_done
      	//Article
      	soit indcs_art = TableCherche(COL_IDArticle,gId_article,rechercheIdentique)
      	ListeSelectPlus(COMBO_product,indcs_art)
      	//UOM
      	soit Indice_uom = TableCherche(COL_IDunit_of_mesur,gID_uom,rechercheIdentique)
      	TableSelectPlus(COMBO_REQ_SELECT_UoM_0_id,Indice_uom)
      	//lot
      	si gnID_lot > 0 ALORS
      		soit Indice_lot = TableCherche(COL_IDLot,gnID_lot,rechercheIdentique)
      		TableSelectPlus(COMBO_lot,Indice_lot)
      	FIN
      	SI COMBO_product.COL_tracability = Faux ALORS
      		GR_Lot..Visible 	= Faux
      		FEN_operation_edit..Hauteur = FEN_operation_edit..Hauteur - 47
      	FIN
      FIN
