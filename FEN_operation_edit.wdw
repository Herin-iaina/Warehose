  procedures :
   -
     name : Echo_product
     procedure_id : 2222382264339587002
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_product ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_product()
      
      // afficher touts les articles dans le combo
      sql_art est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Article.IDArticle AS IDArticle,	<§cs:3§>
      	Article.name_product AS name_product,	<§cs:3§>
      	Article.Tracability AS Tracability,	<§cs:3§>
      	Article.IDunit_of_mesur AS IDunit_of_mesur,<§cs:3§>
      	unit_of_mesur.ratio as ratio<§cs:3§>
      FROM <§cs:3§>
      	Article<§cs:3§>
      	inner join<§cs:3§>
      	unit_of_mesur<§cs:3§>
      	on unit_of_mesur.IDunit_of_mesur = Article.IDunit_of_mesur<§cs:3§>
      WHERE <§cs:3§>
      	Article.Type_product = 'Stockable'<§cs:3§>
      	AND	Article.IDSociety = {Param_IDSociety}<§cs:3§>
      ]
      
      sql_art.Param_IDSociety = gnID_societe
      soit nrc = HExécuteRequêteSQL(sql_art)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      sinon
      	ListeSupprimeTout(COMBO_product)
      	pour tout sql_art
      		soit nIndcs = TableAjouteLigne(COMBO_product)
      		COMBO_product[nIndcs].COL_IDArticle		= sql_art.IDArticle
