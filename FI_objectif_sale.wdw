   -
     name : echo_data
     procedure_id : 2226103861736091817
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // echo_data ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE echo_data()
      
      JAUGE_JaugeObjectif..BorneMax 	= 100
      JAUGE_JaugeObjectif..BorneMin	= 0
      
      soit xTabLeValJauge = 0n 0.00
      xBormax = 200000
      xBorne = 50000
      SI xBormax > 0 ALORS
      	xTabLeValJauge = xBorne * 100 / xBormax
      FIN
      
      LIB_Objectif2 	= xBorne
      LIB_Objectif	= xBormax
      
      SI xTabLeValJauge <= 50 ALORS
      	JAUGE_JaugeObjectif..CouleurJauge 	= RVB(255,51,51)
      	LIB_Objectif1..Couleur				= RVB(255,51,51)
      SINON SI xTabLeValJauge >= 51 _ET_ xTabLeValJauge <= 75
      	JAUGE_JaugeObjectif..CouleurJauge 	= RVB(255,102,255)
      	LIB_Objectif1..Couleur				= RVB(255,102,255)
      SINON SI xTabLeValJauge >= 76	
      	JAUGE_JaugeObjectif..CouleurJauge 	= RVB(255,204,102)
      	LIB_Objectif1..Couleur				= RVB(255,204,102)
      FIN
      
      xTabLeValJauge = (xTabLeValJauge < 100 ? xTabLeValJauge sinon 100)
      
      POUR i = 0n 0.00 a xTabLeValJauge
      	JAUGE_JaugeObjectif = i
      	LIB_Objectif1 = i + "%"
      FIN
     type : 458752
