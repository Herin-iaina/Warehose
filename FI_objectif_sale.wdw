     name : Select_sale_objectif
     procedure_id : 2225820299405039678
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Select_sale_objectif ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Select_sale_objectif() <métier>
      
      xBojectif sont des numériques (32,2)  = 0n 0.00
      
      sql_select_objectif est une requête SQL = 
      [
      	SELECT
      	Parametre_vente.objectif_ca AS Objectifs 
      	FROM 
      	Parametre_vente
      	WHERE
      	Parametre_vente.IDSociety = {ParamSocietID}
      ]
      sql_select_objectif.ParamSocietID = gnID_societe
      
      soit nrc = HExécuteRequêteSQL(sql_select_objectif)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	POUR TOUT sql_select_objectif
      		xBojectif = sql_select_objectif.Objectifs
      	FIN
      FIN
      renvoyer xBojectif
     type : 458752
   -
     name : Select_sale_total
     procedure_id : 2225825496315553942
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // [ <Résultat> = ] Select_sale_total ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	numérique : <indiquez ici les valeurs possibles ainsi que leur interprétation>
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Select_sale_total() <métier>
      
      xCaRealiser sont des numériques (32,2)  = 0n 0.00
      dAteDebMois, dAteFinMois sont des dates
      dAteDebMois = PremierJourDuMois(DateDuJour())
      dAteFinMois	= DernierJourDuMois(DateDuJour())
      sql_select_total_sale est une requête SQL = 
      [
      	SELECT 
      	SUM(Sale_order.Total_TTC) AS Total_sale
      	FROM
      	Sale_order
      	WHERE
      	Sale_order.date_order BETWEEN {dateDeb} AND {dateEnd}
      	AND 
      	Sale_order.IDSociety = {ParamSocietID}
      ]
      sql_select_total_sale.dateDeb	= dAteDebMois
      sql_select_total_sale.dateEnd	= dAteFinMois
      sql_select_total_sale.ParamSocietID = gnID_societe
      soit nrc = HExécuteRequêteSQL(sql_select_total_sale)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	POUR TOUT sql_select_total_sale
      		xCaRealiser = sql_select_total_sale.Total_sale
      	FIN
      FIN
      RENVOYER xCaRealiser
