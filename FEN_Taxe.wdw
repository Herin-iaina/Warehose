               //Ajout ou modification selon  les contextes ajout avant 
               
               SI gbis_new = Vrai ALORS
               	SQl_Add_Taxe est une requête SQL = 
               	[
               	INSERT INTO 
               	Taxe
               	(Name_taxe,Valeur_taxe,type_taxe,IDSociety)
               	VALUES
               	({Name_taxe},{Val_taxe},{typtaxe},{SocietID})
               	]
               	SQl_Add_Taxe.Name_taxe = SAI_Nom
               	SQl_Add_Taxe.Val_taxe  = SAI_Valeur_taxe
               	SQl_Add_Taxe.typtaxe   = COMBO_Type..Valeur
               	SQl_Add_Taxe.SocietID = gnID_societe
               	soit nrc = HExécuteRequêteSQL(SQl_Add_Taxe)
               	SI PAS nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	HLitDernier(Taxe,IDTaxe)
               	TaxeID = Taxe.IDTaxe
               sinon
               	sql_updataxe est une requête SQL = 
               [
               	update 
               	taxe
               	set
               	Name_taxe 	= {Name_taxe},
               	Valeur_taxe	= {Val_taxe},
               	type_taxe	= {typtaxe}
               	where
               	IDTaxe 		= {TaxID}
               ]
               	sql_updataxe.Name_taxe = SAI_Nom
               	sql_updataxe.Val_taxe  = SAI_Valeur_taxe
               	sql_updataxe.typtaxe   = COMBO_Type..Valeur
               	sql_updataxe.TaxID	   = TaxeID
               	soit nrc = HExécuteRequêteSQL(sql_updataxe)
               	SI pas nrc  ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               FIN
               
               Echo_data()
               GR_All_field..EnSaisie = Faux
               BTN_Modifier..Visible = Vrai
               gbCheck_update = Faux
               gbis_new = Faux
               MoiMême..Grisé = Vrai
               ToastSupprimeTout()
               ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
               HLibèreRequête()
