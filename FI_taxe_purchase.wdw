   -
     name : Add_jeton
     procedure_id : 2220798701125904551
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Add_jeton ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Add_jeton()
      
      SaisieSupprimeToutJeton(SAI_taxe)
      Taxe_p est un Jeton
      
      Taxe_p..Libellé = COMBO_taxe_type.COL_Name_taxe
      Taxe_p..Valeur  = COMBO_taxe_type.COL_Valeur_taxe
      
      SAI_taxe.AjouteJeton(Taxe_p)
     type : 458752
   -
     name : Echo_taxe_p
     procedure_id : 2220807643248308318
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Echo_taxe_p (<gIdTaxe>)
      //
      // Paramètres :
      // 	gIdTaxe : <indiquez ici le rôle de gIdTaxe>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      //	gIndces_taxe : <indiquez ici le rôle de gIndcs>
      PROCEDURE Echo_taxe_p(gIdTaxe)
      
      REQ_Select_All_taxe_type.Param_Min = 2
      REQ_Select_All_taxe_type.Param_Max = 3
      REQ_Select_All_taxe_type.Param_IDSociety = gnID_societe
      
      soit nrc = HExécuteRequête(REQ_Select_All_taxe_type)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON 
      	ListeAffiche(COMBO_taxe_type)
      	soit gIndces_taxe  = TableCherche(COL_IDTaxe,gIdTaxe,rechercheIdentique)
      	si gIndces_taxe > 0 ALORS
      		ListeSelectPlus(COMBO_taxe_type,gIndces_taxe)
      		Add_jeton()
      	FIN	
      	FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Id_taxe	= COMBO_taxe_type.COL_IDTaxe
      	FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_val_taxe	= COMBO_taxe_type.COL_Valeur_taxe
      FIN
     type : 458752
