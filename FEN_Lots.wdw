   -
     name : Init_fields
     procedure_id : 2175257414802926002
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Init_fields ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Init_fields()
      indcs est un entier
      
      SAI_Name = ""
      LIB_qt_num = 0
      SAI_Expire_date = DateDuJour()
      SAI_Alert = 1
      DonneFocus(SAI_Name)
      // afficher touts les articles dans le combo
      soit nrc = HExécuteRequête(REQ_Select_lot_product,hRequêteDéfaut,gnID_societe)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      ListeAffiche(COMBO_product)
      COMBO_product = ""
      si gId_produit > 0 ALORS
      	indcs = TableCherche(COL_IDArticle,gId_produit,rechercheIdentique)
      	si indcs > 0 ALORS
      		ListeSelectPlus(COMBO_product,indcs)
      	sinon
      		COMBO_product = ""
      	FIN
      sinon
      	COMBO_product = ""
      FIN
     type : 458752
   -
     name : Echo_data_to_sreen
     procedure_id : 2175277811606380729
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_data_to_sreen ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_data_to_sreen()
      
      soit nrc = HExécuteRequête(REQ_Select_lot_id,hRequêteDéfaut,grId_lots)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      POUR TOUT REQ_Select_lot_id 
      	SAI_Name 		= REQ_Select_lot_id.Name
      	SAI_série		= REQ_Select_lot_id.Serial_number
      	SAI_Expire_date = REQ_Select_lot_id.Expire_date
      	SAI_Alert 		= REQ_Select_lot_id.Alert
      	gnId_article 	= REQ_Select_lot_id.IDArticle
      FIN
      // Afficher l'article qui a le lot dans un  combo table
      //combo_article = gnId_article
      nrc = HExécuteRequête(REQ_Select_lot_product,hRequêteDéfaut,gnID_societe)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      ListeSelectPlus(COMBO_product,COMBO_product.Cherche(gnId_article))
      ListeAffiche(COMBO_product)
      //------Affiches la quantité disponible en stock -------
      //requête de sélection sur Availlable_quantity
      nrc = HExécuteRequête(REQ_Select_Availlable_Qt_lot,hRequêteDéfaut,grId_lots)
      si pas nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      soit rQte = 0.00 
      pour TOUT REQ_Select_Availlable_Qt_lot
      	
      	rQte += REQ_Select_Availlable_Qt_lot.Qantity
      	LIB_qt_num = rQte
      	gnId_Qt_availlable = REQ_Select_Availlable_Qt_lot.IDAvailable_quantity
      FIN
      GR_All_fields..EnSaisie = Faux
