               ExécuteAncêtre
              type : 14
            -
              code : |1-
               gnId_article = COMBO_product.COL_IDArticle
               si gnId_article = 0 ALORS
               	retour
               FIN
               // la date d'expiration d'un nouveau lot ne doit pas être une date inférieur ou la date du jours
               SI gbIs_news = Vrai _ET_ DateDifférence(DateDuJour(),SAI_Expire_date) <= 0 ALORS
               	Erreur(<§$0007§>)
               	DonneFocus(SAI_Expire_date)
               	RETOUR
               FIN
               
               //Ajout ou modification selon  les contextes (gbIs_news)
               //ajout avant 
               // le lot est automatiquement actif vrai
               //le numéro de série est vide ""
               si gbIs_news = Vrai ALORS
               	REQ_Add_lots.Param_Name				= SAI_Name
               	REQ_Add_lots.Param_Serial_number	= SAI_série
               	REQ_Add_lots.Param_Expire_date		= SAI_Expire_date
               	REQ_Add_lots.Param_Alert			= SAI_Alert
               	REQ_Add_lots.Param_IDArticle		= gnId_article
               	REQ_Add_lots.Param_IDSociety		= gnID_societe
               	REQ_Add_lots.Param_Stat				= 1
               	soit nrc = HExécuteRequête(REQ_Add_lots)
               	si pas nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	HLitDernier(Lot,IDLot)
               	grId_lots = lot.IDLot
               	
               	//Ajout dans le fichier availlable_quantity avec une valeur 0 de stock
               	nrc = HExécuteRequête(REQ_Add_availlable_Qt,hVérifieDoublon+hVérifieIntégrité,gnId_article,grId_lots,gnID_Ware_House,0)
               	si pas nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	HLitDernier(Available_quantity,IDAvailable_quantity)
               	gnId_Qt_availlable = Available_quantity.IDAvailable_quantity
               	
               sinon
               	REQ_Update_lots.Param_Alert			= SAI_Alert
               	REQ_Update_lots.Param_Expire_date	= SAI_Expire_date
               	REQ_Update_lots.Param_IDArticle		= gnId_article
               	REQ_Update_lots.Param_IDLot			= grId_lots
               	REQ_Update_lots.Param_Name			= SAI_Name
               	REQ_Update_lots.Param_Serial_number	= SAI_série
               	REQ_Update_lots.Param_Stat			= 1
               	soit nrc = HExécuteRequête(REQ_Update_lots)
               	si pas nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	
               	//modification dans le fichier availlable_quantity
               	REQ_Update_availlable_Qt.Param_IDArticle			= gnId_article
               	REQ_Update_availlable_Qt.Param_IDAvailable_quantity_lot	= gnId_Qt_availlable
               	REQ_Update_availlable_Qt.Param_IDLot				= grId_lots
               	REQ_Update_availlable_Qt.Param_IDWarehouse			= gnID_Ware_House
               	REQ_Update_availlable_Qt.Param_Qantity				= LIB_qt_num
               	nrc = HExécuteRequête(REQ_Update_availlable_Qt)
               	si pas nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               FIN
               
               Echo_data_to_sreen()
               BTN_Modifier..Visible = Vrai
               gbCheck_update = Faux
               gbIs_news = Faux
               ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
