     code : |1-
      PROCEDURE MaFenêtre()
      //récupération des information de l'utilisateur 
      gUser_info est une requête SQL = 
      [
      	SELECT<§cs:3§>
      		Name,Image,IDWarehouse<§cs:3§>
      	FROM<§cs:3§>
      		USER<§cs:3§>
      	WHERE<§cs:3§>
      		IDUser = {id_user}<§cs:3§>
      ]
      gUser_info.id_user = gnActif_user_id
      soit nrc= HExécuteRequêteSQL(gUser_info,hRequêteDéfaut)
      si pas nrc ALORS
      	HErreurInfo(hErrComplet)
      sinon
      	images_cripter est un buffer
      	pour TOUT gUser_info 
      		COL_OUTIL.gsNOMUSERS = gUser_info.Name
      		LIB_Name 		= gUser_info.Name
      		images_cripter 	= gUser_info.Image
      		gnID_Ware_House	= gUser_info.IDWarehouse
      	FIN
      	si images_cripter <> "" ALORS
      		IMG_user =  dChargeImage( Décode(images_cripter, encodeBASE64))
      	FIN
      	FIListeSupprimeTout(CFI_conteneur)
      	ChangeFenêtreSource(CFI_conteneur,FI_acceuille)
      	FIListeAffiche(CFI_conteneur)
      	LIB_Titre_de_fenêtre = "Accueil"
      FIN
