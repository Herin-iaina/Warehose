         // Résumé : <indiquez ici ce que fait la procédure>
         // Résumé : <indiquez ici ce que fait la procédure>// Syntaxe :
         // FI_TDB_Stats_Donut ()
         //
         // Paramètres :
         //	Aucun
         // Valeur de retour :
         // 	Aucune
         //
         // Exemple :
         // Indiquez ici un exemple d'utilisation.
         //
         PROCEDURE MaFenêtre()
         
         LIB_Qté_d_article_vendu_par = ChaîneConstruit(LIB_Qté_d_article_vendu_par,NuméroDeMoisVersLettre(MoisEnCours()))
      -
        code : |1+
         procédure interne Actualiser()
         	sqlQteProduit est une requête SQL = 
         [
         	SELECT 
         	SUM(Detaille_order.Quantity) AS TotalQt,
         	Category.Name as Categorie 
         	FROM 
         	Detaille_order
         	inner join Article
         	on Detaille_order.IDArticle = Article.IDArticle
         	inner join Category
         	on Article.IDCategory = Category.IDCategory
         	WHERE
         	IDSale_order IN (SELECT 
         						IDSale_order 
         					FROM
         						Sale_order
         					WHERE
         						IDSociety = {IDSociete}
         					AND
         						date_order BETWEEN {AAMM} AND {AAAAMM}) 
         	group by Category.Name
         	order by Category.Name asc
         ]
         
         	dDebutMoisEncours, dfinMoisEncours sont des dates
         	dDebutMoisEncours	= PremierJourDuMois(DateDuJour())	
         	dfinMoisEncours		= (dDebutMoisEncours..Mois < 12 ? PremierJourDuMois(dDebutMoisEncours..Année,dDebutMoisEncours..Mois + 1) SINON PremierJourDeLAnnée(dDebutMoisEncours..Année + 1))
         	
         	sqlQteProduit.IDSociete	= gnID_societe
         	sqlQteProduit.AAMM		= dDebutMoisEncours
         	sqlQteProduit.AAAAMM	= dfinMoisEncours
         	HExécuteRequêteSQL(sqlQteProduit)
         	soit i = 1
         	grSupprimeTout(GRF_Tunnel)
         	pour tout sqlQteProduit
         		grAjouteDonnée(GRF_Tunnel,1,sqlQteProduit.TotalQt)
         		grEtiquetteCatégorie(GRF_Tunnel,i,sqlQteProduit.Categorie)
         		grEtiquetteSérie(GRF_Tunnel,i,sqlQteProduit.Categorie)
         		i ++
         	FIN
         	grDessine(GRF_Tunnel)
         fin
         
         Actualiser()
         Timer(Actualiser, Hasard(6,12)* 200)
