     p_codes :
      -
        code : |1+
         PROCEDURE MaFenêtre()
      -
        code : |1-
         procédure interne Actualiser()
         	sqldispo est une requête SQL = 
         [
         	SELECT <§cs:3§>
         	COUNT( DISTINCT Article.IDArticle) AS nombre,<§cs:3§>
         	SUM(Available_quantity.Qantity)<§cs:3§>
         	FROM<§cs:3§>
         	Article<§cs:3§>
         	INNER JOIN Available_quantity<§cs:3§>
         	ON Article.IDArticle = Available_quantity.IDArticle<§cs:3§>
         	WHERE<§cs:3§>
         	Article.IDSociety = {IDSociete}<§cs:3§>
         	AND<§cs:3§>
         	Article.Stat = 1<§cs:3§>
         	AND<§cs:3§>
         	Article.Type_product LIKE '%Stockable%'<§cs:3§>
         	HAVING<§cs:3§>
         	SUM(Available_quantity.Qantity)  >= 0	<§cs:3§>
         ]
         
         	sqlarticle est une requête SQL = 
         	[
         	SELECT <§cs:3§>
         	COUNT( DISTINCT Article.IDArticle) AS nombre<§cs:3§>
         	FROM<§cs:3§>
         	Article<§cs:3§>
         	WHERE<§cs:3§>
         	Article.IDSociety = {IDSociete}<§cs:3§>
         	AND<§cs:3§>
         	Article.Stat = 1<§cs:3§>
         	AND<§cs:3§>
         	Article.Type_product LIKE '%Stockable%'	<§cs:3§>
         	]
         	
         	tabValStock est un tableau de numériques = [0,0,0]
         	sqldispo.IDSociete = gnID_societe
         	soit bNrc = HExécuteRequêteSQL(sqldispo)
         	si bNrc  = Faux ALORS
         		HErreurInfo(hErrComplet)
         	sinon
         		pour TOUT sqldispo 
         			tabValStock[1] = sqldispo.nombre
         		FIN
         	FIN
         	HLibèreRequête(sqldispo)
         	
         	sqlarticle.IDSociete = gnID_societe
         	bNrc = HExécuteRequêteSQL(sqlarticle)
         	SI bNrc  = Faux ALORS
         		HErreurInfo(hErrComplet)
         	SINON
         		POUR TOUT sqlarticle 
         			tabValStock[2] = sqlarticle.nombre
         		FIN
         	FIN
         	HLibèreRequête(sqlarticle)
         	
         	si tabValStock[2] > 0 ALORS
         		tabValStock[3] = tabValStock[1] * 100/ tabValStock[2]
         	FIN
         	LIB_135 = Arrondi(tabValStock[3],2) + "%"
         	TableauSupprimeTout(tabValStock)
         fin
         
         Actualiser()
         Timer(Actualiser,Hasard(6,12) * 200)
