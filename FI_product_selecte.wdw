   -
     name : Echo_article
     procedure_id : 2220586465308491237
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Echo_article ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	Echo_article : <indiquez ici le rôle de Echo_article>
      PROCEDURE Echo_article()
      
      sql_article est une requête SQL = 
      [
      	SELECT DISTINCT
      	Article.name_product AS name_product,
      	Article.IDArticle AS ID_article,
      	Article.Type_product AS Type_product,
      	Article.Purchase_price as Purchase_price,
      	unit_of_mesur.IDunit_of_mesur AS IDunit_of_mesur,
      	unit_of_mesur.ratio as ratio
      	FROM 
      	Article
      	INNER JOIN
      	unit_of_mesur
      	ON Article.IDUOM_P = unit_of_mesur.IDunit_of_mesur
      	WHERE 
      	(
      	Article.IDSociety = {SocietID}
      	AND Article.Stat = 1
      	)
      ]
      sql_article.SocietID = gnID_societe
      soit nrc = HExécuteRequêteSQL(sql_article )
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	ListeSupprimeTout(COMBO_Article)
      	POUR TOUT sql_article
      		soit nIndcs = TableAjouteLigne(COMBO_Article) 
      		COMBO_Article[nIndcs].COL_Article  	= sql_article.name_product
      		COMBO_Article[nIndcs].COL_ID_art	= sql_article.ID_article
      		COMBO_Article[nIndcs].col_Type	  	= sql_article.Type_product
      		COMBO_Article[nIndcs].col_UOM_p	  	= sql_article.IDunit_of_mesur
      		COMBO_Article[nIndcs].COL_ratio_p	= sql_article.ratio
      		COMBO_Article[nIndcs].COL_Purchase_price = sql_article.Purchase_price
      	FIN
      FIN
     type : 458752
   -
     name : selected_product
     procedure_id : 2220596352323809005
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // selected_product ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE selected_product()
      
      sql_select_ppp est une requête SQL = 
      [
      	SELECT 
      	product_per_partners.Price,
      	product_per_partners.IDunit_of_mesur,
      	unit_of_mesur.ratio
      	FROM
      	product_per_partners
      	INNER JOIN
      	unit_of_mesur 
      	ON product_per_partners.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur
      	WHERE
      	product_per_partners.IDArticle = {param_ProductID}
      	AND
      	product_per_partners.IDPartners = {Param_PartnerID}
      ]
      
      sql_select_ppp.param_ProductID = COMBO_Article.COL_ID_art
      sql_select_ppp.Param_PartnerID = gPartnersID
      soit nrc = HExécuteRequêteSQL(sql_select_ppp )
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      
      
      xRatio_p, xPrice_p sont des numériques
      nIdarticle, nIdUom_p sont des entiers
      SI HNbEnr(sql_select_ppp) > 0 ALORS
      	POUR TOUT sql_select_ppp 
      		xRatio_p = sql_select_ppp.ratio
      		xPrice_p = sql_select_ppp.Price
      		nIdUom_p = sql_select_ppp.IDunit_of_mesur
      	FIN
      SINON
      	xRatio_p = COMBO_Article.COL_ratio_p
      	xPrice_p = COMBO_Article.COL_Purchase_price
      	nIdUom_p = COMBO_Article.col_UOM_p
      FIN
      nIdarticle = COMBO_Article.COL_ID_art
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Id_product			= nIdarticle
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_RatioUomPuchase	= xRatio_p
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_ID_uom				= nIdUom_p
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Prix_HT			= xPrice_p
      //ChangeFenêtreSource(FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Udm.CFI_Uom_P)
     type : 458752
