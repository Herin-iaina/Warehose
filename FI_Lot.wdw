      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Color_expired_table() <IHM>
      
      soit nIndcs = TABLE_lot_Expire..Occurrence
      POUR i = nIndcs à 1 pas -1
      	soit nDtExp_tab = ChaîneVersDate(TABLE_lot_Expire[i].COL_Expire_date,"AAAAMMJJ")
      	soit nDtExp		= DateDifférence(DateDuJour(),nDtExp_tab)
      	SI nDtExp_tab < DateDuJour() ALORS
      		TABLE_lot_Expire[i]..CouleurFond = RVB(254, 113, 113)
      	SINON SI nDtExp > 0 _ET_ nDtExp <= TABLE_lot_Expire[i].COL_Alert
      		TABLE_lot_Expire[i]..CouleurFond = RVB(204, 102, 255)
      	FIN
      FIN
