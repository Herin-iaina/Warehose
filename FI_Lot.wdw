   -
     name : Echo_data
     procedure_id : 2207575901670623667
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_data ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_data()
      
      REQ_Select_lot.Param_IDSociety	= gnID_societe
      soit nrc = HExécuteRequête(REQ_Select_lot)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	TableAffiche(TABLE_lot)
      	TableSupprimeTout(TABLE_lot_Expire)
      	pour tout REQ_Select_lot
      		soit date_expire = DateDifférence(DateDuJour(),REQ_Select_lot.Expire_date)
      
      		si REQ_Select_lot.Alert >= date_expire _et_ REQ_Select_lot.Qantity > 0 ALORS
      			soit nIndcs = TableAjouteLigne(TABLE_lot_Expire)
      			TABLE_lot_Expire[nIndcs].COL_IDLot			= REQ_Select_lot.IDLot
      			TABLE_lot_Expire[nIndcs].COL_IDArticle		= REQ_Select_lot.IDArticle
      			TABLE_lot_Expire[nIndcs].COL_Alert			= REQ_Select_lot.Alert
      			TABLE_lot_Expire[nIndcs].COL_Expire_date	= REQ_Select_lot.Expire_date
      			TABLE_lot_Expire[nIndcs].COL_Name			= REQ_Select_lot.Name
      			TABLE_lot_Expire[nIndcs].COL_Name_product	= REQ_Select_lot.name_product
      			TABLE_lot_Expire[nIndcs].COL_Serial_number	= REQ_Select_lot.Serial_number
      			TABLE_lot_Expire[nIndcs].COL_Quantité		= REQ_Select_lot.Qantity
      			TABLE_lot_Expire[nIndcs].COL_Stat			= REQ_Select_lot.Stat
      		FIN
      	FIN
      FIN
      Color_expired_and_alert()
     type : 458752
   -
     name : Color_expired_and_alert
     procedure_id : 2207774750079747151
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Color_expired_and_alert ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Color_expired_and_alert() <IHM>
      
      soit nIndcs = TABLE_lot..Occurrence
      pour i = nIndcs à 1 pas -1
      	soit nDtExp = DateDifférence(DateDuJour(),TABLE_lot[i].COL_Expire_date)
      	SI TABLE_lot[i].COL_Expire_date < DateDuJour() ALORS
      		TABLE_lot[i]..CouleurFond = RVB(254, 113, 113)
      	SINON SI nDtExp > 0 _ET_ nDtExp <= TABLE_lot[i].COL_Alert
      		TABLE_lot[i]..CouleurFond = RVB(108, 179, 251)
      	FIN
      FIN
      
      //Color_expired_table()
     type : 458752
   -
