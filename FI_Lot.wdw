       identifier : 0x2ea2d97e1720c4af
       internal_properties : 
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1+
            selon FI_Lot..Plan
            	cas 1
            		soit nIndcs = TABLE_lot.Select()
            		SI TABLE_lot..Occurrence > 0 _ET_ nIndcs > 0 ALORS
            			Ouvre(FEN_Lots,Faux,TABLE_lot.COL_IDLot,TABLE_lot.COL_IDArticle)
            			Echo_data()
            			Color_expired_and_alert()
            			TableSelectPlus(TABLE_lot,nIndcs)
            		FIN
            		sortir selon
            	cas 2
            		soit nIndcs = TABLE_lot_Expire.Select()
            		SI TABLE_lot_Expire..Occurrence > 0 _ET_ nIndcs > 0 ALORS
            			Ouvre(FEN_Lots,Faux,TABLE_lot_Expire.COL_IDLot,TABLE_lot_Expire.COL_IDArticle)
            			Echo_data()
            			Color_expired_table()
            			TableSelectPlus(TABLE_lot_Expire,nIndcs)
            		FIN
            		sortir selon
            	AUTRE CAS
            		retour
            FIN
