  -
    name : BTN_Valider
    identifier : 0x2eb811941a5667ff
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         si COMBO_product_mere.COL_IDArticle > 0 _et_ COMBO_product_fille.COL_IDArticle > 0 ALORS
         	si SAI_qte > grQtArt ALORS
         		ToastSupprimeTout()
         		ToastAffiche("Veillez vérifier la quantité à traiter",toastCourt,cvMilieu,chCentre,BleuFoncé)
         		RepriseSaisie(SAI_qte)
         	FIN
         	si COMBO_product_mere.COL_Tracability = Vrai _et_ COMBO_Lot.COL_ID_lot <= 0 ALORS
         		ToastSupprimeTout()
         		ToastAffiche("Veillez vérifier choisir un lot",toastCourt,cvMilieu,chCentre,BleuFoncé)
         		ComboOuvre(COMBO_Lot)
         		retour
         	FIN
         	SI COMBO_product_fille.COL_Tracability = Vrai _ET_ COMBO_Lot_fille.COL_ID_lot <= 0 ALORS
         		ToastSupprimeTout()
         		ToastAffiche("Veillez vérifier choisir un lot",toastCourt,cvMilieu,chCentre,BleuFoncé)
         		ComboOuvre(COMBO_Lot_fille)
         		RETOUR
         	FIN
         	
         	soit LotID = (COMBO_product_mere.COL_Tracability = Vrai ? COMBO_Lot.COL_ID_lot sinon 0)
         	Soit LotIDFille = (COMBO_product_fille.COL_Tracability = Vrai ? COMBO_Lot_fille.COL_ID_lot sinon 0)
         	
         	sql_Update_Qte est une requête SQL = 
         [
         	update 
         	Available_quantity
         	set
         	Available_quantity.Qantity = {Qte_Article}
         	where
         	Available_quantity.IDAvailable_quantity = {IdStock_Article}
         ]
         	sql_select_Qte est une requête SQL = 
         [
         	select
         	Available_quantity.IDAvailable_quantity,
         	Available_quantity.Qantity
         	from
         	Available_quantity
         	where
         	Available_quantity.IDArticle = {ID_Article}
         	and
         	Available_quantity.IDLot = {ID_lot}
         	And
         	Available_quantity.IDWarehouse = {IDStock}
         ]
         	Sql_stock_move est une requête SQL = 
         [
         	insert into 
         	Stock_move
         	(date_move,Ref_move,Qantity,IDAvailable_quantity,IDArticle,IDSource,IDestination,Qantity_initial,Ecart,IDUser)
         	values
         	({Date_Mvnmt},{Ref},{Qte},{IdSotk},{Id_Article},{IdStokSrc},{IdStockDst},{QteInit},{Ecrt},{Users})
         ]
         	Sql_Add_Deconditioning est une requête SQL = 
         [
         	INSERT INTO 
         	Deconditioning	
         	(Create_date,IDArticle,ID_Sub_Product,Quantity,Qantity,IDSociety,IDUser,ID_destination,IDLot,IDWarehouse,Id_lot_fille)
         	values
         	({Date_Mvnmt},{Id_Article_mere},{Id_Article_fille},{Qte_mere},{Qte_fille},{SocietID},{UserID},{IDStockDestination},{IDLot_Mere},{IDStockSrc},{IDLot_fille})
         ]
         
         	soit date_op est une dateheure
         	date_op..PartieDate = DateDuJour()
         	date_op..PartieHeure = Maintenant()
         	
         	// mise à jour quantité article mère
         	sql_select_Qte.ID_Article = COMBO_product_mere.COL_IDArticle
         	sql_select_Qte.ID_lot	 = LotID
         	sql_select_Qte.IDStock	 = COMBO_warehouse.COL_IDWarehouse
         	soit nrc =  HExécuteRequêteSQL(sql_select_Qte,COL_ProcéduresGlobales.MaConnexion,hRequêteDéfaut+hRequêteSansCorrection)
         	si pas nrc ALORS
         		HErreurInfo(hErrComplet)
         	sinon si HNbEnr(sql_select_Qte) > 0
         		soit rQte_mere 	= 0.00
         		soit nIDSock 	= 0
         		soit qt_init	= 0.00
         		pour TOUT sql_select_Qte
         			rQte_mere 	= sql_select_Qte.Qantity - SAI_qte
         			nIDSock 	= sql_select_Qte.IDAvailable_quantity
         			qt_init		= sql_select_Qte.Qantity
         		FIN
         		
         		sql_Update_Qte.IdStock_Article	= nIDSock
         		sql_Update_Qte.Qte_Article		= rQte_mere
         		// Modification de stock mère
         		nrc = HExécuteRequêteSQL(sql_Update_Qte)
         		SI PAS nrc ALORS
         			HErreurInfo(hErrComplet)
         		fin
         		HLibèreRequête(sql_Update_Qte)
         		
         		//Mouvement de stock mère
         		Sql_stock_move.Date_Mvnmt	= date_op
         		Sql_stock_move.Ref			= "Déconditionnement"
         		Sql_stock_move.Qte			= SAI_qte
         		Sql_stock_move.IdSotk		= nIDSock
         		Sql_stock_move.Id_Article	= COMBO_product_mere.COL_IDArticle
         		Sql_stock_move.IdStokSrc	= COMBO_warehouse.COL_IDWarehouse
         		Sql_stock_move.IdStockDst	= COMBO_destination.COL_IDWarehouse
         		Sql_stock_move.QteInit		= qt_init
         		Sql_stock_move.Ecrt			= SAI_qte
         		Sql_stock_move.Users		= gnActif_user_id
         		nrc = HExécuteRequêteSQL(Sql_stock_move)
         		SI PAS nrc ALORS
         			HErreurInfo(hErrComplet)
         		FIN
         		HLibèreRequête(Sql_stock_move)
         		
         		// Ajout de détaille du déconditionnement
         		Sql_Add_Deconditioning.Date_Mvnmt			= date_op
         		Sql_Add_Deconditioning.Id_Article_mere		= COMBO_product_mere.COL_IDArticle
         		Sql_Add_Deconditioning.Id_Article_fille		= COMBO_product_fille.COL_IDArticle
         		Sql_Add_Deconditioning.Qte_mere				= SAI_qte
         		Sql_Add_Deconditioning.Qte_fille			= grQteAdd
         		Sql_Add_Deconditioning.SocietID				= gnID_societe
         		Sql_Add_Deconditioning.UserID				= gnActif_user_id
         		Sql_Add_Deconditioning.IDStockDestination	= COMBO_destination.COL_IDWarehouse
         		Sql_Add_Deconditioning.IDLot_Mere			= LotID
         		Sql_Add_Deconditioning.IDStockSrc			= COMBO_warehouse.COL_IDWarehouse
         		Sql_Add_Deconditioning.IDLot_fille			= LotIDFille
         		
         		nrc =  HExécuteRequêteSQL(Sql_Add_Deconditioning)
         		SI PAS nrc ALORS
         			HErreurInfo(hErrComplet)
         		FIN
         		HLibèreRequête(Sql_Add_Deconditioning)
         	FIN
         	HLibèreRequête(sql_select_Qte)
         	
         	// mise à jour quantité article mère
         	sql_select_Qte.ID_Article = COMBO_product_fille.COL_IDArticle
         	sql_select_Qte.ID_lot	 = LotIDFille
         	sql_select_Qte.IDStock	 = COMBO_destination.COL_IDWarehouse
         	soit nIdStockFille  = 0
         	soit rQte_FillInit	= 0.00
         	nrc =  HExécuteRequêteSQL(sql_select_Qte)
         	SI PAS nrc ALORS
         		HErreurInfo(hErrComplet)
         	SINON SI HNbEnr(sql_select_Qte) > 0
         		pour tout sql_select_Qte
         			 nIdStockFille = sql_select_Qte.IDAvailable_quantity
         			 rQte_FillInit 	= sql_select_Qte.Qantity 
         		FIN		
         	fin
         	
         	si nIdStockFille > 0 ALORS		
         		sql_Update_Qte.IdStock_Article	= nIdStockFille
         		sql_Update_Qte.Qte_Article		= rQte_FillInit + grQteAdd
         		// Modification de stock fille
         		nrc = HExécuteRequêteSQL(sql_Update_Qte)
         		SI PAS nrc ALORS
         			HErreurInfo(hErrComplet)
         		FIN
         		HLibèreRequête(sql_Update_Qte)
         	sinon
         		//Ajout de stock fille
         		REQ_Add_availlable_Qt.Param_IDArticle	= COMBO_product_fille.COL_IDArticle
         		REQ_Add_availlable_Qt.Param_IDLot		= LotIDFille
         		REQ_Add_availlable_Qt.Param_IDWarehouse	= COMBO_destination.COL_IDWarehouse
         		REQ_Add_availlable_Qt.Param_Qantity		= grQteAdd
         		nrc = HExécuteRequête(REQ_Add_availlable_Qt)
         		SI PAS nrc ALORS
         			HErreurInfo(hErrComplet)
         		FIN
         		HLibèreRequête(REQ_Add_availlable_Qt)
         	FIN
         	
         	//Mouvement de stock mère
         	Sql_stock_move.Date_Mvnmt	= date_op
         	Sql_stock_move.Ref			= "Déconditionnement"
         	Sql_stock_move.Qte			= grQteAdd
         	Sql_stock_move.IdSotk		= nIdStockFille
         	Sql_stock_move.Id_Article	= COMBO_product_fille.COL_IDArticle
         	Sql_stock_move.IdStokSrc	= COMBO_warehouse.COL_IDWarehouse
         	Sql_stock_move.IdStockDst	= COMBO_destination.COL_IDWarehouse
         	Sql_stock_move.QteInit		= rQte_FillInit
         	Sql_stock_move.Ecrt			= grQteAdd
         	Sql_stock_move.Users		= gnActif_user_id
         	nrc = HExécuteRequêteSQL(Sql_stock_move)
         	SI PAS nrc ALORS
         		HErreurInfo(hErrComplet)
         	FIN
         	HLibèreRequête(Sql_stock_move)
         	
         	ToastSupprimeTout()
         	ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
         	ferme_()
         	
         FIN
         
