               [
               	INSERT INTO <§cs:3§>
               	detaille_purchase<§cs:3§>
               	(IDPurchase,IDArticle,Quantity,IDunit_of_mesur,Purchase_price,Discount,IDTaxe,sub_ttl_ht,sub_ttl_ttc)<§cs:3§>
               	VALUES<§cs:3§>
               	({Purchase_ID},{Product_Id},{Qte},{UOM_id},{Priceht},{Rem_val},{Vat_ID},{line_ttl_ht},{line_ttl_ttc})<§cs:3§>
               ]
               
               sql_purchase_order_line_delete est une requête SQL = 
               [
               	DELETE FROM <§cs:3§>
               	detaille_purchase<§cs:3§>
               	WHERE<§cs:3§>
               	detaille_purchase.IDPurchase = {Purchase_id}<§cs:3§>
               ]
               // ligne
               sql_purchase_order_line_delete.Purchase_id = gnId_purchase
               soit nrc = HExécuteRequêteSQL(sql_purchase_order_line_delete)
               SI PAS nrc ALORS
               	HErreurInfo(hErrComplet)
               FIN
               
               SI TABLE_purchase_order_line..Occurrence > 0 ALORS
               	POUR TOUTE LIGNE DE TABLE_purchase_order_line
               		SI TABLE_purchase_order_line.COL_Id_product > 0 ALORS
               			sql_purchase_order_line.Purchase_ID	= gnId_purchase
               			sql_purchase_order_line.Product_Id	= TABLE_purchase_order_line.COL_Id_product
               			sql_purchase_order_line.Qte			= TABLE_purchase_order_line.COL_Quantité
               			sql_purchase_order_line.UOM_id		= TABLE_purchase_order_line.COL_ID_uom
               			sql_purchase_order_line.Priceht		= TABLE_purchase_order_line.COL_Prix_HT
               			sql_purchase_order_line.Rem_val		= TABLE_purchase_order_line.COL_Remise
               			sql_purchase_order_line.Vat_ID		= TABLE_purchase_order_line.COL_Id_taxe
               			sql_purchase_order_line.line_ttl_ht	= TABLE_purchase_order_line.COL_Sous_total_HT
               			sql_purchase_order_line.line_ttl_ttc= TABLE_purchase_order_line.COL_Sous_total_HT + TABLE_purchase_order_line.COL_Total_taxe
               			
               			nrc = HExécuteRequêteSQL(sql_purchase_order_line)
               			SI PAS nrc ALORS
               				HErreurInfo(hErrComplet)
               			FIN
               		FIN
               	FIN
               FIN
               
