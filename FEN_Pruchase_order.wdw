      num_doc est une requête SQL = 
      [
      	SELECT<§cs:3§>
      	IDNumero_document, <§cs:3§>
      	number_next	<§cs:3§>
      	FROM<§cs:3§>
      	Numero_document	<§cs:3§>
      	WHERE<§cs:3§>
      	label = 'Purchase_Order'<§cs:3§>
      ]
      soit nrc  = HExécuteRequêteSQL(num_doc)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON SI HNbEnr(num_doc) = 0 // ajoute une 
      	 nrc = HExécuteRequêteSQL("Add_mun_session","INSERT INTO Numero_document (label,number_next) VALUES ('Purchase_Order',1)")
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	FIN
      	xNum_order_Next = 1	 
      SINON SI HNbEnr(num_doc) > 0
      	pour TOUT num_doc 
      		xNum_order_Next = num_doc.number_next
      	FIN
      FIN
      
      renvoyer xNum_order_Next
