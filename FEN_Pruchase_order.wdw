   -
     name : Add_operation_move
     procedure_id : 2222038254565721126
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Add_operation_move ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Add_operation_move()
      
      soit nUm_operation =  Num_operation_move_next()
      soit Complet_num	= "00000"
      //formatage du numéro de document dans la base en chaine de caractere
      SI Taille(nUm_operation) < 5 ALORS
      	Complet_num = Tronque(Complet_num,Taille(nUm_operation),nombreDeCaractèresASupprimer) + nUm_operation
      SINON
      	Complet_num = nUm_operation
      FIN
      
      
      //Ajout de type d’opération
      sql_add_operation_move est une requête SQL = 
      [
      	INSERT INTO <§cs:3§>
      	Operation_type_move<§cs:3§>
      	(date_order,type_operation,Etat,num_doc,Num_order,IDPurchase,IDUser,IDSociety)<§cs:3§>
      	VALUES<§cs:3§>
      	({date_op},{Type_op},{etat_op},{Num_origine_op},{num_op},{Purchase_id},{userID},{societID})<§cs:3§>
      ]
      
      // Ajout de détaille d’opération
      sql_add_detaille_operation est une requête SQL = 
      [
      	INSERT INTO <§cs:3§>
      	Detaille_operation_move<§cs:3§>
      	(Quantity,quantit_done,IDOperation_type_move,IDArticle,IDLot,IDunit_of_mesur)<§cs:3§>
      	VALUES<§cs:3§>
      	({Qt_op},{Qt_done_op},{operationID},{ArticleID},{LotID},{UoMID})<§cs:3§>
      ]
      
      //line purchase
      sql_select_order_line est une requête SQL = 
      [
      	SELECT<§cs:3§>
      	detaille_purchase.IDArticle,<§cs:3§>
      	detaille_purchase.Quantity,<§cs:3§>
      	detaille_purchase.IDunit_of_mesur,<§cs:3§>
      	Article.Tracability<§cs:3§>
      	FROM<§cs:3§>
      	detaille_purchase<§cs:3§>
      	INNER JOIN<§cs:3§>
      	Article<§cs:3§>
      	ON detaille_purchase.IDArticle = Article.IDArticle <§cs:3§>
      	WHERE<§cs:3§>
      	detaille_purchase.IDPurchase = {Purchase_ID}<§cs:3§>
      	AND<§cs:3§>
      	Article.Type_product = 'Stockable'<§cs:3§>
      ]
      // lot
      Sql_select_lot est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Lot.IDLot<§cs:3§>
      	FROM<§cs:3§>
      	Lot<§cs:3§>
      	WHERE<§cs:3§>
      	Lot.Stat = 1<§cs:3§>
      	AND<§cs:3§>
      	Lot.IDArticle = {param_ArtID}<§cs:3§>
      	ORDER BY<§cs:3§>
      	Lot.Expire_date ASC<§cs:3§>
      ]
      
      sql_add_operation_move.date_op			= DateDuJour()
      sql_add_operation_move.Type_op			= 1
      sql_add_operation_move.etat_op			= "Nouveau"
      sql_add_operation_move.Num_origine_op	= LIB_Num_order
      sql_add_operation_move.num_op			= ChaîneConstruit("In/%1",Complet_num)
      sql_add_operation_move.Purchase_id		= gnId_purchase
      sql_add_operation_move.userID			= gnActif_user_id
      sql_add_operation_move.societID			= gnID_societe
      
      soit nId_operation est un entier sur 8 octets = 0
      soit nrc = HExécuteRequêteSQL(sql_add_operation_move)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	HLitDernier(Operation_type_move,IDOperation_type_move)
      	nId_operation = Operation_type_move.IDOperation_type_move
      	
      FIN
      
      sql_select_order_line.Purchase_ID = gnId_purchase
      nrc = HExécuteRequêteSQL(sql_select_order_line)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON SI HNbEnr(sql_select_order_line) > 0
      	POUR TOUT sql_select_order_line
      		
      		sql_add_detaille_operation.Qt_op		= sql_select_order_line.Quantity
      		sql_add_detaille_operation.Qt_done_op	= 0
      		sql_add_detaille_operation.operationID	= nId_operation
      		sql_add_detaille_operation.ArticleID	= sql_select_order_line.IDArticle
      		sql_add_detaille_operation.UoMID		= sql_select_order_line.IDunit_of_mesur
      		soit nLot_ID = 0
      //		SI sql_select_order_line.Tracability = 1 ALORS
      //			soit idArt = sql_select_order_line.IDArticle
      //			Sql_select_lot.param_ArtID = idArt
      //			nrc = HExécuteRequêteSQL(Sql_select_lot)
      //			SI  nrc = Faux ALORS
      //				HErreurInfo(hErrComplet)
      //			SINON
      //				nLot_ID = Sql_select_lot.IDLot
      //			FIN
      //			HLibèreRequête(Sql_select_lot)
      //		FIN
      		sql_add_detaille_operation.LotID		= nLot_ID
      		nrc = HExécuteRequêteSQL(sql_add_detaille_operation)
      		SI  nrc = Faux ALORS
      			HErreurInfo(hErrComplet)
      		FIN
      	FIN
      FIN
      
      // mise à jour du numéro d’opération
      UPDATEOPERATION EST une requête SQL = 
      [
      	UPDATE Numero_document SET number_next = {num_next} WHERE label = 'Operation_move_in'<§cs:3§>
      ]
      UPDATEOPERATION.num_next = nUm_operation + 1
      
      nrc = HExécuteRequêteSQL(UPDATEOPERATION)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
     type : 458752
