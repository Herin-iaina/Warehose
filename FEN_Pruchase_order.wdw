     name : selected_product
     procedure_id : 2220620159327965847
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //selected_product ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	selected_product : <indiquez ici le rôle de selected_product>
      PROCEDURE selected_product(gIndcs,gPartnersID)
      
      
      si gIndcs < 0 ALORS
      	gIndcs = 1
      FIN
      sql_select_ppp est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	product_per_partners.Price,<§cs:3§>
      	product_per_partners.IDunit_of_mesur,<§cs:3§>
      	unit_of_mesur.ratio<§cs:3§>
      	FROM<§cs:3§>
      	product_per_partners<§cs:3§>
      	INNER JOIN<§cs:3§>
      	unit_of_mesur <§cs:3§>
      	ON product_per_partners.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur<§cs:3§>
      	WHERE<§cs:3§>
      	product_per_partners.IDArticle = {param_ProductID}<§cs:3§>
      	AND<§cs:3§>
      	product_per_partners.IDPartners = {Param_PartnerID}<§cs:3§>
      ]
      
      sql_select_ppp.param_ProductID = TABLE_purchase_order_line.COMBO_Articles.COL_ID_art
      sql_select_ppp.Param_PartnerID = gPartnersID
      soit nrc = HExécuteRequêteSQL(sql_select_ppp)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      
      
      xRatio_p, xPrice_p sont des numériques
      nIdarticle, nIdUom_p sont des entiers
      SI HNbEnr(sql_select_ppp) > 0 ALORS
      	POUR TOUT sql_select_ppp 
      		xRatio_p = sql_select_ppp.ratio
      		xPrice_p = sql_select_ppp.Price
      		nIdUom_p = sql_select_ppp.IDunit_of_mesur
      	FIN
      SINON
      	xRatio_p = TABLE_purchase_order_line.COMBO_Articles.COL_ratio_p
      	xPrice_p = TABLE_purchase_order_line.COMBO_Articles.COL_Purchase_price
      	nIdUom_p = TABLE_purchase_order_line.COMBO_Articles.col_UOM_p
      FIN
      nIdarticle = TABLE_purchase_order_line.COMBO_Articles.COL_ID_art
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Id_product			= nIdarticle
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_RatioUomPuchase	= xRatio_p
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_RatioUoM			= xRatio_p
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_ID_uom				= nIdUom_p
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Prix_HT			= xPrice_p
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Prix_p				= xPrice_p
      FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Article.SAI_SansNom1 = TABLE_purchase_order_line.COMBO_Articles.COL_Article
      ChangeFenêtreSource(FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Taxe.CFI_taxe_p,FI_taxe_purchase,TABLE_purchase_order_line.COMBO_Articles.COL_taxe_p,gIndcs)
      FEN_Pruchase_order.TABLE_purchase_order_line.COL_Udm.CFI_Uom_P.ChangeFenêtreSource(FI_Uom_purchase,nIdUom_p,gIndcs)
      
      
      Compute_Purchase_Price_HT()
      Compute_subtotal_line()
      Compute_taxe()
      Compute_total()
      
      //ChangeFenêtreSource(FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Udm.CFI_Uom_P,FI_Uom_purchase,nIdUom_p,gIndcs)
      //FEN_Pruchase_order.TABLE_purchase_order_line[gIndcs].COL_Udm.CFI_Uom_P.ChangeFenêtreSource(FI_Uom_purchase,nIdUom_p,gIndcs)
     type : 458752
