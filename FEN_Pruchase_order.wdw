     name : Compute_Purchase_Price_HT
     procedure_id : 2220973489161042010
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Compute_Purchase_Price_HT ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	prix_p : <indiquez ici le rôle de prix_p>
      PROCEDURE Compute_Purchase_Price_HT()
      
      //calcul le prix unitaire HT
      soit rPrix_compute = 0.00
      soit ratio_fix = TABLE_purchase_order_line.COL_RatioUomPuchase
      soit ratio_var = TABLE_purchase_order_line.COL_RatioUoM
      soit prix_p    = TABLE_purchase_order_line.COL_Prix_p
      si ratio_fix > 0 ALORS
      	rPrix_compute = prix_p * ratio_var / ratio_fix
      FIN
      
      TABLE_purchase_order_line.COL_Prix_HT = rPrix_compute
      
      Compute_subtotal_line()
     type : 458752
   -
     name : Compute_subtotal_line
     procedure_id : 2220980034691205161
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Compute_subtotal_line ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Compute_subtotal_line()
      
      // calcul de sous total
      soit Prix_unit_price = TABLE_purchase_order_line.COL_Prix_HT
      soit Quantite        = TABLE_purchase_order_line.COL_Quantité
      soit discount_p      = TABLE_purchase_order_line.COL_Remise / 100
      soit subtotal_line   = Prix_unit_price * (1 - discount_p ) * Quantite
      TABLE_purchase_order_line.COL_Sous_total_HT = subtotal_line
