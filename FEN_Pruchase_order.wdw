     name : Echo_article
     procedure_id : 2220619304629407046
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Echo_article ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	Echo_article : <indiquez ici le rôle de Echo_article>
      PROCEDURE Echo_article()
      
      sql_article est une requête SQL = 
      [
      	SELECT DISTINCT<§cs:3§>
      	Article.name_product AS name_product,<§cs:3§>
      	Article.IDArticle AS ID_article,<§cs:3§>
      	Article.Type_product AS Type_product,<§cs:3§>
      	Article.Purchase_price AS Purchase_price,<§cs:3§>
      	Article.Id_TaxePurchase as Id_TaxePurchase,<§cs:3§>
      	unit_of_mesur.IDunit_of_mesur AS IDunit_of_mesur,<§cs:3§>
      	unit_of_mesur.ratio AS ratio<§cs:3§>
      	FROM <§cs:3§>
      	Article<§cs:3§>
      	INNER JOIN<§cs:3§>
      	unit_of_mesur<§cs:3§>
      	ON Article.IDUOM_P = unit_of_mesur.IDunit_of_mesur<§cs:3§>
      	WHERE <§cs:3§>
      	(<§cs:3§>
      	Article.IDSociety = {SocietID}<§cs:3§>
      	AND Article.Stat = 1<§cs:3§>
      	)<§cs:3§>
      ]
      sql_article.SocietID = gnID_societe
      
      soit nrc = HExécuteRequêteSQL(sql_article)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      
      	ListeSupprimeTout(TABLE_purchase_order_line.COMBO_Articles)
      	POUR TOUT sql_article
      		soit nIndcs = TableAjouteLigne(TABLE_purchase_order_line.COMBO_Articles) 
      		TABLE_purchase_order_line.COMBO_Articles[nIndcs].COL_Article  	= sql_article.name_product
      		TABLE_purchase_order_line.COMBO_Articles[nIndcs].COL_ID_art		= sql_article.ID_article
      		TABLE_purchase_order_line.COMBO_Articles[nIndcs].col_Type	  	= sql_article.Type_product
      		TABLE_purchase_order_line.COMBO_Articles[nIndcs].col_UOM_p	  	= sql_article.IDunit_of_mesur
      		TABLE_purchase_order_line.COMBO_Articles[nIndcs].COL_ratio_p		= sql_article.ratio
      		TABLE_purchase_order_line.COMBO_Articles[nIndcs].COL_Purchase_price = sql_article.Purchase_price
      		TABLE_purchase_order_line.COMBO_Articles[nIndcs].COL_taxe_p		= sql_article.Id_TaxePurchase
      	FIN
      	
      FIN
     type : 458752
