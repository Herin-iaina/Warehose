   -
     name : Echo_pruchase_order_line
     procedure_id : 2221659172037070582
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_pruchase_order_line ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_pruchase_order_line()
      
      Echo_article()
      // order line
      sql_select_order_line est une requête SQL = 
      [
      	SELECT<§cs:3§>
      	detaille_purchase.IDArticle,<§cs:3§>
      	detaille_purchase.Quantity,<§cs:3§>
      	detaille_purchase.IDunit_of_mesur,<§cs:3§>
      	detaille_purchase.Purchase_price,<§cs:3§>
      	detaille_purchase.Discount,<§cs:3§>
      	detaille_purchase.IDTaxe,<§cs:3§>
      	detaille_purchase.sub_ttl_ht,<§cs:3§>
      	Article.name_product,<§cs:3§>
      	uom_product.ratio as ratio_pr,<§cs:3§>
      	unit_of_mesur.ratio,<§cs:3§>
      	Taxe.Valeur_taxe<§cs:3§>
      	FROM<§cs:3§>
      	detaille_purchase<§cs:3§>
      	inner join<§cs:3§>
      	Article<§cs:3§>
      	on detaille_purchase.IDArticle = Article.IDArticle <§cs:3§>
      	inner join<§cs:3§>
      	unit_of_mesur as uom_product<§cs:3§>
      	on Article.IDUOM_P = uom_product.IDunit_of_mesur <§cs:3§>
      	inner join<§cs:3§>
      	unit_of_mesur<§cs:3§>
      	ON detaille_purchase.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur <§cs:3§>
      	left join<§cs:3§>
      	Taxe	<§cs:3§>
      	on detaille_purchase.IDTaxe = Taxe.IDTaxe<§cs:3§>
      	WHERE<§cs:3§>
      	detaille_purchase.IDPurchase = {Purchase_ID}<§cs:3§>
      ]
      
      //sélection produit par fournisseur
      sql_select_ppp est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	product_per_partners.Price,<§cs:3§>
      	unit_of_mesur.ratio<§cs:3§>
      	FROM<§cs:3§>
      	product_per_partners<§cs:3§>
      	INNER JOIN<§cs:3§>
      	unit_of_mesur <§cs:3§>
      	ON product_per_partners.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur<§cs:3§>
      	WHERE<§cs:3§>
      	product_per_partners.IDArticle = {param_ProductID}<§cs:3§>
      	AND<§cs:3§>
      	product_per_partners.IDPartners = {Param_PartnerID}<§cs:3§>
      ]
      
      soit gPartnersID = COMBO_supplier.COL_IDPartners
      sql_select_ppp.param_ProductID = TABLE_purchase_order_line.COMBO_Articles.COL_ID_art
      sql_select_ppp.Param_PartnerID = gPartnersID
      
      soit nrc = HExécuteRequêteSQL(sql_select_ppp)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      
      
      sql_select_order_line.Purchase_ID = gnId_purchase
      
      nrc = HExécuteRequêteSQL(sql_select_order_line)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	TableSupprimeTout(TABLE_purchase_order_line)
      	POUR TOUT sql_select_order_line
      		soit nIndcs = TableAjouteLigne(TABLE_purchase_order_line)
      		soit id_article_ = sql_select_order_line.IDArticle
      		TABLE_purchase_order_line[nIndcs].COL_Id_product	= id_article_
      		TABLE_purchase_order_line[nIndcs].COL_Id_taxe		= sql_select_order_line.IDTaxe
      		TABLE_purchase_order_line[nIndcs].COL_ID_uom		= sql_select_order_line.IDunit_of_mesur
      		soit qte = sql_select_order_line.Quantity
      		TABLE_purchase_order_line[nIndcs].COL_Quantité		= qte
      		soit price_ht_p =  sql_select_order_line.Purchase_price
      		TABLE_purchase_order_line[nIndcs].COL_Prix_HT		= price_ht_p
      		soit discount = sql_select_order_line.Discount
      		TABLE_purchase_order_line[nIndcs].COL_Remise		= discount
      		TABLE_purchase_order_line[nIndcs].COL_Article.SAI_SansNom1 = sql_select_order_line.name_product
      		
      		soit rech_article_id = TableCherche(TABLE_purchase_order_line.COL_Article.COMBO_Articles.COL_ID_art,id_article_,rechercheIdentique)
      		ListeSelectPlus(TABLE_purchase_order_line.COL_Article.COMBO_Articles,rech_article_id)
      		
      		//----------------Article par fournisseur
      		sql_select_ppp.param_ProductID = id_article_
      		sql_select_ppp.Param_PartnerID = gPartnersID
      		
      		nrc = HExécuteRequêteSQL(sql_select_ppp)
      		SI PAS nrc ALORS
      			HErreurInfo(hErrComplet)
      		FIN
      		
      		
      		xRatio_p, xPrice_p sont des numériques
      		SI HNbEnr(sql_select_ppp) > 0 ALORS
      			POUR TOUT sql_select_ppp 
      				xRatio_p = sql_select_ppp.ratio
      				xPrice_p = sql_select_ppp.Price
      			FIN
      		SINON
      			xRatio_p = sql_select_order_line.ratio_pr
      			xPrice_p = TABLE_purchase_order_line.COMBO_Articles.COL_Purchase_price			
      		FIN
      		//----------------------------------------
      
      		TABLE_purchase_order_line[nIndcs].COL_Prix_p		= xPrice_p
      		TABLE_purchase_order_line[nIndcs].COL_RatioUomPuchase= sql_select_order_line.ratio_pr
      		TABLE_purchase_order_line[nIndcs].COL_RatioUoM 		= xRatio_p
      		
      		ChangeFenêtreSource(FEN_Pruchase_order.TABLE_purchase_order_line[nIndcs].COL_Taxe.CFI_taxe_p,FI_taxe_purchase,sql_select_order_line.IDTaxe,nIndcs)
      		ChangeFenêtreSource(FEN_Pruchase_order.TABLE_purchase_order_line[nIndcs].COL_Udm.CFI_Uom_P,FI_Uom_purchase,sql_select_order_line.IDunit_of_mesur,nIndcs)
      //		TABLE_purchase_order_line.COL_Udm.CFI_Uom_P.ChangeFenêtreSource(FI_Uom_purchase,sql_select_order_line.IDunit_of_mesur,nIndcs)
      //		TABLE_purchase_order_lineCOL_Taxe.CFI_taxe_p.ChangeFenêtreSource(FI_taxe_purchase,sql_select_order_line.IDTaxe,nIndcs)
      		
      		soit vat_val = sql_select_order_line.Valeur_taxe
      		TABLE_purchase_order_line[nIndcs].COL_val_taxe 		= vat_val
      		TABLE_purchase_order_line[nIndcs].COL_Total_taxe 	= price_ht_p * qte * vat_val / 100
      		TABLE_purchase_order_line[nIndcs].COL_Sous_total_HT	= price_ht_p * ( 1 - discount / 100) * qte
      		//		TABLE_purchase_order_line[nIndcs].
      		//		TABLE_purchase_order_line[nIndcs].
      	FIN
      	Compute_taxe()
      	Compute_total()
      FIN
