     name : Compute_taxe
     procedure_id : 2221014703667624870
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Compute_taxe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Compute_taxe()
      
      taTaxetabe est un tableau associatif de numériques
      
      soit tax_val = TABLE_purchase_order_line.COL_val_taxe
      Soit qte 	 = TABLE_purchase_order_line.COL_Quantité
      soit prixHt  = TABLE_purchase_order_line.COL_Prix_HT
      Soit total_taxe = qte * prixHt * tax_val / 100
      TABLE_purchase_order_line.COL_Total_taxe = total_taxe
      
      POUR TOUTE LIGNE DE TABLE_purchase_order_line
      	soit txe_ID est un numérique = TABLE_purchase_order_line.COL_Id_taxe
      	taTaxetabe[txe_ID] += TABLE_purchase_order_line.COL_Total_taxe
      FIN
      
      Sql_taxe est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Taxe.IDTaxe,<§cs:3§>
      	Taxe.Name_taxe<§cs:3§>
      	FROM<§cs:3§>
      	Taxe<§cs:3§>
      	WHERE<§cs:3§>
      	Taxe.IDTaxe = {Param_IDTaxe}<§cs:3§>
      ]
      
      TableSupprimeTout(TABLE_taxe)
      POUR TOUT  sTotal_vat_p, xId_vat_p de taTaxetabe
      	SI xId_vat_p > 0 ALORS
      		Sql_taxe.Param_IDTaxe = xId_vat_p
      		soit nrc = HExécuteRequêteSQL(Sql_taxe)
      		SI PAS nrc ALORS
      			HErreurInfo(hErrComplet)
      		FIN
      		soit nDcs = TableAjouteLigne(TABLE_taxe)
      		TABLE_taxe[nDcs].COL_id_taxe	= Sql_taxe.IDTaxe
      		TABLE_taxe[nDcs].COL_Taxe		= Sql_taxe.Name_taxe
      		TABLE_taxe[nDcs].COL_Total		= sTotal_vat_p
      	FIN	
      FIN
     type : 458752
