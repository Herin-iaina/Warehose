           p_codes :
            -
              code : |1-
               //sauvegarde
               Sablier(Vrai)
               SI COMBO_supplier.COL_IDPartners < 1 ALORS
               	ToastSupprimeTout()
               	ToastAffiche("Opération non valide" + RC + "Ajouter un fournisseur",toastCourt,cvMilieu,chCentre,BleuFoncé)
               	ComboOuvre(COMBO_supplier)
               	RETOUR
               FIN
               
               
               sql_purchase_save est une requête SQL = 
               [
               	insert into <§cs:3§>
               	Purchase<§cs:3§>
               	(Create_date,Note,Stat,Etat,IDPartners,IDSociety,IDUser,Num_order)<§cs:3§>
               	values <§cs:3§>
               	({DateOder},{Note_Po},{Stat_Po},{Etat_Po},{SuplyerID},{SocietID},{UserID},{Num_Po})<§cs:3§>
               ]
               
               sql_purchase_update est une requête SQL = 
               [
               	update <§cs:3§>
               	Purchase<§cs:3§>
               	set<§cs:3§>
               	Create_date	= {DateOder},<§cs:3§>
               	Note		= {Note_Po},<§cs:3§>
               	Stat		= {Stat_Po},<§cs:3§>
               	Etat		= {Etat_Po},<§cs:3§>
               	IDPartners	= {SuplyerID},<§cs:3§>
               	IDSociety	= {SocietID},<§cs:3§>
               	IDUser		= {UserID}<§cs:3§>
               	where<§cs:3§>
               	Purchase.IDPurchase = {Param_PurchasID}<§cs:3§>
               ]
               
               //ligne 
               sql_purchase_order_line est une requête SQL = 
               [
               	insert into <§cs:3§>
               	detaille_purchase<§cs:3§>
               	(IDPurchase,IDArticle,Quantity,IDunit_of_mesur,Purchase_price,Discount,IDTaxe,sub_ttl_ht,sub_ttl_ttc)<§cs:3§>
               	values<§cs:3§>
               	({Purchase_ID},{Product_Id},{Qte},{UOM_id},{Priceht},{Rem_val},{Vat_ID},{line_ttl_ht},{line_ttl_ttc})<§cs:3§>
               ]
               
               sql_purchase_order_line_delete est une requête SQL = 
               [
               	delete From <§cs:3§>
               	detaille_purchase<§cs:3§>
               	where<§cs:3§>
               	detaille_purchase.IDPurchase = {Purchase_id}<§cs:3§>
               ]
               
               si gbIs_new = Vrai ALORS
               	sql_purchase_save.DateOder	= SAI_Date1
               	sql_purchase_save.Note_Po	= SAI_note
               	sql_purchase_save.Stat_Po	= Vrai
               	sql_purchase_save.Etat_Po	= "Brouillon"
               	sql_purchase_save.SuplyerID	= COMBO_supplier.COL_IDPartners
               	sql_purchase_save.SocietID	= gnID_societe
               	sql_purchase_save.UserID	= gnActif_user_id
               	sql_purchase_save.Num_Po	= LIB_Num_order
               	
               	soit nrc = HExécuteRequêteSQL(sql_purchase_save)
               	SI PAS nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	HLitDernier(Purchase,IDPurchase)
               	gnId_purchase = Purchase.IDPurchase
               	Update_num_Purchase_Order()
               sinon
               	sql_purchase_update.DateOder	= SAI_Date1
               	sql_purchase_update.Note_Po		= SAI_note
               	sql_purchase_update.Stat_Po		= gbStatDoc
               	sql_purchase_update.Etat_Po		= gsEtat_doc
               	sql_purchase_update.SuplyerID	= COMBO_supplier.COL_IDPartners
               	sql_purchase_update.SocietID	= gnID_societe
               	sql_purchase_update.UserID		= gnActif_user_id
               	sql_purchase_update.Param_PurchasID = gnId_purchase
               	soit nrc = HExécuteRequêteSQL(sql_purchase_update) 
               	SI PAS nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               FIN
               
               // ligne
               sql_purchase_order_line_delete.Purchase_id = gnId_purchase
               soit nrc = HExécuteRequêteSQL(sql_purchase_order_line_delete)
               si pas nrc ALORS
               	HErreurInfo(hErrComplet)
               FIN
               HLibèreRequête(sql_purchase_order_line_delete)
               
               soit Nboccrs = TABLE_purchase_order_line..Occurrence
               si Nboccrs > 0 ALORS
               	POUR i = Nboccrs à 1 pas -1
               		SI TABLE_purchase_order_line[i].COL_Id_product > 0 ALORS
               			sql_purchase_order_line.Purchase_ID	= gnId_purchase
               			sql_purchase_order_line.Product_Id	= TABLE_purchase_order_line[i].COL_Id_product
               			sql_purchase_order_line.Qte			= TABLE_purchase_order_line[i].COL_Quantité
               			sql_purchase_order_line.UOM_id		= TABLE_purchase_order_line[i].COL_ID_uom
               			sql_purchase_order_line.Priceht		= TABLE_purchase_order_line[i].COL_Prix_HT
               			sql_purchase_order_line.Rem_val		= TABLE_purchase_order_line[i].COL_Remise
               			sql_purchase_order_line.Vat_ID		= TABLE_purchase_order_line[i].COL_Id_taxe
               			sql_purchase_order_line.line_ttl_ht	= TABLE_purchase_order_line[i].COL_Sous_total_HT
               			sql_purchase_order_line.line_ttl_ttc= TABLE_purchase_order_line[i].COL_Sous_total_HT + TABLE_purchase_order_line.COL_Total_taxe
               			
               			nrc = HExécuteRequêteSQL(sql_purchase_order_line)
               			SI PAS nrc ALORS
               				HErreurInfo(hErrComplet)
               			FIN
               		FIN
               	FIN
               FIN
               
               
               //Update_num_Purchase_Order()
               Echo_purchase_order()
               
               SI gnId_purchase > 0 ALORS
               	BTN_Modifier..Visible 	= Vrai
               	//	BTN_Save..Visible		= Faux
               	BTN_Annuler..Visible	= Vrai
               	BTN_Annuler..Grisé		= Faux
               	//	BTN_Supprimer..Visible	= Faux
               	//	BTN_Valider..Grisé 		= Vrai
               	BTN_Draft..Visible		= Faux
               FIN
               GR_All_fields..EnSaisie = Faux
               Sablier(Faux)
               
               ToastSupprimeTout()
               ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
