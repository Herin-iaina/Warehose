      //
      PROCEDURE Num_operation_move_next()
      
      soit xNum_order_Next = 0 
      //numéro de commande
      //récupère le numéro
      num_doc est une requête SQL = 
      [
      	SELECT<§cs:3§>
      	IDNumero_document, <§cs:3§>
