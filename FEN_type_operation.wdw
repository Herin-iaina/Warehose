   -
     name : Echo_data_operation
     procedure_id : 2222015091806546462
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_data_operation ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_data_operation()
      
      sql_operation_type est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Operation_type_move.IDOperation_type_move,<§cs:3§>
      	Operation_type_move.date_order,<§cs:3§>
      	Operation_type_move.num_doc,<§cs:3§>
      	Operation_type_move.Num_order,<§cs:3§>
      	Operation_type_move.type_operation,<§cs:3§>
      	Operation_type_move.Etat,<§cs:3§>
      	Operation_type_move.IDPurchase,<§cs:3§>
      	USER.Name<§cs:3§>
      	FROM<§cs:3§>
      	Operation_type_move<§cs:3§>
      	INNER JOIN<§cs:3§>
      	USER<§cs:3§>
      	ON Operation_type_move.IDUser = USER.IDUser<§cs:3§>
      	WHERE<§cs:3§>
      	Operation_type_move.IDSociety = {Param_societID}<§cs:3§>
      	AND<§cs:3§>
      	Operation_type_move.IDPurchase = {Param_PurchaseID}<§cs:3§>
      ]
      SI gnDocID > 0 ALORS
      	sql_operation_type.Param_PurchaseID = gnDocID
      FIN
      sql_operation_type.Param_societID = gnID_societe
      
      soit nrc = HExécuteRequêteSQL(sql_operation_type)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	TableSupprimeTout(TABLE_Operation)
      	POUR TOUT sql_operation_type
      		soit nIndcs = TableAjouteLigne(TABLE_Operation)
      		TABLE_Operation[nIndcs].COL_Id_operation	= sql_operation_type.IDOperation_type_move
      		TABLE_Operation[nIndcs].COL_Date			= sql_operation_type.date_order
      		TABLE_Operation[nIndcs].COL_Réf				= sql_operation_type.Num_order
      		TABLE_Operation[nIndcs].COL_origine			= sql_operation_type.num_doc
      		TABLE_Operation[nIndcs].COL_Par				= sql_operation_type.Name
      		soit typOps									= sql_operation_type.type_operation
      		TABLE_Operation[nIndcs].COL_Statu			= sql_operation_type.Etat
      		TABLE_Operation[nIndcs].COL_Type 			= (typOps = 1 ? "Réception" SINON "livraison"  )
      		TABLE_Operation[nIndcs].COL_id_doc			= sql_operation_type.IDPurchase
      		COMBO_type_operation						= (typOps = 1 ? 1 sinon 2)
      	FIN
      FIN
      
     type : 458752
   -
     name : Echo_detaille_operation
     procedure_id : 2222018257197528988
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Echo_detaille_operation (<xId_operation_op>)
      //
      // Paramètres :
      // 	xId_operation_op : <indiquez ici le rôle de xId_operation_op>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_detaille_operation()
      
      sql_detaille_operation est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Detaille_operation_move.IDArticle,<§cs:3§>
      	Detaille_operation_move.IDunit_of_mesur,<§cs:3§>
      	Detaille_operation_move.IDLot,<§cs:3§>
      	Detaille_operation_move.Quantity,<§cs:3§>
      	Detaille_operation_move.quantit_done,<§cs:3§>
      	Article.name_product,<§cs:3§>
      	Article.Tracability,<§cs:3§>
      	unit_of_mesur.ratio as ration_op,<§cs:3§>
      	unit_of_mesur.Name AS uom_name,<§cs:3§>
      	uom_article.ratio AS ration_product,<§cs:3§>
      	Lot.Name<§cs:3§>
      	FROM<§cs:3§>
      	Detaille_operation_move<§cs:3§>
      	INNER JOIN<§cs:3§>
      	Article<§cs:3§>
      	ON Detaille_operation_move.IDArticle = Article.IDArticle<§cs:3§>
      	INNER JOIN<§cs:3§>
      	unit_of_mesur as uom_article<§cs:3§>
      	ON	Article.IDunit_of_mesur = uom_article.IDunit_of_mesur<§cs:3§>
      	INNER JOIN<§cs:3§>
      	unit_of_mesur<§cs:3§>
      	ON	Detaille_operation_move.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur<§cs:3§>
      	LEFT JOIN<§cs:3§>
      	Lot<§cs:3§>
      	ON Detaille_operation_move.IDLot = Lot.IDLot<§cs:3§>
      	WHERE<§cs:3§>
      	Detaille_operation_move.IDOperation_type_move = {Param_IdOperation}<§cs:3§>
      ]
      
      sql_operation_type est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Operation_type_move.IDOperation_type_move,<§cs:3§>
      	Operation_type_move.date_order,<§cs:3§>
      	Operation_type_move.num_doc,<§cs:3§>
      	Operation_type_move.Num_order,<§cs:3§>
      	Operation_type_move.type_operation,<§cs:3§>
      	Operation_type_move.Etat<§cs:3§>
      	FROM<§cs:3§>
      	Operation_type_move<§cs:3§>
      	WHERE<§cs:3§>
      	Operation_type_move.IDOperation_type_move = {Param_IDOperation}<§cs:3§>
      ]
      
      
      sql_operation_type.Param_IDOperation = xId_operation
      
      soit nrc = HExécuteRequêteSQL(sql_operation_type)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	POUR TOUT sql_operation_type
      		gsTatu = sql_operation_type.Etat
      		si gsTatu = "Terminé" ALORS
      			TABLE_operation_detaille..EnSaisie = Faux
      			BTN_Modifier..Visible	= Faux
      			BTN_Valider..Grisé 		= Vrai
      		sinon
      			
      			TABLE_operation_detaille..EnSaisie = Faux
      			BTN_Modifier..Visible	= Vrai
      			BTN_Valider..Grisé 		= Faux
      		FIN
      		SAI_Date1 				= sql_operation_type.date_order
      		SAI_Document_d_origine	= sql_operation_type.num_doc
      		COMBO_type_operation	= sql_operation_type.type_operation
      		SAI_Réf					= sql_operation_type.Num_order
      	FIN
      FIN
      
      sql_detaille_operation.Param_IdOperation = xId_operation
      nrc = HExécuteRequêteSQL(sql_detaille_operation)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	TableSupprimeTout(TABLE_operation_detaille)
      	POUR TOUT sql_detaille_operation
      		soit nIndcs = TableAjouteLigne(TABLE_operation_detaille)
      		TABLE_operation_detaille[nIndcs].COL_Id_Article			= sql_detaille_operation.IDArticle
      		TABLE_operation_detaille[nIndcs].COL_Article			= sql_detaille_operation.name_product
      		TABLE_operation_detaille[nIndcs].COL_Id_uom				= sql_detaille_operation.IDunit_of_mesur
      		TABLE_operation_detaille[nIndcs].COL_UdM				= sql_detaille_operation.uom_name
      		TABLE_operation_detaille[nIndcs].COL_Id_lot				= sql_detaille_operation.IDLot
      		TABLE_operation_detaille[nIndcs].COL_Lot				= sql_detaille_operation.Name
      		TABLE_operation_detaille[nIndcs].COL_Quantité_à_traiter	= sql_detaille_operation.Quantity
      		TABLE_operation_detaille[nIndcs].COL_tracability		= sql_detaille_operation.Tracability
      		TABLE_operation_detaille[nIndcs].COL_ration_uom_op		= sql_detaille_operation.ration_op
      		TABLE_operation_detaille[nIndcs].COL_ration_uom_product	= sql_detaille_operation.ration_product
      		
      		SI gsTatu = "Terminé" ALORS
      			TABLE_operation_detaille[nIndcs].COL_Quantité_fait		= sql_detaille_operation.quantit_done
      		SINON
      			TABLE_operation_detaille[nIndcs].COL_Quantité_fait		= 0
      		FIN		
      	FIN
      FIN
     type : 458752
