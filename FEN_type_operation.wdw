     name : Num_operation_move_next
     procedure_id : 2222026129873034332
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // [ <Résultat> = ] Num_operation_move_next ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	entier : <indiquez ici les valeurs possibles ainsi que leur interprétation>
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Num_operation_move_next()
      
      soit xNum_order_Next = 0 
      //numéro de commande
      //récupère le numéro
      num_docn est une requête SQL = 
      [
      	SELECT<§cs:3§>
      	IDNumero_document, <§cs:3§>
      	number_next	<§cs:3§>
      	FROM<§cs:3§>
      	Numero_document	<§cs:3§>
      	WHERE<§cs:3§>
      	label = {Operation_move_type}<§cs:3§>
      ]
      
      sql_num_operation est une chaîne
      SELON COMBO_type_operation..Valeur
      	CAS 1
      		num_docn.Operation_move_type = "Operation_move_in"
      		sql_num_operation  = "INSERT INTO Numero_document (label,number_next) VALUES ('Operation_move_in',1)"
      		SORTIR SELON
      	CAS 2
      		num_docn.Operation_move_type = "Operation_move_out"
      		sql_num_operation  = "INSERT INTO Numero_document (label,number_next) VALUES ('Operation_move_out',1)"
      	AUTRE CAS
      		
      FIN
      
      soit nrc = HExécuteRequêteSQL(num_docn)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON SI HNbEnr(num_docn) = 0 // ajoute une 
      	nrc = HExécuteRequêteSQL("Add_mun_session",sql_num_operation)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	FIN
      	xNum_order_Next = 1	 
      SINON SI HNbEnr(num_docn) > 0
      	pour tout num_docn
      		xNum_order_Next = num_docn.number_next
      	FIN
      	
      FIN
      
      RENVOYER xNum_order_Next
