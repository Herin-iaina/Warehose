   -
     name : Add_operation
     procedure_id : 2222072090360170794
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Add_operation (<stat_op> est chaîne)
      //
      // Paramètres :
      //	stat_op (chaîne ANSI) : <indiquez ici le rôle de stat_op>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Add_operation(stat_op , reliquat sont des chaînes)
      
      soit nUm_operation =  Num_operation_move_next()
      
      nUm_operation = (nUm_operation = 0 ? 1 sinon nUm_operation)
      soit Complet_num	= "00000"
      //formatage du numéro de document dans la base en chaine de caractere
      SI Taille(nUm_operation) < 5 ALORS
      	Complet_num = Tronque(Complet_num,Taille(nUm_operation),nombreDeCaractèresASupprimer) + nUm_operation
      SINON
      	Complet_num = nUm_operation
      FIN
      
      
      //Ajout de type d’opération
      sql_add_operation_move est une requête SQL = 
      [
      	INSERT INTO <§cs:3§>
      	Operation_type_move<§cs:3§>
      	(date_order,type_operation,Etat,num_doc,Num_order,IDPurchase,IDUser,IDSociety)<§cs:3§>
      	VALUES<§cs:3§>
      	({date_op},{Type_op},{etat_op},{Num_origine_op},{num_op},{Purchase_id},{userID},{societID})<§cs:3§>
      ]
       soit sNumop_string = ""
      SELON COMBO_type_operation..Valeur
      	
      	CAS 1
      		sNumop_string = ChaîneConstruit("In/%1",Complet_num)
      		SORTIR SELON
      	CAS 2
      		sNumop_string = ChaîneConstruit("Out/%1",Complet_num)
      		SORTIR SELON
      	AUTRE CAS
      		SORTIR SELON
      FIN
      sql_add_operation_move.date_op			= DateDuJour()
      sql_add_operation_move.Type_op			= 1
      sql_add_operation_move.etat_op			= stat_op
      sql_add_operation_move.Num_origine_op	= num_doc + " "  + reliquat
      sql_add_operation_move.num_op			= sNumop_string
      sql_add_operation_move.Purchase_id		= gnDocID 
      sql_add_operation_move.userID			= gnActif_user_id
      sql_add_operation_move.societID			= gnID_societe
      
      soit nrc = HExécuteRequêteSQL(sql_add_operation_move)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	HLitDernier(Operation_type_move,IDOperation_type_move)
      	xId_operation = Operation_type_move.IDOperation_type_move	
      FIN
      
      // mise à jour du numéro d’opération
      UPDATEOPERATION EST une requête SQL = 
      [
      	UPDATE Numero_document SET number_next = {num_next} WHERE label = {Operation_move_type}<§cs:3§>
      ]
      UPDATEOPERATION.num_next = nUm_operation + 1
      
      selon COMBO_type_operation
      	cas 1
      		UPDATEOPERATION.Operation_move_type = "Operation_move_in"
      		sortir selon
      	cas 2
      		UPDATEOPERATION.Operation_move_type = "Operation_move_out"
      	AUTRE CAS
      		
      FIN
      
      nrc = HExécuteRequêteSQL(UPDATEOPERATION)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
     type : 458752
   -
     name : Add_dettaille_and_create_reliquat
     procedure_id : 2222081569353067535
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Add_dettaille_and_create_reliquat ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Add_dettaille_and_create_reliquat()
      
      // Ajout de détaille d’opération
      sql_add_detaille_operation est une requête SQL = 
      [
      	INSERT INTO <§cs:3§>
      	Detaille_operation_move<§cs:3§>
      	(Quantity,quantit_done,IDOperation_type_move,IDArticle,IDLot,IDunit_of_mesur)<§cs:3§>
      	VALUES<§cs:3§>
      	({Qt_op},{Qt_done_op},{operationID},{ArticleID},{LotID},{UoMID})<§cs:3§>
      ]
      
      Update_operation()
      
      reliquat est un entier = 0
      POUR TOUTE LIGNE DE TABLE_operation_detaille
      	soit reste = TABLE_operation_detaille.COL_Quantité_à_traiter - TABLE_operation_detaille.COL_Quantité_fait
      	SI TABLE_operation_detaille.COL_Quantité_fait > 0 ALORS
      		soit quantite = (reste < 0 ? TABLE_operation_detaille.COL_Quantité_à_traiter sinon TABLE_operation_detaille.COL_Quantité_fait )
      		sql_add_detaille_operation.Qt_op		= TABLE_operation_detaille.COL_Quantité_à_traiter
      		sql_add_detaille_operation.Qt_done_op	= quantite
      		sql_add_detaille_operation.operationID	= xId_operation
      		sql_add_detaille_operation.ArticleID	= TABLE_operation_detaille.COL_Id_Article
      		sql_add_detaille_operation.UoMID		= TABLE_operation_detaille.COL_Id_uom
      		sql_add_detaille_operation.LotID		= TABLE_operation_detaille.COL_Id_lot
      		
      		soit nrc = HExécuteRequêteSQL(sql_add_detaille_operation)
      		SI PAS nrc ALORS
      			HErreurInfo(hErrComplet)
      		FIN
      	FIN
      	SI reste > 0 ALORS
      		reliquat += 1
      	FIN
      FIN
      
      Add_stock_Avaible_and_move(Faux)
      
      SI reliquat > 0 ALORS
      	Add_operation("Nouveau", "Reliquat" )
      	POUR TOUTE LIGNE DE TABLE_operation_detaille
      		soit reste = TABLE_operation_detaille.COL_Quantité_à_traiter - TABLE_operation_detaille.COL_Quantité_fait
      		SI reste > 0 ALORS
      			sql_add_detaille_operation.Qt_op		= reste
      			sql_add_detaille_operation.Qt_done_op	=  0
      			sql_add_detaille_operation.operationID	= xId_operation
      			sql_add_detaille_operation.ArticleID	= TABLE_operation_detaille.COL_Id_Article
      			sql_add_detaille_operation.UoMID		= TABLE_operation_detaille.COL_Id_uom
      			sql_add_detaille_operation.LotID		= TABLE_operation_detaille.COL_Id_lot
      			
      			soit nrc = HExécuteRequêteSQL(sql_add_detaille_operation)
      			SI PAS nrc ALORS
      				HErreurInfo(hErrComplet)
      			FIN
      		FIN
      	FIN
      FIN
