   -
     name : Add_stock_Avaible_and_move
     procedure_id : 2222123763115367347
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Add_stock_Avaible_and_move (<bISdone> est booléen)
      //
      // Paramètres :
      //	bISdone (booléen) : <indiquez ici le rôle de dhdate>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	TABLE_transfert : <indiquez ici le rôle de TABLE_transfert>
      // 	COMBO_warehouse : <indiquez ici le rôle de COMBO_warehouse>
      // 	COMBO_destination : <indiquez ici le rôle de COMBO_destination>
      // 	rQte_source_init : <indiquez ici le rôle de rQte_source_init>
      // 	COL_Quantité_à_transferer : <indiquez ici le rôle de COL_Quantité_à_transferer>
      // 	IDTranfert : <indiquez ici le rôle de IDTranfert>
      PROCEDURE Add_stock_Avaible_and_move(bISdone est un booléen)
      
      // sélection emplacement source
      soit xid_source_stock = 0.00
      sql_destination est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Warehouse.IDWarehouse<§cs:3§>
      	FROM<§cs:3§>
      	Warehouse<§cs:3§>
      	WHERE<§cs:3§>
      	Warehouse.Type_Empacement = 3<§cs:3§>
      	AND<§cs:3§>
      	Warehouse.IDSociety = {SocietID}<§cs:3§>
      ]
      sql_destination.SocietID = gnID_Ware_House
      soit nrc = HExécuteRequêteSQL(sql_destination)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON SI HNbEnr(sql_destination) > 0
      	POUR TOUT sql_destination
      		xid_source_stock = sql_destination.IDWarehouse
      	FIN
      SINON SI HNbEnr(sql_destination) = 0 // ajout s'il n a pas de stock source
      	gSql_codes_Warehouse est une Requête SQL = 
      	[
      	INSERT INTO<§cs:3§>
      	Warehouse<§cs:3§>
      	(Name,Type_Empacement,Removal_strategy,IDSociety)<§cs:3§>
      	VALUES <§cs:3§>
      	('Stock client',2,'FIFO',{ID_societe})<§cs:3§>
      	]
      	gSql_codes_Warehouse.ID_societe = gnID_societe
      	nrc = HExécuteRequêteSQL(gSql_codes_Warehouse)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	FIN
      	HLitDernier(Warehouse,IDWarehouse)
      	xid_source_stock = Warehouse.IDWarehouse
      FIN
      
      
      //Mise à jour de la quantité de l'emplacement destination
      Sql_Select_Qte_destination est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	Available_quantity.Qantity AS Quantite,<§cs:3§>
      	Available_quantity.IDAvailable_quantity AS IDAvailable_quantity<§cs:3§>
      	FROM<§cs:3§>
      	Available_quantity<§cs:3§>
      	WHERE<§cs:3§>
      	Available_quantity.IDArticle = {ID_Art}<§cs:3§>
      	AND<§cs:3§>
      	Available_quantity.IDLot	= {ID_lot}<§cs:3§>
      	AND<§cs:3§>
      	Available_quantity.IDWarehouse = {StockID}<§cs:3§>
      ]
      Sql_Upadte_Source est une requête SQL = 
      [
      	UPDATE <§cs:3§>
      	Available_quantity<§cs:3§>
      	SET<§cs:3§>
      	Available_quantity.Qantity = {Source_qte}<§cs:3§>
      	WHERE<§cs:3§>
      	Available_quantity.IDAvailable_quantity = {ID_Avalaible}<§cs:3§>
      ]
      
      //Ajout de la quantité de l'emplacement de destination
      SQL_add_Quanity_dest est une requête SQL = 
      [
      	INSERT INTO Available_quantity<§cs:3§>
      	( <§cs:3§>
      		IDArticle,	<§cs:3§>
      		IDLot,	<§cs:3§>
      		IDWarehouse,	<§cs:3§>
      		Qantity<§cs:3§>
      	)<§cs:3§>
      VALUES<§cs:3§>
      	( <§cs:3§>
      		{Param_IDArticle},	<§cs:3§>
      		{Param_IDLot},	<§cs:3§>
      		{Param_IDWarehouse},	<§cs:3§>
      		{Param_Qantity}<§cs:3§>
      	 )<§cs:3§>
      ]
      
      //ajout de mouvement de stock
      Qsl_add_stock_move est une requête SQL = 
      [
      	INSERT INTO<§cs:3§>
      	Stock_move<§cs:3§>
      	(date_move,Ref_move,Qantity,IDAvailable_quantity,IDArticle,IDSource,IDestination,Qantity_initial,Ecart,IDUser,IDOperation_type_move	)<§cs:3§>
      	VALUES<§cs:3§>
      	({Date_Mvt},{ref},{Qte},{id_Available},{Id_product},{SourceID},{DestinationID},{Qte_init},{Ecart},{user},{IDOperation})<§cs:3§>
      ]
      
      
      
      pour TOUTE LIGNE DE TABLE_operation_detaille
      	// destination
      	soit rQte_init_dest  = 0.00
      	soit rQte_final_dest = 0.00
      	soit rID_Available_dest = 0.00
      	soit rAtio_op 		 = 0.00
      	soit rAtio_product	 = 0.00
      	Soit rQteToAdd		 = 0.00
      	soit qte_done = (bISdone = Vrai ? TABLE_operation_detaille.COL_Quantité_à_traiter sinon TABLE_operation_detaille.COL_Quantité_fait)
      	SI qte_done > 0 ALORS
      		rAtio_op 		= TABLE_operation_detaille.COL_ration_uom_op
      		rAtio_product 	= TABLE_operation_detaille.COL_ration_uom_product
      		rQteToAdd		= qte_done * rAtio_op / rAtio_product
      		Sql_Select_Qte_destination.ID_Art = TABLE_operation_detaille.COL_Id_Article
      		SI TABLE_operation_detaille.COL_Id_lot > 0 ALORS // si le lot est définie
      			Sql_Select_Qte_destination.ID_lot = TABLE_operation_detaille.COL_Id_lot
      		FIN
      		Sql_Select_Qte_destination.StockID= COMBO_destination.COL_IDWarehouse
      		nrc = HExécuteRequêteSQL(Sql_Select_Qte_destination)
      		SI PAS nrc ALORS
      			HErreurInfo(hErrComplet)
      		SINON SI HNbEnr(Sql_Select_Qte_destination) > 0
      			//Update
      			POUR TOUT Sql_Select_Qte_destination
      				rQte_init_dest 		= Sql_Select_Qte_destination.Quantite
      				rID_Available_dest	= Sql_Select_Qte_destination.IDAvailable_quantity
      			FIN
      			rQte_final_dest = rQteToAdd + rQte_init_dest
      			Sql_Upadte_Source.ID_Avalaible	= rID_Available_dest
      			Sql_Upadte_Source.Source_qte	= rQte_final_dest
      			nrc = HExécuteRequêteSQL(Sql_Upadte_Source)
      			SI PAS nrc ALORS
      				HErreurInfo(hErrComplet)
      			FIN
      			HLibèreRequête(Sql_Upadte_Source)
      			
      		SINON SI HNbEnr(Sql_Select_Qte_destination) = 0
      			//Ajout
      			SQL_add_Quanity_dest.Param_IDArticle	= TABLE_operation_detaille.COL_Id_Article
      			SQL_add_Quanity_dest.Param_IDLot		= TABLE_operation_detaille.COL_Id_lot
      			SQL_add_Quanity_dest.Param_IDWarehouse	= COMBO_destination.COL_IDWarehouse
      			SQL_add_Quanity_dest.Param_Qantity		= rQteToAdd
      			nrc= HExécuteRequêteSQL(SQL_add_Quanity_dest)
      			SI PAS nrc ALORS
      				HErreurInfo(hErrComplet)
      			FIN
      			HLibèreRequête(SQL_add_Quanity_dest)
      			
      			HLitDernier(Available_quantity,IDAvailable_quantity)
      			rID_Available_dest = Available_quantity.IDAvailable_quantity
      						
      		FIN
      		//Mouvement de stock
      		dhDateMouv est une dateheure // date et heure de mouvement 
      		dhDateMouv..PartieDate = DateSys()
      		dhDateMouv..PartieHeure = HeureSys()
      		Qsl_add_stock_move.Date_Mvt		= dhDateMouv
      		Qsl_add_stock_move.ref			= SAI_Réf
      		Qsl_add_stock_move.Qte			= rQteToAdd
      		Qsl_add_stock_move.id_Available	= rID_Available_dest
      		Qsl_add_stock_move.Id_product	= TABLE_operation_detaille.COL_Id_Article
      		Qsl_add_stock_move.SourceID		= xid_source_stock
      		Qsl_add_stock_move.DestinationID= COMBO_destination.COL_IDWarehouse
      		Qsl_add_stock_move.Qte_init		= rQte_init_dest
      		Qsl_add_stock_move.Ecart		= rQteToAdd
      		Qsl_add_stock_move.user			= gnActif_user_id
      		Qsl_add_stock_move.IDOperation	= xId_operation
      		
      		nrc = HExécuteRequêteSQL(Qsl_add_stock_move)
      		SI PAS nrc ALORS
      			HErreurInfo(hErrComplet)
      		FIN
      		HLibèreRequête(Qsl_add_stock_move)
      		HLibèreRequête(Sql_Select_Qte_destination)
      	FIN
      FIN
      	
     type : 458752
