  -
    name : BTN_Valider
    identifier : 0x2ed6176f381ac152
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         Sablier(Vrai)
         soit nboccrs = TABLE_operation_detaille..Occurrence
         for i = nboccrs à 1 pas -1 
         	si TABLE_operation_detaille[i].COL_tracability = vrai _et_ TABLE_operation_detaille[i].COL_Id_lot = 0 ALORS
         		soit lot_error = ChaîneConstruit("L'article %1 devrai avoir un lot" + rc + "Linge : %2",TABLE_operation_detaille[i].COL_Article, i)
         		ToastSupprimeTout()
         		ToastAffiche(lot_error,toastCourt,cvMilieu,chCentre,BleuFoncé)
         		retour
         	FIN
         END
         
         si nboccrs > 0 ALORS
         	si gbIsNew =Vrai ALORS
         		Add_operation_move_detaille()
         		Add_stock_Avaible_and_move(Vrai)
         	sinon
         		soit total_fait = TABLE_operation_detaille.COL_Quantité_fait[ligneTotal]
         		SI total_fait = 0 ALORS
         			//1 : Réceptionner tout
         			//2 : Annuler
         			SELON Dialogue(<§?1ED6514D0B34E0D2§>)
         				// Réceptionner tout
         				CAS 1
         					delete_detaitaille_operation()
         					Add_detaille_operation_no_reliquat()
         					Add_stock_Avaible_and_move(Vrai)
         				// Annuler
         				CAS 2
         					RETOUR
         			FIN
         		SINON
         			delete_detaitaille_operation()
         			Add_dettaille_and_create_reliquat()
         		FIN
         	FIN
         FIN
         
         Echo_detaille_operation()
         Echo_data_operation()
         Update_stat_purchase()
         
         Sablier(Faux)
         
         ToastSupprimeTout()
         ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
         
         
         
         
