     p_codes :
      -
        code : |1+
         PROCEDURE MaFenêtre()
      -
        code : |1-
         procédure interne Actualiser()
         	NumbreCustomer est une requête SQL = 
         	[
         	SELECT 
         	COUNT(Partners.IDPartners) AS partnerNumbers
         	FROM
         	Partners
         	WHERE
         	Partners.Is_supplier = 1
         	AND
         	actifs = 1
         	AND
         	Partners.IDSociety = {IDSocietyUser}
         	]
         	
         	NumbreCustomer.IDSocietyUser = gnID_societe
         	soit xNumbers = 0
         	soit nrc = HExécuteRequêteSQL(NumbreCustomer)
         	SI nrc = Faux ALORS
         		HErreurInfo(hErrComplet)
         	SINON
         		
         		POUR TOUT NumbreCustomer 
         			xNumbers = NumbreCustomer.partnerNumbers
         		FIN
         		SI 999 < xNumbers < 1000000 ALORS
         			LIB_135 = xNumbers / 1000 + "K"
         		sinon si 1000000 <= xNumbers  
         			LIB_135 = xNumbers / 1000000 + "M"
         		SINON
         			LIB_135 = xNumbers
         		FIN
         		
         	FIN
         	HLibèreRequête(NumbreCustomer)
         fin
         
         Actualiser()
         timer(Actualiser,18000)
