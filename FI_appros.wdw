     code : |1-
      PROCEDURE AddPurchaseOrderLine(ID_purchase,ID_article,qte_com,ID_Partner)
      
      // ajout de détaille de commande d'achat
      sql_AddPurchaseOrderLine est une requête SQL = 
      [
      	INSERT INTO 
      	detaille_purchase
      	(IDPurchase,IDArticle,Quantity,IDunit_of_mesur,Purchase_price,Discount,IDTaxe,sub_ttl_ht,sub_ttl_ttc)
      	VALUES
      	({Purchase_ID},{Product_Id},{Qte},{UOM_id},{Priceht},{Rem_val},{Vat_ID},{line_ttl_ht},{line_ttl_ttc})
      ]
      
      sql_select_ppp est une requête SQL = 
      [
      	SELECT 
      	product_per_partners.Price,
      	product_per_partners.IDunit_of_mesur,
      	unit_of_mesur.ratio
      	FROM
      	product_per_partners
      	INNER JOIN
      	unit_of_mesur 
      	ON product_per_partners.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur
      	WHERE
      	product_per_partners.IDArticle = {param_ProductID}
      	AND
      	product_per_partners.IDPartners = {Param_PartnerID}
      ]
      
      sql_select_product est une requête SQL = 
      [
      	select 
      	Article.Purchase_price,
      	Article.IDUOM_P,
      	article.Id_TaxePurchase,
      	unit_of_mesur.ratio,
      	uom_stock.ratio as ratio_prod,
      	Taxe.IDTaxe,
      	taxe.Valeur_taxe
      	from
      	Article
      	inner join
      	unit_of_mesur
      	on unit_of_mesur.IDunit_of_mesur = Article.IDUOM_P 
      	INNER JOIN
      	unit_of_mesur uom_stock
      	ON uom_stock.IDunit_of_mesur = Article.IDunit_of_mesur		
      	left join	
      	taxe
      	on Taxe.Valeur_taxe = Article.Id_TaxePurchase
      	where
      	Article.IDArticle = {Param_Idarticle}
      ]
      
      sql_select_ppp.param_ProductID = ID_article
      sql_select_ppp.Param_PartnerID = ID_Partner
      soit nrc = HExécuteRequêteSQL(sql_select_ppp)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      
      xDRatio_p, xDPrice_p, xDIdUom_p,xratiostock, xTaxe_val, xTaxeID, xPriceHT, xPriceTTC sont des numériques // valeur par defaut
      nrc = HExécuteRequêteSQL(sql_select_product)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      sinon
      	POUR TOUT sql_select_product 
      		xDRatio_p = sql_select_product.ratio
      		xratiostock= sql_select_product.ratio_prod
      		xDPrice_p = sql_select_product.Purchase_price
      		xDIdUom_p = sql_select_product.IDUOM_P
      		xTaxeID	  = sql_select_product.IDTaxe
      		xTaxe_val = sql_select_product.Valeur_taxe
      	FIN
      FIN
      
      
      xRatio_p, xPrice_p sont des numériques
       nIdUom_p sont des entiers
      SI HNbEnr(sql_select_ppp) > 0 ALORS
      	POUR TOUT sql_select_ppp 
      		xRatio_p = sql_select_ppp.ratio
      		xPrice_p = sql_select_ppp.Price
      		nIdUom_p = sql_select_ppp.IDunit_of_mesur
      	FIN
      SINON
      	xRatio_p = xDRatio_p
      	xPrice_p = xDPrice_p
      	nIdUom_p = xDIdUom_p
      FIN
      
      qte_com = qte_com * xratiostock / xRatio_p
      xPriceHT = qte_com * xPrice_p
      soit taxettl = qte_com * xPrice_p * xTaxe_val / 100
      xPriceTTC = xPriceHT + taxettl
      
      sql_AddPurchaseOrderLine.Purchase_ID	= ID_purchase
      sql_AddPurchaseOrderLine.Product_Id		= ID_article
      sql_AddPurchaseOrderLine.Qte			= qte_com
      sql_AddPurchaseOrderLine.UOM_id			= nIdUom_p
      sql_AddPurchaseOrderLine.Priceht		= xPrice_p
      sql_AddPurchaseOrderLine.Rem_val		= 0
      sql_AddPurchaseOrderLine.Vat_ID			= xTaxeID
      sql_AddPurchaseOrderLine.line_ttl_ht	= xPriceHT
      sql_AddPurchaseOrderLine.line_ttl_ttc	= xPriceTTC
      
      nrc = HExécuteRequêteSQL(sql_AddPurchaseOrderLine)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
