      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] AddPurchaseOrder (<id_partner>)
      //
      // Paramètres :
      //	id_partner : <indiquez ici le rôle de id_partner>
      // Valeur de retour :
      // 	Type indéterminé : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE AddPurchaseOrder(id_partner)
      
      sql_addPurchaseOrder est une requête SQL = 
      [
      	INSERT INTO 
      	Purchase
      	(Create_date,Note,Stat,Etat,IDPartners,IDSociety,IDUser,Num_order)
      	VALUES 
      	({DateOder},{Note_Po},{Stat_Po},{Etat_Po},{SuplyerID},{SocietID},{UserID},{Num_Po})
      ]
      
      
      soit gnNumOrder 	= Num_purchase_order()
      soit Complet_num	= "00000"
      //formatage du numéro de document dans la base en chaine de caractere
      SI Taille(gnNumOrder) < 5 ALORS
      	Complet_num = Tronque(Complet_num,Taille(gnNumOrder),nombreDeCaractèresASupprimer) + gnNumOrder
      SINON
      	Complet_num = gnNumOrder
      FIN
      
      sql_addPurchaseOrder.DateOder	= DateDuJour()
      sql_addPurchaseOrder.Note_Po	= ""
      sql_addPurchaseOrder.Stat_Po	= Vrai
      sql_addPurchaseOrder.Etat_Po	= "Brouillon"
      sql_addPurchaseOrder.SuplyerID	= id_partner
      sql_addPurchaseOrder.SocietID	= gnID_societe
      sql_addPurchaseOrder.UserID		= gnActif_user_id
      sql_addPurchaseOrder.Num_Po		= ChaîneConstruit("PO/%1",Complet_num)
      soit nrc = HExécuteRequêteSQL(sql_addPurchaseOrder)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      HLitDernier(Purchase,IDPurchase)
      soit gnId_purchase = Purchase.IDPurchase
      Update_num_Purchase_Order()
      
      renvoyer gnId_purchase
