     name : Create_purchase_order
     procedure_id : 2222719728819850192
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Create_purchase_order (<nid_aricle>, <nId_partner>, <rQuantite_regle>)
      //
      // Paramètres :
      //	nid_aricle : <indiquez ici le rôle de nid_aricle>
      //	nId_partner : <indiquez ici le rôle de nId_partner>
      //	rQuantite_regle : <indiquez ici le rôle de rQuantite_regle>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	rQuantite_to_recepte : <indiquez ici le rôle de rQuantite_to_recepte>
      PROCEDURE Create_purchase_order(nid_aricle,nId_partner,rQuantite_regle)
      
      soit rQuantite_to_recepte 	= 0.00
      Sql_verifier_reception est une requête SQL = 
      [
      	SELECT 
      	((Detaille_operation_move.Quantity * unit_of_mesur.ratio) / uom_article.ratio) AS quantite
      	FROM
      	Detaille_operation_move
      	INNER JOIN
      	unit_of_mesur
      	ON unit_of_mesur.IDunit_of_mesur = Detaille_operation_move.IDunit_of_mesur
      	INNER JOIN 	
      	Article
      	ON Article.IDArticle = Detaille_operation_move.IDArticle
      	INNER JOIN
      	Operation_type_move
      	ON Operation_type_move.IDOperation_type_move = Detaille_operation_move.IDOperation_type_move
      	INNER JOIN
      	unit_of_mesur uom_article
      	ON uom_article.IDunit_of_mesur = Article.IDunit_of_mesur
      	WHERE
      	Operation_type_move.type_operation = 1
      	AND
      	Operation_type_move.Etat = 'Nouveau'
      	AND
      	Article.IDArticle = {Param_IDArticle}
      ]
      
      // Vérifier s'il y a un commande fournisseur de l'article en état nouveau 
      sql_verifie_purchase_order_detaille est une requête SQL = 
      [
      	SELECT 
      	detaille_purchase.IDdeteaill_purchase,
      	unit_of_mesur.ratio,
      	uom_article.ratio AS ratio_prod,
      	((detaille_purchase.Quantity * unit_of_mesur.ratio) / uom_article.ratio)  AS quantite,
      	Purchase.IDPurchase
      	FROM
      	detaille_purchase
      	INNER JOIN  
      	Purchase
      	ON Purchase.IDPurchase = detaille_purchase.IDPurchase
      	INNER JOIN 
      	unit_of_mesur
      	ON unit_of_mesur.IDunit_of_mesur = detaille_purchase.IDunit_of_mesur
      	INNER JOIN 
      	Article
      	ON Article.IDArticle = detaille_purchase.IDArticle
      	INNER JOIN
      	unit_of_mesur uom_article
      	ON uom_article.IDunit_of_mesur = Article.IDunit_of_mesur
      	WHERE
      	Purchase.IDPartners = {Param_IDPartners}
      	AND
      	detaille_purchase.IDArticle = {Param_IDArticle}
      	AND
      	Purchase.Etat = 'Brouillon'
      ]
      // Vérifier s'il y a un commande fournisseur sans l'article en état nouveau 
      sql_purchase_order est une requête SQL = 
      [
      	SELECT 
      	Purchase.IDPurchase
      	FROM
      	Purchase
      	WHERE
      	Purchase.IDPartners = {Param_IDPartners_p}
      	AND
      	Purchase.Etat = 'Brouillon'
      ]
      
      
      Sql_verifier_reception.Param_IDArticle = nid_aricle
      soit nrc = HExécuteRequêteSQL(Sql_verifier_reception)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON SI HNbEnr(Sql_verifier_reception) = 0
      	// générer un commande automatique avec toute la quantité nécessaire du règle de stock
      	//fait
      //	soit nId_purchase = AddPurchaseOrder(nId_partner)
      //	AddPurchaseOrderLine(nId_purchase,nid_aricle,rQuantite_regle,nId_partner)
      	//
      	sql_verifie_purchase_order_detaille.Param_IDPartners	= nId_partner
      	sql_verifie_purchase_order_detaille.Param_IDArticle		= nid_aricle
      	SI PAS  HExécuteRequêteSQL(sql_verifie_purchase_order_detaille) ALORS
      		HErreurInfo(hErrComplet)
      	SINON SI HNbEnr(sql_verifie_purchase_order_detaille) = 0 // on va créer ou ajouter un document d'achat
      		soit nId_purchase		= 0
      		sql_purchase_order.Param_IDPartners_p = nId_partner
      		SI PAS  HExécuteRequêteSQL(sql_purchase_order) ALORS
      			HErreurInfo(hErrComplet)
      		SINON SI HNbEnr(sql_purchase_order) = 0 //on va créer un document d'achat
      			//Fait				
      			AddPurchaseOrderLine(AddPurchaseOrder(nId_partner),nid_aricle,rQuantite_regle,nId_partner)
      		SINON SI HNbEnr(sql_purchase_order) > 0 //on va on va ajouter l'article dans la commande
      			POUR TOUT sql_purchase_order
      				nId_purchase = sql_purchase_order.IDPurchase
      			FIN
      			//fait
      			AddPurchaseOrderLine(nId_purchase,nid_aricle,rQuantite_regle,nId_partner)
      		FIN
      		HLibèreRequête(sql_purchase_order)
      	SINON SI HNbEnr(sql_verifie_purchase_order_detaille) > 0 // Modification du document d'achat
      		soit quantite_commander =  0.00
      		soit id_detailPurchase	= 0
      		soit ratio_uom			= 0.00
      		soit ratio_prod			= 0.00
      		POUR TOUT sql_verifie_purchase_order_detaille
      			id_detailPurchase	= sql_verifie_purchase_order_detaille.IDdeteaill_purchase
      			quantite_commander 	= sql_verifie_purchase_order_detaille.quantite
      			ratio_uom		= sql_verifie_purchase_order_detaille.ratio
      			ratio_prod		= sql_verifie_purchase_order_detaille.ratio_prod
      			
      		FIN
      		
      		SI rQuantite_regle > quantite_commander ALORS
      			// fait
      			rQuantite_regle = rQuantite_regle  * ratio_prod / ratio_uom
      			UpdatePurchaseOrderLine(id_detailPurchase,rQuantite_regle)
      		FIN
      		HLibèreRequête(sql_verifie_purchase_order_detaille)
      	FIN	
      	//
      SINON SI HNbEnr(Sql_verifier_reception) > 0
      
      	// générer un commande automatique avec la quantité manquant pour satisfaire le stock
      	POUR TOUT Sql_verifier_reception
      		rQuantite_to_recepte = Sql_verifier_reception.quantite
      	FIN
      	SI rQuantite_to_recepte < rQuantite_regle ALORS // la quantité  à réceptionner est insuffisante 
      		rQuantite_regle =  rQuantite_regle - rQuantite_to_recepte
      		
      		sql_verifie_purchase_order_detaille.Param_IDPartners	= nId_partner
      		sql_verifie_purchase_order_detaille.Param_IDArticle		= nid_aricle
      		SI PAS  HExécuteRequêteSQL(sql_verifie_purchase_order_detaille) ALORS
      			HErreurInfo(hErrComplet)
      		SINON SI HNbEnr(sql_verifie_purchase_order_detaille) = 0 // on va créer ou ajouter un document d'achat
      			soit nId_purchase		= 0
      			sql_purchase_order.Param_IDPartners_p = nId_partner
      			SI PAS  HExécuteRequêteSQL(sql_purchase_order) ALORS
      				HErreurInfo(hErrComplet)
      			SINON SI HNbEnr(sql_purchase_order) = 0 //on va créer un document d'achat
      				//Fait				
      				AddPurchaseOrderLine(AddPurchaseOrder(nId_partner),nid_aricle,rQuantite_regle,nId_partner)
      			SINON SI HNbEnr(sql_purchase_order) > 0 //on va on va ajouter l'article dans la commande
      				POUR TOUT sql_purchase_order
      					nId_purchase = sql_purchase_order.IDPurchase
      				FIN
      				//fait
      				AddPurchaseOrderLine(nId_purchase,nid_aricle,rQuantite_regle,nId_partner)
      			FIN
      			HLibèreRequête(sql_purchase_order)
      		SINON SI HNbEnr(sql_verifie_purchase_order_detaille) > 0 // Modification du document d'achat
      			soit quantite_commander =  0.00
      			soit id_detailPurchase	= 0
      			soit ratio_uom			= 0.00
      			soit ratio_prod			= 0.00
      			POUR TOUT sql_verifie_purchase_order_detaille
      				id_detailPurchase	= sql_verifie_purchase_order_detaille.IDdeteaill_purchase
      				quantite_commander 	= sql_verifie_purchase_order_detaille.quantite
      				ratio_uom		= sql_verifie_purchase_order_detaille.ratio
      				ratio_prod		= sql_verifie_purchase_order_detaille.ratio_prod
      				
      			FIN
      			
      			SI rQuantite_regle > quantite_commander ALORS
      				// fait
      				rQuantite_regle = rQuantite_regle  * ratio_prod / ratio_uom
      				UpdatePurchaseOrderLine(id_detailPurchase,rQuantite_regle)
      			FIN
      			HLibèreRequête(sql_verifie_purchase_order_detaille)
      		FIN				
      	FIN
      FIN
      HLibèreRequête(Sql_verifier_reception)
