     name : UpdatePurchaseOrderLine
     procedure_id : 2222698782264004147
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //UpdatePurchaseOrderLine (<id_detailPurchase>, <quantiteAcommander>)
      //
      // Paramètres :
      //	id_detailPurchase : <indiquez ici le rôle de id>
      // 	quantiteAcommander : <indiquez ici le rôle de quantiteAcommander>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE UpdatePurchaseOrderLine(id_detailPurchase,quantiteAcommander)
      
      //sélection des information
      //modifier la somme du commande
      sql_SelectTotalOrderLine est une requête SQL = 
      [
      	SELECT 
      	detaille_purchase.Purchase_price,
      	detaille_purchase.Discount,
      	Taxe.Valeur_taxe
      	FROM 
      	detaille_purchase
      	LEFT JOIN
      	Taxe
      	ON Taxe.IDTaxe = detaille_purchase.IDTaxe
      	WHERE
      	detaille_purchase.IDdeteaill_purchase = {param_IDPurchase_line}
      ]
      
      //Modification de ligne de commande achat
      sql_UpdatePurchaseOrderLine est une requête SQL = 
      [
      	UPDATE 
      	detaille_purchase
      	SET
      	detaille_purchase.Quantity = {Param_quantity},
      	detaille_purchase.sub_ttl_ht = {param_sub_ttl_ht},
      	detaille_purchase.sub_ttl_ttc = {param_sub_ttl_ttc}
      	WHERE
      	detaille_purchase.IDdeteaill_purchase = {Param_IDdeteaill_purchase}
      ]
      
      soit rPrix_ht	= 0.00
      soit rPrix_ttc 	= 0.00
      soit rTax_val   = 0.00
      soit rTax_ttl   = 0.00
      soit rPrix_unit	= 0.00
      soit rDiscount	= 0.00
      sql_SelectTotalOrderLine.param_IDPurchase_line = id_detailPurchase
      soit nrc = HExécuteRequêteSQL(sql_SelectTotalOrderLine )
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	POUR TOUT sql_SelectTotalOrderLine
      		rTax_val	= sql_SelectTotalOrderLine.Valeur_taxe
      		rPrix_unit	= sql_SelectTotalOrderLine.Purchase_price
      		rDiscount	= sql_SelectTotalOrderLine.Discount
      	FIN
      FIN
      HLibèreRequête(sql_SelectTotalOrderLine)
      
      rPrix_ht = rPrix_unit * (1 - (rDiscount / 100)) * quantiteAcommander
      rTax_ttl = rPrix_unit * (rTax_val / 100) * quantiteAcommander
      rPrix_ttc = rPrix_ht + rTax_ttl
      
      sql_UpdatePurchaseOrderLine.Param_IDdeteaill_purchase 	= id_detailPurchase
      sql_UpdatePurchaseOrderLine.Param_quantity				= quantiteAcommander
      sql_UpdatePurchaseOrderLine.param_sub_ttl_ht			= rPrix_ht
      sql_UpdatePurchaseOrderLine.param_sub_ttl_ttc			= rPrix_ttc
      nrc = HExécuteRequêteSQL(sql_UpdatePurchaseOrderLine )
      si pas nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      HLibèreRequête(sql_UpdatePurchaseOrderLine)
