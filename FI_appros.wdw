     procedure_id : 2213186558756169223
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // compute_stock_rule ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE compute_stock_rule()
      
      sql_stock_rule est une requête SQL = 
      [
      	SELECT
      	Replenishment_rule.IDReplenishment_rule AS IDReplenishment_rule,
      	Replenishment_rule.IDArticle AS IDArticle,
      	Replenishment_rule.Quantity_max AS Quantity_max,
      	Replenishment_rule.Quantity_min AS Quantity_min,
      	SUM(Available_quantity.Qantity) AS Stock
      	FROM
      	Replenishment_rule
      	INNER JOIN Available_quantity
      	ON Replenishment_rule.IDArticle = Available_quantity.IDArticle
      	WHERE
      	Replenishment_rule.IDSociety = {SocietID}
      	AND
      	Replenishment_rule.Init_date <= {Param_InitDate}
      	AND
      	Replenishment_rule.End_date >= {Param_datEnd}
      	AND
      	Replenishment_rule.Stat = 1
         	GROUP BY
      	IDReplenishment_rule,
      	IDArticle,
      	Quantity_max,
      	Quantity_min
      	having
      	SUM(Available_quantity.Qantity) <= Quantity_min
      ]
      
      sql_Price_article_No_partner est une requête SQL = 
      [
      	SELECT 
      	Article.IDArticle AS IDArticle,
      	Article.name_product AS Produit,
      	Article.Purchase_price AS Price
      	FROM
      	Article
      	WHERE
      	Article.IDArticle = {ProductID}
      ]
      
      sql_Price_article est une requête SQL = 
      [
      	SELECT 
      	Article.IDArticle AS IDArticle,
      	Article.name_product AS Produit,
      	product_per_partners.Price AS Price,
      	product_per_partners.IDPartners AS IDPartners,
      	Partners.Name AS Supplyer
      	FROM
      	Article
      	LEFT OUTER JOIN product_per_partners
      	ON Article.IDArticle = product_per_partners.IDArticle
      	LEFT OUTER JOIN Partners
      	ON product_per_partners.IDPartners = Partners.IDPartners
      	WHERE
      	Article.IDArticle = {ProductID}
      	AND
      	product_per_partners.Qantity <= {Param_Quantite}
      	ORDER BY Price DESC
      ]
      
      soit dateJrs est une date = DateDuJour()
      
      sql_stock_rule.SocietID 		= gnID_societe
      sql_stock_rule.Param_InitDate 	= dateJrs
      sql_stock_rule.Param_datEnd 	= dateJrs
      TableSupprimeTout(TABLE_Appros)
      
      soit nrc = HExécuteRequêteSQL(sql_stock_rule)
      SI PAS nrc ALORS //,COL_ProcéduresGlobales.MaConnexion,hRequêteDéfaut+hRequêteSansCorrection
      	HErreurInfo(hErrComplet)
      SINON SI HNbEnr(sql_stock_rule) > 0
      	
      	TABLE_Appros.COL_prix..Visible = Appros_position
      	soit Qte_appro 				= 0.00
      	soit nid_aricle 			= 0
      	soit nId_partner			= 0
      	POUR TOUT sql_stock_rule
      		soit indecs = TableAjouteLigne(TABLE_Appros)
      		Qte_appro = sql_stock_rule.Quantity_max - sql_stock_rule.Stock
      		Qte_appro = (Qte_appro > 0 ? Qte_appro SINON sql_stock_rule.Quantity_max)
      		sql_Price_article.ProductID 		= sql_stock_rule.IDArticle
      		sql_Price_article.Param_Quantite	= Qte_appro
      		
      		nrc = HExécuteRequêteSQL(sql_Price_article)
      		SI PAS nrc ALORS
      			HErreurInfo(hErrComplet)
      		SINON SI HNbEnr(sql_Price_article) > 0
      			soit exeption = "En cours"
      			POUR TOUT sql_Price_article
      				nid_aricle 			= sql_Price_article.IDArticle
      				nId_partner			= sql_Price_article.IDPartners
      				TABLE_Appros[indecs].COL_Id_Article 	= nid_aricle
      				TABLE_Appros[indecs].COL_Produit		= sql_Price_article.Produit
      				TABLE_Appros[indecs].COL_Id_supplyer	= nId_partner
      				TABLE_Appros[indecs].COL_Fournisseur	= sql_Price_article.Supplyer
      				TABLE_Appros[indecs].COL_Quantité		= Qte_appro
      				TABLE_Appros[indecs].COL_ID_Stock_rule	= sql_stock_rule.IDReplenishment_rule
      				TABLE_Appros[indecs].COL_prix			= sql_Price_article.Price
      				TABLE_Appros[indecs].COL_Etat			= (sql_Price_article.Supplyer <> "" ? "En cours" SINON exeption)
      				TABLE_Appros[indecs].COL_Frn.BTN_frn..Visible	= (sql_Price_article.IDPartners > 0 ? Vrai SINON Faux)
      			FIN
      			Create_purchase_order(nid_aricle,nId_partner,Qte_appro)
      			
      		SINON
      			sql_Price_article_No_partner.ProductID = sql_stock_rule.IDArticle
      			nrc = HExécuteRequêteSQL(sql_Price_article_No_partner)
      			SI PAS nrc ALORS
      				HErreurInfo(hErrComplet)
      			SINON SI HNbEnr(sql_Price_article_No_partner) > 0
      				soit exeption = "Exception"
      				pour tout sql_Price_article_No_partner
      					TABLE_Appros[indecs].COL_Id_Article 	= sql_Price_article_No_partner.IDArticle
      					TABLE_Appros[indecs].COL_Produit		= sql_Price_article_No_partner.Produit
      					TABLE_Appros[indecs].COL_Id_supplyer	= 0
      					TABLE_Appros[indecs].COL_Fournisseur	= ""
      					TABLE_Appros[indecs].COL_Quantité		= Qte_appro
      					TABLE_Appros[indecs].COL_ID_Stock_rule	= sql_stock_rule.IDReplenishment_rule
      					TABLE_Appros[indecs].COL_prix			= sql_Price_article_No_partner.Price
      					TABLE_Appros[indecs].COL_Etat			=  exeption
      					TABLE_Appros[indecs].COL_Frn.BTN_frn..Visible	= Faux
      				FIN
      				
      			FIN
      		FIN
      	FIN
      	
      FIN
      
