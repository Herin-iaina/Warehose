          name : BTN_Valider
          identifier : 0x2e2c60620a6e0349
          internal_properties : 
          type : 4
          code_elements :
           internal_properties : BQAAAAUAAADsbNg3Bwbq07WyZo+5tdIpxUTQLb8vq5nyaOo2gVNL3bKuaE2fYbrEiIZB7FeyD/Rn
           type_code : 4
           p_codes :
            -
              code : |1-
               //Exécute le traitement défini dans le modèle
               ExécuteAncêtre
              type : 14
            -
              code : |1+
               //Exécute le traitement défini dans le modèle
               ExécuteAncêtre
               
               //----------------------------fonction
               //Calcul  de ratio
               Val_ratio est un réel = 1
               selon rType_UdM
               	cas 1
               		Val_ratio = 1
               		
               	cas 2
               		Val_ratio = SAI_Ratio
               		
               	cas 3
               		Val_ratio = 1 / SAI_Ratio
               		
               	AUTRE CAS
               		
               FIN
               
               //Ajout ou modification selon  les contextes (gbIs_news)
               //ajout avant 
               si gbIs_news = Vrai ALORS //Ajout d'unité de mesure
               	soit nrc = HExécuteRequête(REQ_Add_UOM,hVérifieDoublon+hVérifieIntégrité,SAI_Name,Val_ratio,COMBO_Type..Valeur,gnID_societe)
               	si pas nrc ALORS
               		hErreurInfo(errComplet)
               	FIN
               	HLitDernier(unit_of_mesur,IDunit_of_mesur)
               	gnIdUOM  = unit_of_mesur.IDunit_of_mesur
               sinon //Modification
               	soit nrc  = HExécuteRequête(REQ_Update_UOM,hVérifieIntégrité,SAI_Name,Val_ratio,COMBO_Type..Valeur,gnIdUOM)
               	si pas nrc ALORS
               		HErreurInfo(errComplet)
               	FIN
               
               FIN
               echo_data()
               
               // Mettre la fenêtre en mode affichage seul
               //GR_UOM..Etat = AffichageSeulement
               GR_UOM..EnSaisie = Faux
               BTN_Modifier..Visible = Vrai // A modifier
               MoiMême..Grisé = Vrai
               Success_error_message("Opération réussi")
