  procedures :
   -
     name : Echo_Help
     procedure_id : 2174276418497688917
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] Echo_Help (<nVal_type> est entier, <ratio>)
      //
      // Paramètres :
      //	nVal_type (entier) : <indiquez ici le rôle de Val_type>
      //	ratio : <indiquez ici le rôle de ratio>
      // Valeur de retour :
      // 	chaîne ANSI : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_Help(nVal_type est un entier,ratio)
      
      eg est une chaîne
      
      SELON nVal_type
      	CAS 1 
      		eg = "1 de cette unité = unité de référence"
      		ratio = 1
      		ratio..Grisé = Vrai
      		rType_UdM = 1
      			CAS 2
      		eg = "1 de cette unité = ratio * unité de référence"
      		SAI_Ratio..Grisé = Faux
      		DonneFocus(SAI_Ratio)		
      		rType_UdM = 2
      			CAS 3
      		eg = "1 de cette unité = unité de référence / ratio"
      		ratio..Grisé = Faux
      		DonneFocus(SAI_Ratio)
      		rType_UdM = 3
      			AUTRE CAS
      				
      FIN
      renvoyer   eg
     type : 458752
   -
     name : Init_fields
     procedure_id : 2174279497990219511
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Init_fields ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Init_fields()
      
      SAI_Name = ""
      COMBO_Type = 1
      LIB_Eg = "1 de cette unité = unité de référence"
      rType_UdM = 1
      SAI_Ratio = 1
      SAI_Ratio..Grisé = Vrai
      GR_UOM..EnSaisie = Vrai
      BTN_Modifier..Visible = Faux
     type : 458752
   -
     name : echo_data
     procedure_id : 2185275374477042786
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // echo_data ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE echo_data()
      
      //requêtes et projection des enregistrements
      soit  nrc =  HExécuteRequête(REQ_Selecte_ID_uom,hRequêteDéfaut,gnIdUOM)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      POUR TOUT REQ_Selecte_ID_uom 
      	SAI_Name = REQ_Selecte_ID_uom.Name
      	COMBO_Type = REQ_Selecte_ID_uom.type_of_ratio
      	SI REQ_Selecte_ID_uom.type_of_ratio  = 3 ALORS
      		SAI_Ratio = 1 / REQ_Selecte_ID_uom.ratio
      	SINON
      		SAI_Ratio = REQ_Selecte_ID_uom.ratio	 		
      	FIN
      	
      	LIB_Eg = Echo_Help(COMBO_Type..Valeur,SAI_Ratio)
      FIN
     type : 458752
