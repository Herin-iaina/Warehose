            SI ZR_Paiment..Occurrence > 0 _ET_ indcs > 0  ALORS
            	tail est une chaîne = ZR_Paiment.SAI_DONNE..Valeur
            	tail = Tronque(tail,1,nombreDeCaractèresASupprimer)
            	ZR_Paiment[indcs].SAI_DONNE = Arrondi(Val(tail),2)
            	ZR_Paiment[indcs].LIB_monton = ZR_Paiment[indcs].SAI_DONNE
            	Compute_Price_Donne()
            	
            	SI  ZR_Paiment[indcs].SAI_DONNE = 0 ALORS
            		Price_Paid	-= 1
