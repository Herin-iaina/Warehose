          internal_properties : 
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              code : |1+
               occrs est un entier = TABLE_Sale_article..Occurrence
               si occrs > 0 ALORS
               	Sablier(Vrai)
               	occurc_mp est un entier = ZR_Paiment..Occurrence
               	gxTotal_order		= TABLE_Sale_article.COL_TTC[ligneTotal]
               	gxSubTotal_Ht		= TABLE_Sale_article.COL_TTL_HT[ligneTotal]
               	gxTotalVatbTotal_Ht	= TABLE_Sale_article.COL_Taxe_compute[ligneTotal]
               	gxDiscountTTL		= TABLE_Sale_article.COL_Discount_TTL[ligneTotal]
               	LIB_APAYE			= gxTotal_order
               	SAI_Somme_a_payer	= gxTotal_order
               	
               	// vérifie que tous les produit avec suivi on tous un lot
               	POUR i = occrs A 1 PAS -1
               		si TABLE_Sale_article[i].COL_Tracability = vrai _et_ TABLE_Sale_article[i].COL_ID_LOT <= 0 ALORS
               			ToastSupprimeTout()
               			ToastAffiche(ChaîneConstruit("Veillez ajouter un lot sur cet produit: %1 ligne %2",TABLE_Sale_article[i].COL_Product,i),toastCourt,cvMilieu,chCentre,BleuFoncé)
               			TableSelectPlus(TABLE_Sale_article,i)
               			retour
               		FIN
               	FIN
               	
               	si occurc_mp = 0 ALORS
               		DISP_Somme_Paid..Visible	= Vrai
               		DISP_titre_table..Visible	= Faux
               		ZR_Paiment..Visible			= Faux
               		LIB_DUE_VAL..Visible		= Faux
               		BTN_Validate..Grisé 		= Vrai
               		Etat_BTN_Fact(Vrai)
               	sinon
               		Comptut_Donne()
               		xMonton est un numérique
               		rest_pd =  donne - SAI_Somme_a_payer
               		SI rest_pd < 0 ALORS
               			POUR i = 1 A occurc_mp
               				
               				SI i = 1 ALORS
               					xMonton						= SAI_Somme_a_payer
               					ZR_Paiment[i].LIB_DUE_VALs	= xMonton
               					xMonton						= xMonton - ZR_Paiment[i].SAI_DONNE
               				SINON
               					ZR_Paiment[i].LIB_DUE_VALs	= xMonton
               					xMonton						= xMonton - ZR_Paiment[i].SAI_DONNE	
               				FIN
               				ZR_Paiment[i].LIB_CHANGE = ""
               			FIN			
               			LIB_DUE_VAL				= rest_pd * -1
               			LIB_DUE_VAL..Visible	= Vrai
               			BTN_Validate..Grisé		= Vrai
               			Etat_BTN_Fact(Vrai)
               		SINON
               			POUR i = 1 A occurc_mp
               				
               				SI i = 1 ALORS
               					xMonton						= SAI_Somme_a_payer
               					ZR_Paiment[i].LIB_DUE_VALs	= xMonton
               					xMonton						= xMonton - ZR_Paiment[i].SAI_DONNE
               				SINON
               					ZR_Paiment[i].LIB_DUE_VALs	= xMonton
               					xMonton						= xMonton - ZR_Paiment[i].SAI_DONNE	
               				FIN	
               				ZR_Paiment[i].LIB_CHANGE = rest_pd	
               			FIN
               			LIB_DUE_VAL				= rest_pd
               			LIB_DUE_VAL..Visible	= Faux
               			BTN_Validate..Grisé		= Faux
               			Etat_BTN_Fact(Faux)
               		FIN
               	FIN
               	Sablier(Faux)
               	FI_Sale_order..Plan = 2
               	//Nom de client
               	si id_custom	= 0 ALORS
               		BTN_Add_custome..Visible	= Vrai
               		IMG_custome..Visible		= Faux
               		LIB_Custome_name..Visible	= Faux
               		BTN_Del_custome..Visible	= Faux
               		LIB_Custome1..Visible		= Faux
               		BTN_Del_custome1..Visible	= Faux
               	FIN
               sinon
               	ToastSupprimeTout()
               	ToastAffiche(gPoliceTaille(12) + gStylo(Blanc) + "Le panier est vide"+ rc+ "Veuillez ajouter",toastCourt,cvMilieu,chCentre,BleuFoncé)
               FIN
               
