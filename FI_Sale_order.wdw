            PROCEDURE Edite_discount()
            
            Sablier(Vrai)
            indcs est un entier = TableSelect(TABLE_Sale_article)
            nHtr est un entier
            SI TABLE_Sale_article..Occurrence > 0 _ET_ indcs > 0 _ET_ Val(TABLE_Sale_article[indcs].COL_Discount_Val) <= 10 ALORS
            	Pre_val est une chaîne = TABLE_Sale_article[indcs].COL_Discount_Val
            	Pre_val += MoiMême..Libellé
            	si val(Pre_val) <= 100 ALORS
            		TABLE_Sale_article[indcs].COL_Discount_Val = Pre_val
            	FIN
            	string_to_int est une chaîne = TABLE_Sale_article[indcs].COL_Discount_Val
            	xQt_numder est un numérique = val(string_to_int)
            	TABLE_Sale_article[indcs].COL_discount = Arrondi( xQt_numder,2) / 100
            	TABLE_Sale_article[indcs].COL_Diiscount_ch = "Remise [%xQt_numder%] %"
            	sName_P est une chaîne
            	si TABLE_Sale_article[indcs].COL_lot_name = "" ALORS
            		sName_P = TABLE_Sale_article[indcs].COL_Product
            		nHtr = 36
            	sinon 
            		sName_P = TABLE_Sale_article[indcs].COL_Product + rc + TABLE_Sale_article[indcs].COL_lot_name
            		nHtr = 50
            	fin
            	TABLE_Sale_article[indcs].COL_Name_product = ""
            	TABLE_Sale_article[indcs].COL_Name_product	= sName_P + RC +TABLE_Sale_article[indcs].COL_Diiscount_ch
            	nHtr = nHtr + 14
            	TABLE_Sale_article[indcs]..Hauteur = nHtr
            	Compute_Total_price_product(indcs)	
            FIN
            Total_info()
            Sablier(Faux)
