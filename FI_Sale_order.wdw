           code : |1-
            // Résumé : <indiquez ici ce que fait la procédure>
            // Syntaxe :
            // Add_lot_product (<nIdc>)
            //
            // Paramètres :
            // 	nIdc : <indiquez ici le rôle de nIdc>
            // Valeur de retour :
            // 	Aucune
            //
            // Exemple :
            // Indiquez ici un exemple d'utilisation.
            //
            PROCEDURE Add_lot_product(nIdc)
            
            sql_WareHouse est une requête SQL = 
            [
            	SELECT<§cs:6§>
            	Removal_strategy<§cs:6§>
            	FROM<§cs:6§>
            	Warehouse<§cs:6§>
            	WHERE<§cs:6§>
            	IDWarehouse = {id_Wh}<§cs:6§>
            ]
            
            sql_WareHouse.id_Wh = gnId_StockUse
            soit nrc = HExécuteRequêteSQL(sql_WareHouse)
            SI PAS nrc ALORS
            	HErreurInfo(hErrComplet)
            SINON
            	TABLE_Sale_article[nIdc].COL_Name_product = ""
            	POUR TOUT sql_WareHouse
            		SI sql_WareHouse.Removal_strategy = "FIFO" ALORS
            			SQL_Lot est une requête SQL = 
            			[
            				SELECT <§cs:6§>
            				Lot.IDLot AS IDLot,<§cs:6§>
            				lot.Name as lots<§cs:6§>
            				FROM <§cs:6§>
            				Lot<§cs:6§>
            				inner join <§cs:6§>
            				Available_quantity<§cs:6§>
            				on lot.IDLot = Available_quantity.IDLot<§cs:6§>
            				WHERE <§cs:6§>
            				Lot.IDArticle = {Param_IDArticle}<§cs:6§>
            				AND	Lot.Stat = 1<§cs:6§>
            				and Available_quantity.Qantity > 0<§cs:6§>
            				ORDER BY <§cs:6§>
            				Expire_date ASC<§cs:6§>
            			]
            			SQL_Lot.Param_IDArticle = ZR_Produit.SAI_IDArticle
            			nrc = HExécuteRequêteSQL(SQL_Lot)
            			SI PAS nrc ALORS
            				HErreurInfo(hErrComplet)
            			SINON SI HNbEnr(SQL_Lot) > 0
            //				TABLE_Sale_article[nIdc].COL_ID_LOT = SQL_Lot.IDLot
            //				TABLE_Sale_article[nIdc].COL_lot_name = SQL_Lot.lots
            				pour tout SQL_Lot
            					TABLE_Sale_article[nIdc].COL_ID_LOT = SQL_Lot.IDLot
            					TABLE_Sale_article[nIdc].COL_lot_name = SQL_Lot.lots
            				FIN
            				
            			FIN
            		SINON SI sql_WareHouse.Removal_strategy = "LIFO"
            			SQL_Lot est une requête SQL = 
            			[
            				SELECT <§cs:6§>
            				Lot.IDLot AS IDLot,<§cs:6§>
            				Lot.Name AS lots<§cs:6§>
            				FROM <§cs:6§>
            				Lot<§cs:6§>
            				WHERE <§cs:6§>
            				Lot.IDArticle = {Param_IDArticle}<§cs:6§>
            				AND	Lot.Stat = 1<§cs:6§>
            				ORDER BY <§cs:6§>
            				Expire_date ASC<§cs:6§>
            			]
            			SQL_Lot.Param_IDArticle = ZR_Produit.SAI_IDArticle
            			nrc = HExécuteRequêteSQL(SQL_Lot)
            			SI PAS nrc  ALORS
            				HErreurInfo(hErrComplet)
            			SINON SI HNbEnr(SQL_Lot)
            				pour tout SQL_Lot
            					TABLE_Sale_article[nIdc].COL_ID_LOT = SQL_Lot.IDLot
            					TABLE_Sale_article[nIdc].COL_lot_name = SQL_Lot.lots
            				FIN
            				
            			FIN
            		FIN
            	FIN
            FIN
