     procedure_id : 2203486267771128506
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Param_sale ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Param_sale()
      
      
      Num_order sont des chaîne
      gnId_param_Pos, sNum_order_val sont des entier sans signe sur 8 octets
      
      REQ_Select_Param_Sale.Param_IDSociety = gnID_societe
      soit nrc = HExécuteRequête(REQ_Select_Param_Sale)
      SI PAS nrc ALORS 
      	HErreurInfo(hErrComplet)
      SINON 
      	
      	POUR TOUT REQ_Select_Param_Sale
      		gnId_param_Pos = REQ_Select_Param_Sale.IDParametre_vente
      		bPrix_TTC	= REQ_Select_Param_Sale.Prix_ttc
      		bDiscount	= REQ_Select_Param_Sale.Discount
      		bInV		= REQ_Select_Param_Sale.Invoicing
      		sHeader		= REQ_Select_Param_Sale.Header
      		sFooter		= REQ_Select_Param_Sale.Footer
      		Num_order	= REQ_Select_Param_Sale.Num_order
      		gbDirectPrint = REQ_Select_Param_Sale.ShowPrint
      		gnId_StockUse = REQ_Select_Param_Sale.IDWarehouse
      	FIN
      	BTN_discount..Grisé		= PAS bDiscount
      	BTN_Cancel_taxe..Grisé	= bPrix_TTC
      	
      	
      	
      	sql_MP est une requête SQL =
      	[
      		SELECT <§cs:6§>
      			IDParametre_vente,	<§cs:6§>
      			Paiment_Methode_POS.IDMoyen_paiement,<§cs:6§>
      			Journal<§cs:6§>
      		FROM<§cs:6§>
      			Paiment_Methode_POS<§cs:6§>
      		INNER JOIN Moyen_paiement ON Paiment_Methode_POS.IDMoyen_paiement = Moyen_paiement.IDMoyen_paiement<§cs:6§>
      		WHERE<§cs:6§>
      			IDParametre_vente = {id_Pos}<§cs:6§>
      	]
      	sql_MP.id_Pos = gnId_param_Pos
      	nrc = HExécuteRequêteSQL(sql_MP)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	SINON SI HNbEnr(sql_MP) > 0
      		
      		indcs est un entier
      		POUR TOUT sql_MP
      			indcs = ZoneRépétéeAjouteLigne(ZR_Moyen_Paid)
      			ZR_Moyen_Paid[indcs].BTN_MP..Libellé = sql_MP.Journal
      			ZR_Moyen_Paid[indcs].LIB_ID_methode  = sql_MP.IDMoyen_paiement
      		FIN
      	FIN
      	sql_Num_O est une requête SQL = 
      	[
      		SELECT <§cs:6§>
      		number_next<§cs:6§>
      		FROM<§cs:6§>
      		Numero_document<§cs:6§>
      		WHERE<§cs:6§>
      		label = 'POS'	<§cs:6§>
      	]
      	nrc = HExécuteRequêteSQL(sql_Num_O)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	SINON
      		pour tout sql_Num_O
      			sNum_order_val = sql_Num_O.number_next
      		FIN
      		
      	FIN
      FIN
      
      
      SI Num_order [=] "%%AAAA%%" ALORS
      	Num_order = Remplace(Num_order,"%%AAAA%%","%1",MotComplet+SansCasse)
      FIN
      SI Num_order [=] "%%MM%%" ALORS
      	Num_order = Remplace(Num_order,"%%MM%%","%2",MotComplet+SansCasse)
      FIN
      SI Num_order [=] "%%JJ%%" ALORS
      	Num_order = Remplace(Num_order,"%%JJ%%","%3",MotComplet+SansCasse)
      FIN
      SI Num_order [=] "%%HH%%" ALORS
      	Num_order = Remplace(Num_order,"%%HH%%","%4",MotComplet+SansCasse)
      FIN
      SI Num_order [=] "%%MN%%" ALORS
      	Num_order = Remplace(Num_order,"%%MN%%","%5",MotComplet+SansCasse)
      FIN
      SI Num_order [=] "%%SS%%" ALORS
      	Num_order = Remplace(Num_order,"%%SS%%","%6",MotComplet+SansCasse)
      FIN
      
      gsNum_session_ch = Num_order
