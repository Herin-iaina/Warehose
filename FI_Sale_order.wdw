     name : Echo_product
     procedure_id : 2205238159260742350
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_product ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_product()
      
      img est un buffer
      sql_article est une requête SQL = 
      [
      	SELECT <§cs:6§>
      	Article.IDArticle AS IDArticle,	<§cs:6§>
      	Article.Image AS Image,	<§cs:6§>
      	Article.name_product AS name_product,	<§cs:6§>
      	Article.Type_product AS Type_product,	<§cs:6§>
      	Article.Description AS Description,	<§cs:6§>
      	Article.Can_be_sale AS Can_be_sale,	<§cs:6§>
      	Article.Sale_price AS Sale_price,	<§cs:6§>
      	Article.IDCategory AS IDCategory,	<§cs:6§>
      	Article.IDunit_of_mesur AS IDunit_of_mesur,	<§cs:6§>
      	Article.Tracability AS Tracability,	<§cs:6§>
      	Article.Methode_of_tracability AS Methode_of_tracability,<§cs:6§>
      	Article.IDTaxe as idtaxe,<§cs:6§>
      	Article.Stat AS Stat,<§cs:6§>
      	Article.Margin,	<§cs:6§>
      	Available_quantity.IDArticle AS IDArticle_Av,	<§cs:6§>
      	Available_quantity.IDWarehouse AS IDWarehouse,	<§cs:6§>
      	SUM(Available_quantity.Qantity) AS la_somme_Qantity,	<§cs:6§>
      	unit_of_mesur.IDunit_of_mesur AS IDunit_of_mesur_un,	<§cs:6§>
      	unit_of_mesur.Name AS Name,	<§cs:6§>
      	Article.IDSociety AS IDSociety,<§cs:6§>
      	taxe.Valeur_taxe<§cs:6§>
      	<§cs:6§>
      	FROM	<§cs:6§>
      	(<§cs:6§>
      	(<§cs:6§>
      	Article<§cs:6§>
      	LEFT OUTER JOIN<§cs:6§>
      	unit_of_mesur<§cs:6§>
      	ON Article.IDunit_of_mesur = unit_of_mesur.IDunit_of_mesur<§cs:6§>
      	)<§cs:6§>
      	LEFT OUTER JOIN<§cs:6§>
      	Available_quantity<§cs:6§>
      	ON Article.IDArticle = Available_quantity.IDArticle<§cs:6§>
      	)<§cs:6§>
      	LEFT OUTER JOIN<§cs:6§>
      	taxe<§cs:6§>
      	on article.IDTaxe = taxe.IDTaxe<§cs:6§>
      	<§cs:6§>
      	WHERE <§cs:6§>
      	(<§cs:6§>
      	Article.Can_be_sale >= 1<§cs:6§>
      	AND	Article.IDSociety = {Param_IDSociety}<§cs:6§>
      	and Available_quantity.IDWarehouse = {Param_IDstock}<§cs:6§>
      	AND	Article.Stat = 1<§cs:6§>
      	)<§cs:6§>
      	GROUP BY <§cs:6§>
      	Article.IDArticle,	<§cs:6§>
      	Article.Image,	<§cs:6§>
      	Article.name_product,	<§cs:6§>
      	Article.Type_product,	<§cs:6§>
      	Article.Description,	<§cs:6§>
      	Article.Can_be_sale,	<§cs:6§>
      	Article.Sale_price,	<§cs:6§>
      	Article.IDCategory,	<§cs:6§>
      	Article.IDunit_of_mesur,	<§cs:6§>
      	Article.Tracability,	<§cs:6§>
      	Article.Methode_of_tracability,<§cs:6§>
      	Article.IDTaxe,	<§cs:6§>
      	Article.Stat,<§cs:6§>
      	Article.Margin,		<§cs:6§>
      	Available_quantity.IDArticle,	<§cs:6§>
      	Available_quantity.IDWarehouse,	<§cs:6§>
      	unit_of_mesur.IDunit_of_mesur,	<§cs:6§>
      	unit_of_mesur.Name,	<§cs:6§>
      	Article.IDSociety,<§cs:6§>
      	taxe.Valeur_taxe<§cs:6§>
      ]
      sql_article.Param_IDSociety = gnID_societe
      sql_article.Param_IDstock = gnId_StockUse
      soit nrc = HExécuteRequêteSQL(sql_article)
      SI PAS nrc  ALORS
      	HErreurInfo(hErrComplet)
      SINON
      	ZoneRépétéeSupprimeTout(ZR_Produit)
      	POUR TOUT sql_article
      		img = sql_article.Image
      		nIndcs est un entier = ZoneRépétéeAjouteLigne(ZR_Produit)
      		ZR_Produit[nIndcs].LIB_Name_product				= sql_article.name_product
      		ZR_Produit[nIndcs].SAI_Description				= sql_article.Description
      		ZR_Produit[nIndcs].SAI_IDArticle				= sql_article.IDArticle
      		ZR_Produit[nIndcs].SAI_IDCategory				= sql_article.IDCategory
      		ZR_Produit[nIndcs].SAI_IDunit_of_mesur			= sql_article.IDunit_of_mesur
      		ZR_Produit[nIndcs].SAI_IDWarehouse				= sql_article.IDWarehouse
      		ZR_Produit[nIndcs].SAI_Methode_of_tracability	= sql_article.Methode_of_tracability
      		ZR_Produit[nIndcs].SAI_Sale_price				= sql_article.Sale_price
      		ZR_Produit[nIndcs].SAI_Type_product				= sql_article.Type_product
      		ZR_Produit[nIndcs].INT_Tracability				= sql_article.Tracability
      		ZR_Produit[nIndcs].LIB_qt						= sql_article.la_somme_Qantity
      		ZR_Produit[nIndcs].LIB_Uom_Name					= sql_article.Name
      		ZR_Produit[nIndcs].SAI_IDAvailable_quantity		= 0
      		ZR_Produit[nIndcs].SAI_IDTaxe					= sql_article.idtaxe
      		ZR_Produit[nIndcs].SAI_Valeur_marge				= sql_article.Margin
      		ZR_Produit[nIndcs].SAI_Valeur_taxe				= sql_article.Valeur_taxe
      		SI Taille(img) > 0 ALORS
      			ZR_Produit[nIndcs].IMG_Produit				= dChargeImage(Décode(img, encodeBASE64))
      		SINON
      			ZR_Produit[nIndcs].IMG_Produit				= IMG_default
      		FIN
      	FIN
      FIN
      Echo_productNotSocakble()
     type : 458752
   -
     name : Update_detaille_session
     procedure_id : 2206713863292588143
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Update_detaille_session (<dhDate> est dateheure, <Id_Sale>)
      //
      // Paramètres :
      //	dhDate (date heure) : <indiquez ici le rôle de Date>
      // 	Id_Sale : <indiquez ici le rôle de Id_Sale>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Update_detaille_session(dhDate est une dateheure,Id_Sale,nID_esepece)
      
      Payement est un tableau associatif de numériques
      Payement_ticket est un tableau associatif de numériques
      xSub_total est un numérique = 0
      //Faire la somme de méthodes de paiement identique au cas ou il y en a plusieurs sur la ligne 
      POUR TOUTE LIGNE DE ZR_Paiment
      	Payement[ZR_Paiment.LIB_ID_PM] += ZR_Paiment.SAI_DONNE
      	Payement_ticket[ZR_Paiment.LIB_methode_mane] += ZR_Paiment.SAI_DONNE
      FIN
      
      //Récupère la somme puis ajoute celle du nouveau commande
      Sql_recup_sum est une requête SQL = 
      [
      	SELECT <§cs:6§>
      	Sous_ttl<§cs:6§>
      	FROM<§cs:6§>
      	Detaille_session<§cs:6§>
      	WHERE<§cs:6§>
      	Detaille_session.IDSession = {SessionId}<§cs:6§>
      	AND<§cs:6§>
      	Detaille_session.id_Methode_Paiement ={IDmethode}<§cs:6§>
      ] 
      
      
      //Mise à jour de détaille de session
      SQl_update_DetailleSession est une requête SQL = 
      [
      	UPDATE <§cs:6§>
      	Detaille_session<§cs:6§>
      	SET<§cs:6§>
      	Sous_ttl = {SubTotal}<§cs:6§>
      	WHERE<§cs:6§>
      	Detaille_session.IDSession = {SessionId}<§cs:6§>
      	AND<§cs:6§>
      	Detaille_session.id_Methode_Paiement ={IDmethode}<§cs:6§>
      ]
      
      
      //Ajout de détaille de paiement
      SQL_Add_Paiement est une requête SQL = 
      [
      	insert into <§cs:6§>
      	Detaille_Paiement<§cs:6§>
      	(date_order,label,Payed,IDSession,IDSale_order,IDMoyen_paiement)<§cs:6§>
      	Values<§cs:6§>
      	({Date_Sale},{Libelle},{Montont},{SessionID},{Id_Sale},{IDPaiement})<§cs:6§>
      ]
      
      
      //Ajout de rendue dans le moyen de paiement type espèce
      SI rest_pd > 0 ALORS
      	SQL_Add_Paiement.Date_Sale	= dhDate
      	SQL_Add_Paiement.SessionID	= gnIDSession
      	SQL_Add_Paiement.ID_sale	= Id_Sale
      	SQL_Add_Paiement.Libelle = "Retour"
      	SQL_Add_Paiement.Montont = rest_pd * -1
      	SQL_Add_Paiement.IDPaiement = nID_esepece
      	soit nrc = HExécuteRequêteSQL(SQL_Add_Paiement)
      	si pas nrc ALORS
      		HErreurInfo(hErrComplet)
      	FIN
      	HLibèreRequête(SQL_Add_Paiement)
      	
      	Somme_ttc est un numérique = 0
      	Sql_recup_sum.IDmethode = nID_esepece
      	Sql_recup_sum.SessionId = gnIDSession
      	nrc = HExécuteRequêteSQL(Sql_recup_sum)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	SINON
      		POUR TOUT Sql_recup_sum
      			Somme_ttc = Sql_recup_sum.Sous_ttl
      		FIN
      	FIN
      	HLibèreRequête(Sql_recup_sum)
      	
      	// mise à jour détaille session
      	SQl_update_DetailleSession.IDmethode = nID_esepece
      	SQl_update_DetailleSession.SessionId = gnIDSession
      	SQl_update_DetailleSession.SubTotal  = Somme_ttc - rest_pd
      	nrc= HExécuteRequêteSQL(SQl_update_DetailleSession)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	FIN
      	HLibèreRequête(SQl_update_DetailleSession)
      	
      FIN
      
      
      POUR TOUT xMontonTT, Id_Methode DE Payement
      
      	Sql_recup_sum.IDmethode = Id_Methode
      	Sql_recup_sum.SessionId = gnIDSession
      	soit nrc = HExécuteRequêteSQL(Sql_recup_sum)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	SINON
      		POUR TOUT Sql_recup_sum
      			xSub_total = Sql_recup_sum.Sous_ttl
      		FIN
      	FIN
      	HLibèreRequête(Sql_recup_sum)
      	// ajout au nouvelle somme
      //	si nID_esepece = Id_Methode ALORS
      //		xSub_total = xSub_total + xMontonTT - rest_pd
      //	sinon
      //		xSub_total = xSub_total + xMontonTT
      //	FIN
      	xSub_total = xSub_total + xMontonTT
      	
      	// mise à jour détaille session
      	SQl_update_DetailleSession.IDmethode = Id_Methode
      	SQl_update_DetailleSession.SessionId = gnIDSession
      	SQl_update_DetailleSession.SubTotal  = xSub_total	
      	nrc = HExécuteRequêteSQL(SQl_update_DetailleSession)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	FIN
      	HLibèreRequête(SQl_update_DetailleSession)
      	
      	//Ajout détaille de paiement
      	SQL_Add_Paiement.Date_Sale	= dhDate
      	SQL_Add_Paiement.SessionID	= gnIDSession
      	SQL_Add_Paiement.ID_sale	= Id_Sale
      	SQL_Add_Paiement.Libelle = "Donné"
