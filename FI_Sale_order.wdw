                   identifier : 0x2e90063e09e5ad3b
                   internal_properties : 
                   type : 4
                   code_elements :
                    type_code : 4
                    p_codes :
                     -
                       code : |1+
                        tableOcc est un entier = 0
                        SI ZR_Produit..Occurrence > 0 ALORS
                        	tableOcc = TableCherche(COL_IDArticle,ZR_Produit.SAI_IDArticle,rechercheIdentique)
                        	SI tableOcc > 0 ALORS				
                        		TABLE_Sale_article[tableOcc].COL_qt_vendue	+= 1.00
                        		TABLE_Sale_article[tableOcc].COL_qt_chaine	= TABLE_Sale_article[tableOcc].COL_qt_vendue 
                        		//TABLE_Sale_article[tableOcc].COL_TTC		= TABLE_Sale_article[tableOcc].COL_qt_vendue * TABLE_Sale_article[tableOcc].COL_Sale_price * (1	+ TABLE_Sale_article[tableOcc].COL_taxe)
                        		SC_client1.Compute_Total_price_product(tableOcc)		
                        		TableSelectPlus(TABLE_Sale_article,tableOcc)
                        	SINON
                        		nIdc est un entier = TableAjouteLigne(TABLE_Sale_article) //TableAjouteLigne(TABLE_Sale_article,ZR_Produit.LIB_Name_product,1,ZR_Produit.LIB_Uom_Name,...
                        		//ZR_Produit.SAI_Sale_price, ZR_Produit.SAI_IDArticle,ZR_Produit.SAI_Type_product,ZR_Produit.SAI_Description,...
                        		//ZR_Produit.SAI_IDCategory,ZR_Produit.SAI_IDunit_of_mesur,ZR_Produit.INT_Tracability,...
                        		//ZR_Produit.SAI_Methode_of_tracability, ZR_Produit.SAI_IDWarehouse,ZR_Produit.LIB_qt,ZR_Produit.SAI_IDAvailable_quantity)
                        		
                        		TABLE_Sale_article[nIdc].COL_Product				= ZR_Produit.LIB_Name_product
                        		TABLE_Sale_article[nIdc].COL_qt_vendue				= 1
                        		TABLE_Sale_article[nIdc].COL_Name					= ZR_Produit.LIB_Uom_Name
                        		TABLE_Sale_article[nIdc].COL_Sale_price				= ZR_Produit.SAI_Sale_price
                        		TABLE_Sale_article[nIdc].COL_IDArticle				= ZR_Produit.SAI_IDArticle
                        		TABLE_Sale_article[nIdc].COL_IDAvailable_quantity	= ZR_Produit.SAI_IDAvailable_quantity
                        		TABLE_Sale_article[nIdc].COL_Description			= ZR_Produit.SAI_Description
                        		TABLE_Sale_article[nIdc].COL_IDunit_of_mesur		= ZR_Produit.SAI_IDunit_of_mesur
                        		TABLE_Sale_article[nIdc].COL_IDWarehouse			= ZR_Produit.SAI_IDWarehouse
                        		TABLE_Sale_article[nIdc].COL_IDCategory				= ZR_Produit.SAI_IDCategory
                        		TABLE_Sale_article[nIdc].COL_La_somme_Qantity		= ZR_Produit.LIB_qt
                        		TABLE_Sale_article[nIdc].COL_Methode_of_tracability	= ZR_Produit.SAI_Methode_of_tracability
                        		TABLE_Sale_article[nIdc].COL_Tracability			= ZR_Produit.INT_Tracability
                        		TABLE_Sale_article[nIdc].COL_Type_product			= ZR_Produit.SAI_Type_product
                        		TABLE_Sale_article[nIdc].COL_TTL_HT					= ZR_Produit.SAI_Sale_price
                        		TABLE_Sale_article[nIdc].COL_idtaxe					= ZR_Produit.SAI_IDTaxe
                        		TABLE_Sale_article[nIdc].COL_taxe					= ZR_Produit.SAI_Valeur_taxe
                        //		TABLE_Sale_article[nIdc].COL_TTC					= ZR_Produit.SAI_Sale_price 
                        //		TABLE_Sale_article[nIdc].COL_Taxe_compute 			= (TABLE_Sale_article[nIdc].COL_Sale_price * TABLE_Sale_article[nIdc].COL_taxe) / 100
                        		TABLE_Sale_article[nIdc].COL_Diiscount_ch 			= ""
                        		TABLE_Sale_article[nIdc].COL_Discount_Val			= 0
                        		TABLE_Sale_article[nIdc].COL_discount				= 0
                        		TABLE_Sale_article[nIdc].COL_marge					= ZR_Produit.SAI_Valeur_marge
                        		SC_client1.Compute_Total_price_product(nIdc)
                        		
                        		// Ajout lot selon la traçabilité
                        		si ZR_Produit.INT_Tracability = 1 ALORS
                        			Add_lot_product(nIdc)
                        			BTN_lots..Grisé = Faux
                        		sinon
                        			BTN_lots..Grisé = Vrai
                        		FIN
                        		TABLE_Sale_article[nIdc].COL_Name_product			= (TABLE_Sale_article[nIdc].COL_lot_name = "" ? ZR_Produit.LIB_Name_product sinon  ...
                        		ZR_Produit.LIB_Name_product +rc +"Lot : " + TABLE_Sale_article[nIdc].COL_lot_name)<§aad:0§>
                        		
                        		
                        		SI nIdc > 0 ALORS
                        			TableSelectPlus(TABLE_Sale_article,nIdc)
                        			selon gnActif_bouton
                        				cas 1
                        					DonneFocus(TABLE_Sale_article.COL_qt_vendue)
                        				cas 2
                        					DonneFocus(TABLE_Sale_article.COL_Sale_price)
                        				cas 3
                        					
                        				AUTRE CAS
                        					
                        			FIN
                        			
                        		FIN
                        		
                        	FIN	
                        	SC_client1.Total_info()
                        FIN
                        
