        procedures :
         -
           name : Compute_Total_price_product
           procedure_id : 2205224419658670430
           type_code : 14
           code : |1+
            // Résumé : <indiquez ici ce que fait la procédure>
            // Syntaxe :
            // Compute_Total_price_product ()
            //
            // Paramètres :
            //	Aucun
            // Valeur de retour :
            // 	Aucune
            //
            // Exemple :
            // Indiquez ici un exemple d'utilisation.
            //
            PROCEDURE Compute_Total_price_product() <IHM>
            remise_val, taxe_val,prix_ht sont des numériques
            
            taxe_val =  (TABLE_Sale_article.COL_Sale_price * TABLE_Sale_article.COL_taxe) / 100
            TABLE_Sale_article.COL_Taxe_compute = TABLE_Sale_article.COL_qt_vendue * taxe_val
            remise_val = TABLE_Sale_article.COL_discount
            SI bPrix_TTC = Faux ALORS
            	prix_ht =  TABLE_Sale_article.COL_Sale_price * (1 - remise_val )
            	prix_ht =  TABLE_Sale_article.COL_qt_vendue * prix_ht
            SINON
            	prix_ht = TABLE_Sale_article.COL_Sale_price - taxe_val
            	prix_ht = prix_ht * (1 - remise_val )
            	prix_ht = TABLE_Sale_article.COL_qt_vendue * prix_ht
            FIN
            TABLE_Sale_article.COL_TTL_HT 	= prix_ht //TABLE_Sale_article.COL_qt_vendue * (TABLE_Sale_article.COL_Sale_price - TABLE_Sale_article.COL_discount)
            TABLE_Sale_article.COL_TTC 		= TABLE_Sale_article.COL_TTL_HT + TABLE_Sale_article.COL_Taxe_compute
            
           type : 458752
