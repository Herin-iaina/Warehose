     name : Compute_Price_Donne
     procedure_id : 2203857417373892276
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Compute_Price_Donne ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Compute_Price_Donne() <IHM>
      occurc_mp est un entier = ZR_Paiment..Occurrence
      xMonton est un numérique = 0
      DISP_Paiment[3]..Hauteur	= 42 * occurc_mp
      SI occurc_mp > 0  ALORS
      	Comptut_Donne()
      	rest_pd =  donne - SAI_Somme_a_payer
      	SI rest_pd < 0 ALORS
      		POUR i = 1 A occurc_mp
      			
      			SI i = 1 ALORS
      				xMonton = SAI_Somme_a_payer
      				ZR_Paiment[i].LIB_DUE_VALs = xMonton
      				xMonton = xMonton - ZR_Paiment[i].SAI_DONNE
      			SINON
      				ZR_Paiment[i].LIB_DUE_VALs = xMonton
      				xMonton = xMonton - ZR_Paiment[i].SAI_DONNE	
      			FIN
      			ZR_Paiment[i].LIB_CHANGE = ""
      		FIN			
      		LIB_DUE_VAL = rest_pd * -1
      		LIB_DUE_VAL..Visible = Vrai
      		BTN_Validate..Grisé	 = Vrai
      		Etat_BTN_Fact(Vrai)
      	SINON
      		POUR i = 1 A occurc_mp
      			
      			SI i = 1 ALORS
      				xMonton = SAI_Somme_a_payer
      				ZR_Paiment[i].LIB_DUE_VALs = xMonton
      				xMonton = xMonton - ZR_Paiment[i].SAI_DONNE
      			SINON
      				ZR_Paiment[i].LIB_DUE_VALs = xMonton
      				xMonton = xMonton - ZR_Paiment[i].SAI_DONNE	
      			FIN	
      			SI i <> occurc_mp ALORS
      				ZR_Paiment[i].LIB_CHANGE = ""
      			SINON
      				ZR_Paiment[i].LIB_CHANGE = rest_pd
      			FIN
      		FIN
      		LIB_DUE_VAL = rest_pd
      		LIB_DUE_VAL..Visible = Faux
      		BTN_Validate..Grisé	 = Faux
      		Etat_BTN_Fact(Faux)
      	FIN
      
      FIN
