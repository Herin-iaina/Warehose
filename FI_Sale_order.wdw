           name : Compute_Total_price_product
           procedure_id : 2205196188816332432
           type_code : 14
           code : |1+
            // Résumé : <indiquez ici ce que fait la procédure>
            // Syntaxe :
            // Compute_Total_price_product (<indcs>)
            //
            // Paramètres :
            // 	indcs : <indiquez ici le rôle de indcs>
            // Valeur de retour :
            // 	Aucune
            //
            // Exemple :
            // Indiquez ici un exemple d'utilisation.
            //
            PROCEDURE Compute_Total_price_product(indcs) <IHM>
            prix_ht, remise_val, taxe_val sont des numérique = 0
             
            taxe_val =  (TABLE_Sale_article[indcs].COL_Sale_price * TABLE_Sale_article[indcs].COL_taxe) / 100
             TABLE_Sale_article[indcs].COL_Taxe_compute = TABLE_Sale_article[indcs].COL_qt_vendue * taxe_val
             remise_val = TABLE_Sale_article[indcs].COL_discount
            si bPrix_TTC = Faux ALORS
            	prix_ht =  TABLE_Sale_article[indcs].COL_Sale_price * (1 - remise_val )
            	prix_ht =  TABLE_Sale_article[indcs].COL_qt_vendue * prix_ht
            	TABLE_Sale_article[indcs].COL_Discount_TTL = TABLE_Sale_article[indcs].COL_Sale_price * remise_val * TABLE_Sale_article[indcs].COL_qt_vendue
            sinon
            	prix_ht =   TABLE_Sale_article[indcs].COL_Sale_price - taxe_val
            	TABLE_Sale_article[indcs].COL_Discount_TTL = prix_ht * remise_val * TABLE_Sale_article[indcs].COL_qt_vendue
            	prix_ht = prix_ht * (1 - remise_val )
            	prix_ht = TABLE_Sale_article[indcs].COL_qt_vendue * prix_ht
            fin
            
            //TABLE_Sale_article[indcs].COL_TTL_HT = TABLE_Sale_article[indcs].COL_qt_vendue * (TABLE_Sale_article[indcs].COL_Sale_price - TABLE_Sale_article[indcs].COL_discount) + prix_ht
            TABLE_Sale_article[indcs].COL_TTL_HT = prix_ht
            TABLE_Sale_article[indcs].COL_TTC 	= TABLE_Sale_article[indcs].COL_TTL_HT + TABLE_Sale_article[indcs].COL_Taxe_compute
           type : 458752
