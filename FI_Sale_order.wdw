   -
     name : Change_price
     procedure_id : 2203427577042912683
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Change_price ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Change_price() <IHM>
      
      indcs_mp est un entier = ZoneRépétéeSelect(ZR_Paiment)
      occurc_mp est un entier = ZR_Paiment..Occurrence
      
      
      SI occurc_mp > 0 _ET_ indcs_mp > 0 _ET_ Taille(ZR_Paiment[indcs_mp].LIB_monton) < 24 ALORS
      	si MoiMême..Libellé <> "C" _et_ MoiMême..Libellé <> "c" alors
      		 ZR_Paiment[indcs_mp].LIB_monton += MoiMême..Libellé
      	FIN
      	string_to_int est une chaîne = ZR_Paiment[indcs_mp].LIB_monton
      	xQt_monton est un numérique = val(string_to_int)
      	ZR_Paiment[indcs_mp].SAI_DONNE = Arrondi( xQt_monton,2)
      	
      FIN
     type : 458752
   -
     name : Compute_price
     procedure_id : 2203434315846661904
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Compute_price ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Compute_price(indcs_mp) <IHM>
      
      occurc_mp est un entier = ZR_Paiment..Occurrence
      
      
      sI occurc_mp > 0  ALORS
      	Comptut_Donne()
      	rest_pd = donne - SAI_Somme_a_payer
      	SI rest_pd < 0 ALORS
      		ZR_Paiment[indcs_mp].LIB_CHANGE = 0
      		ZR_Paiment[indcs_mp].LIB_DUE_VALs = rest_pd * -1
      		LIB_DUE_VAL = rest_pd * -1
      		LIB_DUE_VAL..Visible = Vrai
      		BTN_Validate..Grisé	 = Vrai
      		Etat_BTN_Fact(Vrai)
      		POUR i = 1 A occurc_mp
      			ZR_Paiment[i].LIB_CHANGE = ""
      		FIN
      			
      	SINON
      		ZR_Paiment[indcs_mp].LIB_CHANGE = rest_pd
      		LIB_DUE_VAL = rest_pd * -1
      		LIB_DUE_VAL..Visible = Faux
      		BTN_Validate..Grisé	 = Faux
      		Etat_BTN_Fact(Faux)
      		POUR i = 1 A occurc_mp
      			SI i <> occurc_mp ALORS
      				ZR_Paiment[i].LIB_CHANGE = ""
      			SINON
      				ZR_Paiment[i].LIB_CHANGE = rest_pd
      			FIN
      		FIN
      	FIN
      FIN
      
      
