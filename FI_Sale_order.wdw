       name : BTN_Validate
       identifier : 0x2e95a2dc4942026f
       internal_properties : 
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1+
            //Vérifie l’état de la session
            Sablier(Vrai)
            Check_stat_session()
            
            xQt_befor,xQt_affet,xEcart, xId_stock_available,xId_destination,xNum_order_Next,xID_Order sont des numériques
            Complet_num est une chaîne = "00000"
            
            nID_esepece est un entier sur 8 octets =  Select_MP_Espece()
            si rest_pd > 0 _et_ nID_esepece = 0  ALORS
            	Erreur(<§$0000§>)
            	retour
            FIN
            
            // sélection emplacement de destination
            sql_destination est une requête SQL = 
            [
            	select <§cs:6§>
            	Warehouse.IDWarehouse<§cs:6§>
            	from<§cs:6§>
            	Warehouse<§cs:6§>
            	where<§cs:6§>
            	Warehouse.Type_Empacement = 2<§cs:6§>
            	and<§cs:6§>
            	Warehouse.IDSociety = {SocietID}<§cs:6§>
            ]
            sql_destination.SocietID = gnID_Ware_House
            soit nrc = HExécuteRequêteSQL(sql_destination)
            si pas nrc ALORS
            	HErreurInfo(hErrComplet)
            sinon si HNbEnr(sql_destination) > 0
            	pour tout sql_destination
            		xId_destination = sql_destination.IDWarehouse
            	FIN
            sinon si HNbEnr(sql_destination) = 0
            	gSql_codes_Warehouse est une Requête SQL = 
            	[
            	INSERT INTO<§cs:6§>
            	Warehouse<§cs:6§>
            	(Name,Type_Empacement,Removal_strategy,IDSociety)<§cs:6§>
            	VALUES <§cs:6§>
            	('Stock client',2,'FIFO',{ID_societe})<§cs:6§>
            	]
            	gSql_codes_Warehouse.ID_societe = gnID_societe
            	
            	nrc = HExécuteRequêteSQL(gSql_codes_Warehouse)
            	SI PAS nrc ALORS
            		HErreurInfo(hErrComplet)
            	FIN
            	HLitDernier(Warehouse,IDWarehouse)
            	xId_destination = Warehouse.IDWarehouse
            FIN
             
            //Numéro de session
            dhDates est une dateheure
            dhDates..PartieDate	= DateDuJour()
            dhDates..PartieHeure= Maintenant()
            //sNum_session est une chaîne = ChaîneConstruit(gsNum_session_ch,dhDates..Année,dhDates..Mois,dhDates..Jour,dhDates..Heure,dhDates..Minute,dhDates..Seconde)
            
            //numéro de commande
            //récupère le numéro
            num_doc est une requête SQL = 
            [
            	SELECT<§cs:6§>
            	IDNumero_document, <§cs:6§>
            	number_next	<§cs:6§>
            	FROM<§cs:6§>
            	Numero_document	<§cs:6§>
            	WHERE<§cs:6§>
            	label = 'POS_Order'<§cs:6§>
            ]
            nrc = HExécuteRequêteSQL(num_doc )
            SI PAS nrc ALORS
            	HErreurInfo(hErrComplet)
            SINON SI HNbEnr(num_doc) = 0 // ajoute une 
            	nrc = HExécuteRequêteSQL("Add_mun_session","INSERT INTO Numero_document (label,number_next) VALUES ('POS_Order',1)")
            	SI PAS nrc ALORS
            		HErreurInfo(hErrComplet)
            	FIN
            	xNum_order_Next = 1	 
            SINON SI HNbEnr(num_doc) > 0
            	pour tout num_doc
            		xNum_order_Next = num_doc.number_next
            	FIN
            FIN
            //formatage du numéro de session dans la base en chaine de caractere
            SI Taille(xNum_order_Next) < 5 ALORS
            	Complet_num = Tronque(Complet_num,Taille(xNum_order_Next),nombreDeCaractèresASupprimer) + xNum_order_Next
            SINON
            	Complet_num = xNum_order_Next
            FIN
            sNum_Commande est une chaîne = ChaîneConstruit(gsNum_session_ch,dhDates..Année,dhDates..Mois,dhDates..Jour,dhDates..Heure,dhDates..Minute,dhDates..Seconde)
            sNum_Commande = sNum_Commande +"/" + Complet_num
            
            
            //////////////////////////////gxDiscountTTL
            //Ajout des ligne de vente
            SQl_add_sale_order est une requête SQL = 
            [
            	INSERT INTO <§cs:6§>
            	Sale_order<§cs:6§>
            	(Ref_order,Num_order,date_order,Total_TTC,Session,IDUser,IDPartners,IDSociety)<§cs:6§>
            	VALUES<§cs:6§>
            	({Ref_command},{Num_commande},{date_commande},{TTL_commande},{SessioID},{UserID},{CustomerID},{SocietID})<§cs:6§>
            ]
            SQl_add_sale_order.Ref_command		= ChaîneConstruit("Vente du %1/%2/%3",dhDates..Jour,dhDates..Mois,dhDates..Année)
            SQl_add_sale_order.Num_commande		= sNum_Commande
            SQl_add_sale_order.date_commande	= dhDates
            SQl_add_sale_order.TTL_commande		= gxTotal_order
            SQl_add_sale_order.UserID			= gnActif_user_id
            SQl_add_sale_order.SessioID			= gnIDSession
            SQl_add_sale_order.CustomerID		= id_custom
            SQl_add_sale_order.SocietID			= gnID_societe
            nrc = HExécuteRequêteSQL(SQl_add_sale_order)
            SI PAS nrc ALORS
            	HErreurInfo(hErrComplet)
            sinon
            	//récupère Id commande
            	HLitDernier(Sale_order,IDSale_order)
            	xID_Order = Sale_order.IDSale_order
            fin
            
            //////////////Misè à jour de détaille de session et paiement
            Update_detaille_session(dhDates,xID_Order,nID_esepece)
            
            pour TOUTE LIGNE DE TABLE_Sale_article
            	////////////////////////////////////////////////////////////
            	//sélection de ligne à mettre à jour pour le stock
            	sql_qt_available est une requête SQL = 
            [
            	select <§cs:6§>
            	IDAvailable_quantity,<§cs:6§>
            	Qantity<§cs:6§>
            	from<§cs:6§>
            	Available_quantity<§cs:6§>
            	where<§cs:6§>
            	Available_quantity.IDArticle = {IDProduit}<§cs:6§>
            	And <§cs:6§>
            	Available_quantity.IDWarehouse = {IDStock}<§cs:6§>
            	and<§cs:6§>
            	Available_quantity.IDLot = {lotID}<§cs:6§>
            ]
            	sql_qt_available.IDProduit	= TABLE_Sale_article.COL_IDArticle
            	sql_qt_available.IDStock	= TABLE_Sale_article.COL_IDWarehouse
            	sql_qt_available.lotID		= TABLE_Sale_article.COL_ID_LOT
            	nrc = HExécuteRequêteSQL(sql_qt_available)
            	si pas nrc ALORS
            		HErreurInfo(hErrComplet)
            	sinon si HNbEnr(sql_qt_available) > 0
            		pour tout sql_qt_available
            			xQt_befor 			= sql_qt_available.Qantity
            			xId_stock_available	= sql_qt_available.IDAvailable_quantity
            		FIN
            		////////////////////////////////////
            		///////////////////////////////////
            		//Ajout de mouvement de stock/////
            		/////////////////////////////////
            		xEcart = TABLE_Sale_article.COL_qt_vendue * -1
            		Qsl_add_stock_move est une requête SQL = 
            		[
            			INSERT INTO<§cs:6§>
            			Stock_move<§cs:6§>
            			(date_move,Ref_move,Qantity,IDAvailable_quantity,IDArticle,IDSource,IDestination,Qantity_initial,Ecart,IDUser)<§cs:6§>
            			VALUES<§cs:6§>
            			({Date_Mvt},{ref},{Qte},{id_Available},{Id_product},{SourceID},{DestinationID},{Qte_init},{Ecart},{user})<§cs:6§>
            		]
            		dhDatePc est une dateheure
            		dhDatePc..PartieDate = DateDuJour()
            		dhDatePc..PartieHeure= Maintenant()
            		Qsl_add_stock_move.Date_Mvt			= dhDatePc
            		Qsl_add_stock_move.ref				= "Vente Réf : " + sNum_Commande
            		Qsl_add_stock_move.Qte				= TABLE_Sale_article.COL_qt_vendue
            		Qsl_add_stock_move.id_Available		= xId_stock_available
            		Qsl_add_stock_move.Id_product		= TABLE_Sale_article.COL_IDArticle
            		Qsl_add_stock_move.SourceID			= TABLE_Sale_article.COL_IDWarehouse
            		Qsl_add_stock_move.DestinationID	= xId_destination
            		Qsl_add_stock_move.Qte_init			= xQt_befor
            		Qsl_add_stock_move.Ecart			= TABLE_Sale_article.COL_qt_vendue
            		Qsl_add_stock_move.user				= gnActif_user_id
            		nrc = HExécuteRequêteSQL(Qsl_add_stock_move)
            		SI PAS nrc ALORS
            			HErreurInfo(hErrComplet)
            		FIN
            	SINON SI HNbEnr(sql_qt_available) = 0
            		Qsl_add_stock_move est une requête SQL = 
            		[
            			INSERT INTO<§cs:6§>
            			Stock_move<§cs:6§>
            			(date_move,Ref_move,Qantity,IDAvailable_quantity,IDArticle,IDSource,IDestination,Qantity_initial,Ecart,IDUser)<§cs:6§>
            			VALUES<§cs:6§>
            			({Date_Mvt},{ref},{Qte},{id_Available},{Id_product},{SourceID},{DestinationID},{Qte_init},{Ecart},{user})<§cs:6§>
            		]
            		dhDatePc est une dateheure
            		dhDatePc..PartieDate = DateDuJour()
            		dhDatePc..PartieHeure= Maintenant()
            		Qsl_add_stock_move.Date_Mvt			= dhDatePc
            		Qsl_add_stock_move.ref				= "Vente Réf : " + sNum_Commande
            		Qsl_add_stock_move.Qte				= TABLE_Sale_article.COL_qt_vendue
            		Qsl_add_stock_move.id_Available		= 0
            		Qsl_add_stock_move.Id_product		= TABLE_Sale_article.COL_IDArticle
            		Qsl_add_stock_move.SourceID			= TABLE_Sale_article.COL_IDWarehouse
            		Qsl_add_stock_move.DestinationID	= xId_destination
            		Qsl_add_stock_move.Qte_init			= 0
            		Qsl_add_stock_move.Ecart			= TABLE_Sale_article.COL_qt_vendue
            		Qsl_add_stock_move.user				= gnActif_user_id
            		nrc = HExécuteRequêteSQL(Qsl_add_stock_move)
            		SI PAS nrc ALORS
            			HErreurInfo(hErrComplet)
            		FIN
            	FIN
            	//////////////////////////////////
            	//Mise à jour de stock
            	/////////////////////////////////
            	xQt_affet = xQt_befor - TABLE_Sale_article.COL_qt_vendue
            	sql_update_Stock est une requête SQL = 
            	[
            		UPDATE <§cs:6§>
            		Available_quantity<§cs:6§>
            		SET<§cs:6§>
            		Qantity = {Qte}<§cs:6§>
            		WHERE<§cs:6§>
            		IDAvailable_quantity = {Id_Stock}<§cs:6§>
            	]
            	sql_update_Stock.Qte		= xQt_affet
            	sql_update_Stock.Id_Stock 	= xId_stock_available
            	nrc = HExécuteRequêteSQL(sql_update_Stock)
            	SI PAS nrc ALORS
            		HErreurInfo(hErrComplet)
            	FIN
            
            
            
            /////////////////////////////////////////////////////////////
            	//Ajout des détailles de vente
            	SQL_add_detaille_sale est une requête SQL = 
            [
            	insert into <§cs:6§>
            	Detaille_order<§cs:6§>
            	(Quantity,Discount,Taxe,S_total_HT,S_total_TTC,IDSale_order,IDArticle,Margin)<§cs:6§>
            	values<§cs:6§>
            	({qte},{Discount_val},{Taxe_val},{Sub_total_ht},{sub_total},{Id_sale_order},{Id_product},{Margin_Val})<§cs:6§>
            ]
            	SQL_add_detaille_sale.qte			= TABLE_Sale_article.COL_qt_vendue
            	SQL_add_detaille_sale.Discount_val	= TABLE_Sale_article.COL_Discount_TTL
            	SQL_add_detaille_sale.Taxe_val		= TABLE_Sale_article.COL_Taxe_compute
            	SQL_add_detaille_sale.Sub_total_ht	= TABLE_Sale_article.COL_TTL_HT
            	SQL_add_detaille_sale.sub_total		= TABLE_Sale_article.COL_TTC
            	SQL_add_detaille_sale.Id_sale_order	= xID_Order
            	SQL_add_detaille_sale.Id_product	= TABLE_Sale_article.COL_IDArticle
            	SQL_add_detaille_sale.Margin_Val	= TABLE_Sale_article.COL_marge
            	nrc = HExécuteRequêteSQL(SQL_add_detaille_sale)
            	si pas nrc ALORS
            		HErreurInfo(hErrComplet)
            	FIN
            
            FIN
            
            // mise à jour du numéro de commande
            UPDATENUMSESSION EST une requête SQL = 
            [
            	UPDATE Numero_document SET number_next = {num_next} WHERE label = 'SessionPOS'<§cs:6§>
