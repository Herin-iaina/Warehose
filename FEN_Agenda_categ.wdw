        code : |1+
         bSuppr est un booléen = Faux
         // Supprime les catégories sélectionnées
         POUR i = TABLE_categ_agenda..Occurrence A 1 pas -1
         	si COL_select[i] = 1 ALORS
         		si COL_IDAgendaCatégorie[i] <> 0 ALORS
         			soit nrc = HExécuteRequête(REQ_delete_cat_agenda,hVérifieDoublon+hVérifieIntégrité,COL_IDAgendaCatégorie[i])
         			SI PAS nrc ALORS
         				HErreurInfo(hErrComplet)
         			sinon
         				bSuppr = Vrai // il y avait eu de suppression
         			FIN			
         		FIN
