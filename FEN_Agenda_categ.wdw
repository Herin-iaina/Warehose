         si bSuppr = Vrai ALORS
         	ToastSupprimeTout()
         	ToastAffiche("Suppression terminé",toastCourt,cvMilieu,chCentre,BleuFoncé)
         FIN
         BTN_Supprimer..Visible = Faux
         Echo_data()
