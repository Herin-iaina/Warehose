        enabled : false
      -
        code : |1+
         
         // Vérifie que le nom saisi est unique
         sql_code est une requête SQL = 
         [
         	select 
         		Libellee
         	from
         		AgendaCatégorie
         	where
         		IDAgendaCatégorie <> {id_cat_ag} and Libellee = {lib} and IDSociety = {id_society} 
         ]
         sql_code.lib = COL_Libellé
         sql_code.id_society = gnID_societe
         sql_code.id_cat_ag = COL_IDAgendaCatégorie
         soit nrc = HExécuteRequêteSQL(sql_code)
         si pas nrc ALORS
         	HErreurInfo(hErrComplet)
         sinon
         	si HNbEnr(sql_code) > 0 ALORS
         		ToastSupprimeTout()
         		ToastAffiche("Une autre catégorie porte déjà ce nom", toastCourt, cvMilieu, chCentre) 
         		
         		// Empêche la sauvegarde
         		RepriseSaisie(COL_Libellé)
         	FIN
         FIN
         
        type : 20
      -
        code : |1+
         nCouleurFin 	est un entier
         nCouleurFoncé	est un entier
         iCat			est une image
         nCouleur		est un entier
         nCouleur = COL_Couleur
         
         // Éclairci la couleur
         nCouleurFin 	= TSL(CouleurTeinte(nCouleur), CouleurSaturation(nCouleur), Min(100, CouleurLuminosité(nCouleur) + 10))
         nCouleurFoncé 	= TSL(CouleurTeinte(nCouleur), Max(0, CouleurSaturation(nCouleur) - 20), Max(0,   CouleurLuminosité(nCouleur) - 20))
         
         iCat..Largeur = 16
         iCat..Hauteur = 16
         
         // Début du dessin
         dDébutDessin(iCat, dEffacer + dAvecOpacité)
         dRectangle(0, 0, 15, 15, MagentaClair, MagentaClair)
         
         // Cadre
         dLigne( 2,  1, 13,  1, nCouleurFoncé, 1)
         dLigne( 2, 14, 13, 14, nCouleurFoncé, 1)
         dLigne( 1,  2,  1, 13, nCouleurFoncé, 1)
         dLigne(14,  2, 14, 13, nCouleurFoncé, 1)
         
         // Dégradé
         dRectangleDégradé(2, 2, 14, 14, nCouleurFin, nCouleur)
         
         SI nCouleur = -1 ALORS
         	dLigne(2, 2, 13, 13, RougeFoncé, 1)
         FIN
         
         // Sauvegarde
         COL_image_color = dSauveImageGIF(iCat, enMémoire)
