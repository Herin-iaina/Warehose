               si gbIs_new = Vrai ALORS
               	sql_add est une requête SQL = 
               [
               	insert into 
               		Moyen_paiement	
               		(Journal,Type_Compte,IDSociety)
               	values
               		({journal},{type},{sct})
               ]
               	sql_add.journal = SAI_Journal
               	sql_add.type	= COMBO_Type..ValeurAffichée
               	sql_add.sct		= gnID_societe
               	soit nrc = HExécuteRequêteSQL(sql_add)
               	si pas nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	HLitDernier(Moyen_paiement,IDMoyen_paiement)
               	gId_MP = Moyen_paiement.IDMoyen_paiement
               	gbNo_Empty = Vrai
               sinon
               	sql_update est une requête SQL = 
               [
               	update 
               		Moyen_paiement
               	set
               		Journal		= {journal},
               		Type_Compte	= {type}
               	where
               		IDMoyen_paiement = {id}
               ]
               	sql_update.journal	= SAI_Journal
               	sql_update.type		= COMBO_Type..ValeurAffichée
               	sql_update.id		= gId_MP
               	soit nrc =  HExécuteRequêteSQL(sql_update)
               	si  PAS nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               FIN
               Echo_data()
               GR_All_fields..EnSaisie = Faux
               BTN_Modifier..Visible = Vrai
               BTN_Valider..Grisé	= Faux
               
               gbIs_new = Faux
               gbCheck_update = Faux
               
               ToastSupprimeTout()
               ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
               HLibèreRequête()
