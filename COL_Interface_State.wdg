     name : DemoVersion
     internal_properties : BQAAAAUAAAAlds/MpSiehzM1Fb1nxrFtQhUrimLgqYM5Rzoz/xsFN6Rcs1bZy9CrseQ51/RfnYi7E/2ore8sZI2dx16HNhgEwwdd+tfkKgqwyHGDOzjYmg3HzV0+4YO320maCR6og7/MLAWr5oA12C2Ce7nGQTPh2wxJVnt/Mh3Z3t6Bv+BB9diDtL56
     procedure_id : 1198159463628419543
     type_code : 15
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Description des paramètres d'entrée/sortie de 'DemoVersion' :
      //
      // Syntaxe :
      //DemoVersion ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //// Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle sera répétée en boucle, en attendant 10 minutes et 1 seconde entre chaque appel
      // Chaque appel suivant exécute une seule fois la procédure, sans timer
      // 
      
      procédure DemoVersion()
      
      
      SI gbSDemo = Vrai ALORS
      	soit nNBData = 0
      	
      	nNBData += HNbEnr(Article)
      	nNBData += HNbEnr(Sale_order)
      	nNBData += HNbEnr(Stock_move)
      	nNBData += HNbEnr(Partners)
      	nNBData += HNbEnr(Detaille_order)
      	nNBData += HNbEnr(detaille_purchase)
      	
      	SI nNBData >= 1000 ALORS
      		FEN_Acceuille..Etat = AffichageSeulement
      		FEN_Acceuille.LIB_Titre_de_fenêtre = "Expiration de la version DEMO"
      	FIN
      FIN
     type : 458752
