     name : SelectRuleUsers
     procedure_id : 1197911171473999765
     type_code : 15
     code : |1+
      procédure SelectRuleUsers(gnId_user) <métier>
      
      srSqlRule est une requête SQL = 
      [
      	SELECT <§cs:3§>
      	*<§cs:3§>
      	FROM<§cs:3§>
      	Users_role<§cs:3§>
      	WHERE<§cs:3§>
      	Users_role.IDUser = {IDUsers_}<§cs:3§>
      ]
      
      srSqlRule.IDUsers_ = gnId_user
      HExécuteRequêteSQL(srSqlRule)
      POUR TOUT srSqlRule 
      	COL_ProcéduresGlobales.gtaRulUser[srSqlRule.role] = srSqlRule.value
      FIN
      
