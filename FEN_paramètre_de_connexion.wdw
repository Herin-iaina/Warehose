            -
              code : |1-
               // Cryptage du mot de passe en utilisant l'algorithme AES
               // ------------------------------------------------------------------
               mot_pass = SAI_password
               bufCrypte  = CrypteStandard(mot_pass, bufCles, crypteAES128)
               
               SI bConnexionfail = Faux ALORS // plus de sécurité au cas ou même si ce déjà fait 
               	ToastSupprimeTout()
               	ToastAffiche("Veuillez tester la connexion",toastCourt,cvMilieu,chCentre,BleuFoncé)
               	retour
               FIN
               //Ajout ou modification selon  les contextes, ajout avant 
               si is_new = Vrai ALORS
               	EcranVersFichier(FEN_paramètre_de_connexion,Connexion_param)
               	Connexion_param.password = bufCrypte
               	HEnregistre(Connexion_param)
               	
               	HLitDernier(Connexion_param,IDConnexion_param)
               	id_connex = Connexion_param.IDConnexion_param
               sinon
               	EcranVersFichier(FEN_paramètre_de_connexion,Connexion_param)
               	Connexion_param.password = bufCrypte
               	HModifie(Connexion_param)
               	SelectionConnexion()
               FIN
               
               BTN_Modifier..Visible = Vrai
               GR_All_fields..EnSaisie = Faux
               is_new = Faux
               BTN_Valider..Grisé = Vrai
               //info de la base utiliser
               INIEcrit("BDD", "Install","C/S",fRepEnCours() + "\warehouse.INI")<§aad:0§>
               INIEcrit("BDD", "serveur_mane",SAI_serveur,fRepEnCours() + "\warehouse.INI")
               INIEcrit("BDD", "serveur_port",SAI_serveur_port,fRepEnCours() + "\warehouse.INI")
               INIEcrit("BDD", "base_mane",SAI_bdd,fRepEnCours() + "\warehouse.INI")
               
               ToastSupprimeTout()
               ToastAffiche("Opération réussi ",toastCourt,cvMilieu,chCentre,BleuFoncé)
               
               Ferme()
