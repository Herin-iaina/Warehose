      		bufCrypte				= Connexion_param.password		
      		sRésultat				= DécrypteStandard(bufCrypte, bufCle, crypteAES128)
      		SAI_password			= sRésultat
      		GR_All_fields..EnSaisie	= Faux
      		BTN_Valider..Grisé		= Vrai
      	FIN
      sinon
      	BTN_Modifier..Visible	= Faux
      	BTN_Valider..Grisé		= Vrai
      	
      FIN
      
