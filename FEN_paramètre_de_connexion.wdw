       internal_properties : BQAAAAUAAAA4J400KRPwEOvUzez9QPHdZM1C7sua6j4Kl8h3C4v9VHLa+bLZuhttwMCrg8K75qbAOQnVJ8dKaEIfiWVMSH26PY5RxJMoOB3nNj/7hBu3+hb6u7kAVgSpQbQU5BCb3vabmDTcWwwd5OgBKQ==
       controls :
        -
          name : BTN_tester
          identifier : 0x2e7c78e801a8fea8
          internal_properties : 
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              code : |1-
               Serveur, SGBDs, Provider,InfosEtendues sont des chaînes
               MaConnexion est une Connexion
               
               
               SGBDs = COMBO_sgbd..ValeurAffichée
               
               Sablier()
               Jauge(1)
               SI SGBDs = "HFSQL" ALORS
               	Serveur = ChaîneConstruit("%1:%2",SAI_serveur,SAI_serveur_port)
               	Provider = hAccèsHFClientServeur
               	InfosEtendues = "Infos étendues"
               SINON SI SGBDs = "MySQL"
               	Provider = hAccèsNatifMySQL
               	InfosEtendues = ChaîneConstruit("Server Port=%1",SAI_serveur_port)
               	Serveur = SAI_serveur
               SINON SI SGBDs = "PostgreSQL"
               	Provider = hAccèsNatifPostgreSQL
               	InfosEtendues = ChaîneConstruit("Server Port=%1",SAI_serveur_port)
               	Serveur = SAI_serveur
               FIN
               
               
               SI VérifieExpressionRégulière(SAI_serveur, "([0-9]{1,3}).([0-9]{1,3}).([0-9]{1,3}).([0-9]{1,3})") = Vrai _ET_ SGBDs <> "HFSQL"  ALORS // connexion avec ip 
               	SELON SGBDs // connexion soit à MySQL soit PostgreSQl
               		CAS "MySQL"
               			nNumConnexion = SQLConnecte(SAI_serveur,SAI_user,SAI_password,SAI_bdd,"MySQL","",InfosEtendues)
               			SORTIR SELON
               		CAS "PostgreSQL"
               			nNumConnexion = SQLConnecte(SAI_serveur,SAI_user,SAI_password,SAI_bdd,"PostgreSQL","",InfosEtendues)
               		AUTRE CAS
               			
               	FIN
               		
               	SI nNumConnexion <> 0 ALORS
               		// La connexion s'est bien passée
               //		Info("Connecter avec la base")
               		ToastAffiche("Connexion avec la base de donnée réussi",toastCourt,cvMilieu,BleuFoncé)
               		bConnexionfail = Vrai
               	SINON
               		// La connexion a échoué : affichage d'un message explicatif
               		SQLInfoGene()
               		Erreur("La connexion à la source de données " + SAI_serveur + " a échoué." + RC + ...
               		"Code erreur : " + SQL.Erreur + RC + SQL.MesErreur)
               		bConnexionfail = Faux
               	FIN
               	//	trace("ok")
               SINON // connexion avec nom du serveur
               	// Description de la connexion 
               	MaConnexion..Serveur		= Serveur
               	MaConnexion..Utilisateur	= SAI_user 
               	MaConnexion..MotDePasse		= SAI_password 
               	MaConnexion..BaseDeDonnées	= SAI_bdd
               	MaConnexion..Provider		= Provider 
               	MaConnexion..Accès			= hOLectureEcriture 
               	MaConnexion..InfosEtendues	= InfosEtendues 
               	MaConnexion..OptionsCurseur	= hCurseurClient 
               	bConnexionfail				= HOuvreConnexion(MaConnexion)
               	SI bConnexionfail = Faux ALORS
               		Erreur("Impossible d'établir la connexion avec la base de donnée,"," ré vérifier les paramètres")
               		RETOUR
               	SINON
               //		Info("Connecter avec la base")
               		ToastAffiche("Connexion avec la base de donnée réussi",toastCourt,cvMilieu,BleuFoncé)
               	FIN
               	
               FIN
               
               si bConnexionfail = Vrai ALORS
               	BTN_Valider..Grisé	= Faux
               	gbConnexion_pass	= Vrai
               sinon
               	BTN_Valider..Grisé	= Vrai
               	gbConnexion_pass	= Faux
               FIN
               Sablier(fauxToutSablier)
               Jauge()
