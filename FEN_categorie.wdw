    name : BTN_compute
    identifier : 0x2e2c498f0367d41c
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         // modifications des : prix de vent, marge de produit
         //calcul  de prix
         P_price est un monétaire = 1
         Val_rest est un entier
         soit Nrc = Faux
         
         si grId_categorie <= 0 ALORS
         	retour
         FIN
         Sablier()
         Nrc = HExécuteRequête(REQ_Select_categorie_IdProduct,hRequêteDéfaut,grId_categorie)
         si pas Nrc ALORS
         	HErreurInfo(hErrComplet)
         FIN
         pour TOUT REQ_Select_categorie_IdProduct 
         	P_price = REQ_Select_categorie_IdProduct.Sale_price * SAI_Margin
         	Val_rest = modulo(P_price, SAI_Arrond)
         	si Val_rest < SAI_Venal ALORS
         		P_price = P_price - Val_rest
         	sinon
         		P_price = P_price + (SAI_Arrond - Val_rest)
         	FIN
         	
         	Nrc = HExécuteRequête(REQ_Update_categorie_article,hRequêteDéfaut,P_price,SAI_Margin,REQ_Select_categorie_IdProduct.IDArticle)
         	si pas Nrc ALORS
         		HErreurInfo(hErrComplet)
         	FIN
         	// Mise à jour de la jauge pendant le traitement.
         	Multitâche()   
         FIN
         Sablier(Faux)
        type : 18
      -
        type : 161
        enabled : false
