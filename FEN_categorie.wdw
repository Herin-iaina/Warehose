      PROCEDURE Echo_data_to_sreen()
      
      soit nrc = HExécuteRequête(REQ_Select_categorie_id,hRequêteDéfaut,grId_categorie,gnID_societe)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      POUR TOUT REQ_Select_categorie_id
