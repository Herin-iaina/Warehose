              p_codes :
               -
                 code : |1-
                  
                  
                  sFic est une chaîne
                  // Demande le nom du fichier
                  soit nom = "Export article " + (DateVersChaîne(DateHeureSys(),"JJ_MM_AA HH_MM_SS"))
                  sFic = fSélecteur("", nom, "Choisissez l'emplacement du fichier d'exportation", ...)
                  "Fichiers Excel (*.xlsx) *.xlsx", "xlsx", fselCrée + fselExiste)
                  
                  si sFic <> "" ALORS
                  	Sablier(Vrai)
                  	REQ_ExportArticle.IDSociete = gnID_societe
                  	soit nrc = HExécuteRequête(REQ_ExportArticle)
                  	si nrc = vrai ALORS
                  		JAUGE_Jauge..Visible = Vrai
                  		soit bCrv = HExporteXLS(REQ_ExportArticle,sFic,"*",hExpCréation+hExpNormal,JAUGE_Jauge)
                  		si bCrv = Vrai ALORS
                  			JAUGE_Jauge..Visible = Faux
                  			Success_error_message("Exportation terminer")
                  		sinon
                  			HErreur(hErrPrincipal)
                  		FIN
                  		HLibèreRequête(REQ_ExportArticle)
                  	FIN
                  	Sablier(Faux)
                  sinon
                  	Success_error_message("ERREUR non identifier code 12")
                  FIN
