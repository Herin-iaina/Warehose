   -
     name : Echo_data_to_sreen
     procedure_id : 2174961414245136267
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_data_to_sreen ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_data_to_sreen()
      
      soit nrc = HExécuteRequête(REQ_Select_partners_id,hRequêteDéfaut,grId_partners)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      POUR TOUT REQ_Select_partners_id 
      	images_cripter	= REQ_Select_partners_id.Image
      	SAI_Name		= REQ_Select_partners_id.Name
      	SEL_type		= REQ_Select_partners_id.types_of		
      	INT_Is_customer	= REQ_Select_partners_id.Is_customer
      	INT_Is_supplier	= REQ_Select_partners_id.Is_supplier
      	SAI_Nif			= REQ_Select_partners_id.Nif
      	SAI_Stat		= REQ_Select_partners_id.Stat
      	SAI_Rcs			= REQ_Select_partners_id.Rcs
      	SAI_Cin			= REQ_Select_partners_id.Cin
      	SAI_Adress		= REQ_Select_partners_id.Adress
      	SAI_Phone		= REQ_Select_partners_id.Phone
      	SAI_Email		= REQ_Select_partners_id.Email
      	INT_Stat		= REQ_Select_partners_id.actifs
      	COMBO_Civilité  = REQ_Select_partners_id.sex
      FIN
      IMG_photos..Etat = AffichageSeulement
      GR_All_fields..EnSaisie = Faux	
      
      // affichage du bouton suppression d'image s'il y a dans la champ image
      SI images_cripter = "" ALORS
      	IMG_photos = IMG_photos_default
      	BTN_sup..Visible = Faux
      SINON	
      	IMG_photos = dChargeImage( Décode(images_cripter, encodeBASE64) )
      	images_cripter = IMG_photos
      	BTN_sup..Visible = Vrai
      FIN
     type : 458752
