     p_codes :
      -
        code : |1+
         PROCEDURE MaFenêtre()
      -
        code : |1-
         procédure interne Actualiser()
         	SQLStok est une requête SQL = 
         [
         	select 
         	Warehouse.IDWarehouse,
         	Warehouse.Name as Name_,
         	Article.IDArticle,
         	SUM(Available_quantity.Qantity) AS QteTotal
         	from
         	Warehouse
         	inner join Available_quantity
         	on Warehouse.IDWarehouse = Available_quantity.IDWarehouse
         	inner join article
         	on Available_quantity.IDArticle = Article.IDArticle
         	where
         	Warehouse.IDSociety = {IDSociete}
         	and 
         	Article.Stat = 1
         	group by
         	Warehouse.IDWarehouse,
         	Warehouse.Name,
         	Article.IDArticle
         	order by Warehouse.Name desc
         ]
         
         	SQLStok.IDSociete = gnID_societe
         	soit bNrc = HExécuteRequêteSQL(SQLStok)
         	soit i = 1
         	soit xStockTotal = 0n0
         	taValReq est un tableau associatif de numériques
         	grSupprimeTout(GRF_Stock)
         	si bNrc = Faux ALORS
         		HErreurInfo(hErrComplet)
         	sinon
         		pour TOUT SQLStok 
         			xStockTotal = (SQLStok.QteTotal > 0 ? SQLStok.QteTotal sinon 0)
         			taValReq[SQLStok.Name_] += xStockTotal
         		FIN
         		HLibèreRequête(SQLStok)		
         		pour tout xValStock, sNameStock de taValReq
         			grAjouteDonnée(GRF_Stock,1,i,xValStock)
         			grEtiquetteCatégorie(GRF_Stock,i,sNameStock)
         			i ++
         		FIN
         		grDessine(GRF_Stock)
         		
         	FIN
         	SupprimeTout(taValReq)
         fin
         
         Actualiser()
         Timer(Actualiser,Hasard(6,12)* 400)
