     p_codes :
      -
        code : |1-
         PROCEDURE MaFenêtre()
         LIB_CetteSemaine = ChaîneConstruit( LIB_CetteSemaine, DateVersChaîne(DateDuJour(),maskDateSystème))
      -
        code : |1+
         procédure interne actualiser()
         	
         	SqlnombreVente est une requête SQL = 
         [
         	select 
         	count(IDSale_order) as Nombre
         	from 
         	Sale_order
         	where
         	IDSociety = {IDSociete}
         	and
         	date_order like %{dateOrder}% 
         ]
         
         	SqlnombreVente.IDSociete = gnID_societe
         	SqlnombreVente.dateOrder = DateDuJour()
         	HExécuteRequêteSQL(SqlnombreVente)
         	pour TOUT SqlnombreVente 		
         		LIB_135  = SqlnombreVente.Nombre
         	fin
         fin
         
         actualiser()
         // Choix de la fréquence de rafraîchissement
         Timer(actualiser,Hasard(6,12) * 100)
