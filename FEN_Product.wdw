               	REQ_Add_product.IDunit_of_mesur			= COMBO_UOM.COL_IDunit_of_mesur
               	REQ_Add_product.IDUOM					= COMBO_UOM_purchase.COL_IDunit_of_mesur
               	REQ_Add_product.Images					= Encode(images_cripter, encodeBASE64)
               	REQ_Add_product.Margin					= (SAI_Margin > 0 ? SAI_Margin sinon 1)
               	REQ_Add_product.Methode_of_tracability	= SEL_Suivi
               	REQ_Add_product.Name_products			= SAI_Name_product
               	REQ_Add_product.Param_IDSociety			= gnID_societe
               	REQ_Add_product.Purchase_price			= SAI_Purchase_price
               	REQ_Add_product.Sale_price				= SAI_Sale_price
               	REQ_Add_product.Stat					= INT_Stat
               	REQ_Add_product.Tracability				= SEL_Suivi
               	REQ_Add_product.Type_product			= COMBO_Product_type..ValeurAffichée
               	REQ_Add_product.Param_IDTaxe			= COMBO_Taxe_Sale.COL_ID_taxe
               	REQ_Add_product.Param_Id_TaxePurchase	= COMBO_Taxe_Purchase.COL_ID_taxe
               	REQ_Add_product.Param_deconditioning	= INT_decomditionnement
               	
               	soit nrc = HExécuteRequête(REQ_Add_product)
               	si pas nrc  ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               	HLibèreRequête(REQ_Add_product)
               //	VariableRAZ(gnId_article)
               	HLitDernier(Article,IDArticle,hBlocageNon)
               	Soit id_article = Article.IDArticle
               	gnId_article = id_article
               
               	// ajout de la quantité initial si l'article est stockable
               	si COMBO_Product_type..ValeurAffichée = "Stockable" ALORS
               		Add_availlable_Qt est une requête SQL = 
               [
               	INSERT INTO Available_quantity<§cs:6§>
               	( <§cs:6§>
               		IDArticle,	<§cs:6§>
               		IDLot,	<§cs:6§>
               		IDWarehouse,	<§cs:6§>
               		Qantity<§cs:6§>
               	)<§cs:6§>
               VALUES<§cs:6§>
               	( <§cs:6§>
               		{Param_IDArticle},	<§cs:6§>
               		{Param_IDLot},	<§cs:6§>
               		{Param_IDWarehouse},	<§cs:6§>
               		{Param_Qantity}<§cs:6§>
               	 )<§cs:6§>
               ]
               		Add_availlable_Qt.Param_IDArticle	= gnId_article
               		Add_availlable_Qt.Param_IDLot		= 0
               		Add_availlable_Qt.Param_IDWarehouse	= gnID_Ware_House
               		Add_availlable_Qt.Param_Qantity		= 0
               		nrc = HExécuteRequêteSQL(Add_availlable_Qt)
               		si pas nrc ALORS
               			HErreurInfo(hErrComplet)
               		FIN
               		HLibèreRequête(Add_availlable_Qt)
               	FIN
               sinon
               	Update_article()	
               FIN
               
               // ajout des éléments du table vers le fichier de donnée product_per_partners
               nCompte est un entier = TableOccurrence(TABLE_Supplier)
               
               SI nCompte >= 1 ALORS
               	POUR TOUTE LIGNE DE TABLE_Supplier
               		soit Nrc = faux
               		SI TABLE_Supplier.COL_id = 0 _et_ TABLE_Supplier.COL_id_Supplier > 0 ALORS // ajout
               			Nrc = HExécuteRequête(REQ_Add_product_per_partners,hVérifieIntégrité,gnId_article,TABLE_Supplier.COL_id_Supplier,TABLE_Supplier.COL_Prix, ...
               			TABLE_Supplier.COL_Quantité,TABLE_Supplier.COL_UoM_id)
               			si pas Nrc ALORS
               				HErreurInfo(hErrComplet)
               			FIN
               		sinon // modification
               			Nrc = HExécuteRequête(REQ_Update_product_per_partners,hVérifieIntégrité,gnId_article,TABLE_Supplier.COL_id_Supplier,TABLE_Supplier.COL_Prix,...
               			TABLE_Supplier.COL_Quantité,TABLE_Supplier.COL_id,TABLE_Supplier.COL_UoM_id)
               			si pas Nrc ALORS
               				HErreurInfo(hErrComplet)
               			FIN
               			
               		FIN
               		
               	FIN
               FIN
               
               // suppression des fournisseur d'article
               POUR TOUT iD  DE tabId_table_fournisseur avec iD > 0 DepuisDébut
               	soit nrc = HExécuteRequête(REQ_Delete_PPP,hRequêteDéfaut,iD)
               	si pas nrc ALORS
               		HErreurInfo(hErrComplet)
               	FIN
               FIN
               SupprimeTout(tabId_table_fournisseur)
               TableSupprimeTout(TABLE_Supplier)
               
               echo_data_to_screen()
               
               echo_table_data()
               
               
               GR_All_fields..EnSaisie = Faux
