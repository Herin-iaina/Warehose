   -
     name : Echo_quantity
     procedure_id : 2206840844026452956
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Echo_quantity ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Echo_quantity()
      soit rQt_dispo 	= 0.00
      soit rQt_prev	= 0.00
      SI COMBO_Product_type..Valeur = 1 ALORS
      	sq_sum_quantity est une requête SQL = 
      	[
      		SELECT <§cs:6§>
      		IDArticle,<§cs:6§>
      		SUM(Qantity) AS Quantite<§cs:6§>
      		FROM<§cs:6§>
      		Available_quantity<§cs:6§>
      		inner join<§cs:6§>
      		warehouse<§cs:6§>
      		on Available_quantity.IDWarehouse = Warehouse.IDWarehouse<§cs:6§>
      		WHERE<§cs:6§>
      		IDArticle = {id_product} <§cs:6§>
      		AND<§cs:6§>
      		Warehouse.IDSociety	 ={Id_Societe}<§cs:6§>
      		GROUP BY IDArticle<§cs:6§>
      	]
      	sq_sum_quantity.id_product	= gnId_article
      	sq_sum_quantity.Id_Societe	= gnID_societe
      	soit nrc = HExécuteRequêteSQL(sq_sum_quantity)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	SINON
      		pour tout sq_sum_quantity
      			rQt_dispo	= sq_sum_quantity.Quantite	
      			LIB_Qt 		= rQt_dispo
      		FIN	
      	FIN
      
      	sql_sum_quantity_prev est une requête SQL = 
      	[
      		SELECT<§cs:6§>
      		((Detaille_operation_move.Quantity * unit_of_mesur.ratio) / uom_prod.ratio) AS Quantite_prev<§cs:6§>
      		FROM<§cs:6§>
      		Detaille_operation_move<§cs:6§>
      		INNER JOIN<§cs:6§>
      		Operation_type_move<§cs:6§>
      		ON Operation_type_move.IDOperation_type_move = Detaille_operation_move.IDDetaille_operation_move<§cs:6§>
      		INNER JOIN<§cs:6§>
      		Article<§cs:6§>
      		ON Article.IDArticle = Detaille_operation_move.IDArticle<§cs:6§>
      		INNER JOIN<§cs:6§>
      		unit_of_mesur<§cs:6§>
      		ON unit_of_mesur.IDunit_of_mesur = Detaille_operation_move.IDunit_of_mesur<§cs:6§>
      		INNER JOIN<§cs:6§>
      		unit_of_mesur uom_prod<§cs:6§>
      		ON uom_prod.IDunit_of_mesur = Article.IDUOM_P<§cs:6§>
      		WHERE<§cs:6§>
      		Detaille_operation_move.IDArticle = {Param_IDArticle}<§cs:6§>
      		AND<§cs:6§>
      		(<§cs:6§>
      		Operation_type_move.Etat NOT LIKE 'Terminé'<§cs:6§>
      		)<§cs:6§>
      	]
      	sql_sum_quantity_prev.Param_IDArticle = gnId_article
      	nrc = HExécuteRequêteSQL(sql_sum_quantity_prev)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	SINON
      		pour tout sql_sum_quantity_prev
      			rQt_prev += sql_sum_quantity_prev.Quantite_prev
      		FIN
      	FIN
      	rQt_prev = (rQt_prev > 0 ? rQt_prev + rQt_dispo sinon rQt_prev )
      	LIB_Qt_prev = rQt_prev
      fin
     type : 458752
