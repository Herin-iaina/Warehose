     name : sale_price_compute
     internal_properties : BQAAAAUAAACeL9IfDOSJoAqKpQVEUEtM0zYQgnvZBbNgtoMlRS7UJvggLML63ODnnNUyjZJd5/mLPJjc8iAzBvJWTDaWxBJObaTt8VrETnqDMCrVYWJXPVzeNyoFOexf3jG3TE/tv5qihTfaBCTUIwRFluYpXoVwl1l0MKV/TknzliALCKctNAF/0ks35SQlHDesUej3ElUUjoPntXQXZRQ9jVnSKKAOf7oT5A==
     procedure_id : 2175622761911915352
     type_code : 14
     code : |1+
      // Résumé : <Calcul de prix de vente en fonction prix d'achat; unité des mesures; 
      // Syntaxe :
      //[ <Résultat> = ] sale_price_compute (<P_Price>, <Margin>, <UOM>, <P_UOM>)
      //
      // Paramètres :
      //	P_Price : <indiquez ici le rôle de P_Price>
      //	Margin : <indiquez ici le rôle de Margin>
      //	UOM : <indiquez ici le rôle de UOM>
      //	P_UOM : <indiquez ici le rôle de P_UOM>
      // Valeur de retour :
      // 	variant : // 	Aucune
      //
      // Exemple :
      // un exemple d'utilisation.
      // prix d'achat  = 10 $  ; Marge 1.5 ; UOM = 5; UOM achat = 100
      // prix de vente  = 10 * 1.5 * 5 / 100
      //
      PROCEDURE sale_price_compute(P_Price, Margin,  UOM, P_UOM ,gnArrondir_default, gnVenal_default)
      	rS_price est un réel
      	reste est un entier sur 8 octets
      	si P_Price > 0 ALORS
      		rS_price = P_Price * Margin * UOM / P_UOM
      	sinon
      		rS_price = gnArrondir_default
      	FIN
      	
      	si rS_price > gnArrondir_default  ALORS
      		reste = modulo(rS_price,gnArrondir_default)
      		si reste > gnVenal_default ALORS
      			rS_price = rS_price + (gnArrondir_default - reste)
      		sinon
      			rS_price = rS_price - reste
      		FIN
      	FIN
      	
      	renvoyer Arrondi(rS_price) 
