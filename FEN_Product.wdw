      //
      PROCEDURE echo_data_to_screen()
       nTyp_art est un entier = 2
       nIndcs_taxe_S, nIndcs_taxe_P sont des entiers
       
       //Afficher les taxes
      Echo_taxe_sale()
      Echo_taxe_Purchase()
      //projection des données sur l'écran
      soit nrc = HExécuteRequête(REQ_Select_product_Id,hVérifieIntégrité+hRequêteDéfaut,gnId_article,gnID_societe)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      POUR TOUT REQ_Select_product_Id 
      	SAI_Name_product 		= REQ_Select_product_Id.name_product
      	nTyp_art 				= COMBO_Product_type.Cherche(REQ_Select_product_Id.Type_product,Vrai)
      	COMBO_Product_type 		= nTyp_art
      	INT_Can_be_sale 		= REQ_Select_product_Id.Can_be_sale
      	INT_Can_be_purchase 	= REQ_Select_product_Id.Can_be_purchase
      	SAI_Description 		= REQ_Select_product_Id.Description
      	SAI_Purchase_price 		= REQ_Select_product_Id.Purchase_price
      	SAI_Margin 				= REQ_Select_product_Id.Margin
      	SAI_Sale_price 			= REQ_Select_product_Id.Sale_price
      	COMBO_compute_methode 	= REQ_Select_product_Id.Compute_methode
      	Categorie(REQ_Select_product_Id.IDCategory)
      	UOM_searche(REQ_Select_product_Id.IDunit_of_mesur,REQ_Select_product_Id.IDUOM_P)
      //	UOM_searche(COMBO_UOM_purchase,REQ_Select_product_Id.IDUOM_P)
      	SEL_Suivi 				= REQ_Select_product_Id.Tracability
      	INT_Stat 				= REQ_Select_product_Id.Stat
      	INT_decomditionnement 	= REQ_Select_product_Id.Can_be_deconditioning
      	images_cripter = REQ_Select_product_Id.Image
      	nIndcs_taxe_S = TableCherche(COMBO_Taxe_Sale.COL_ID_taxe,REQ_Select_product_Id.IDTaxe,rechercheIdentique)
      	nIndcs_taxe_P = TableCherche(COMBO_Taxe_Purchase.COL_ID_taxe,REQ_Select_product_Id.Id_TaxePurchase,rechercheIdentique)
      	nIndcs_taxe_S = (nIndcs_taxe_S > 0 ? nIndcs_taxe_S sinon 1)
      	nIndcs_taxe_P = (nIndcs_taxe_P > 0 ? nIndcs_taxe_P SINON 1)
      	ListeSelectPlus(COMBO_Taxe_Sale,nIndcs_taxe_S)
      	ListeSelectPlus(COMBO_Taxe_Purchase,nIndcs_taxe_P)
      //	trace(images_cripter)
      FIN
      Can_Be_sale()
      Can_be_purchase()
      Product_type()
      // affichage du bouton suppression d'image s'il y a dans la champ image
      si images_cripter = "" ALORS
      	IMG_photos = IMG_photos_default
      	BTN_sup..Visible = Faux
      sinon	
