      //
      // 	udm_p : <indiquez ici le rôle de udm_p>
      PROCEDURE UOM_searche(nId_UdM,nId_UdM_P )
      
      ResCherche_s , nResCherche_p sont entier
      
      
      // ouvre l'unité de mesure pour modifier ou ajouter
      //Ouvre(FEN_UOM,Faux,nId_UdM)
