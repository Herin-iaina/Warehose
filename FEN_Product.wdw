      	Ouvre(FEN_product_per_supplyer,Vrai,0,0,0,0)
      	POUR TOUT stNbligns DE gtabLigneS
      		//		Trace(stNbligns.supplier,stNbligns.quantity,stNbligns.Udm,stNbligns.moPrix )
      		
      		SI stNbligns.suplpplier_id > 0 ALORS
      			TableAjouteLigne(TABLE_Supplier,0,stNbligns.suplpplier_id,stNbligns.supplier,stNbligns.quantity,stNbligns.Udm_id,stNbligns.Udm,stNbligns.moPrix)
      		fin
      	FIN
      SINON SI TABLE_Supplier.COL_id_Supplier > 0 _ET_ TABLE_Supplier.COL_UoM_id > 0 _ET_ TableSelectOccurrence(TABLE_Supplier) > 0 _ET_ nIdt > 0
      	Ouvre(FEN_product_per_supplyer,Faux,TABLE_Supplier.COL_id_Supplier,TABLE_Supplier.COL_Quantité,TABLE_Supplier.COL_UoM_id,TABLE_Supplier.COL_Prix)
      	POUR TOUT stNbligns DE gtabLigneS
      		TABLE_Supplier[nIdt].COL_id_Supplier = stNbligns.suplpplier_id
      		TABLE_Supplier[nIdt].COL_Fournisseur = stNbligns.supplier
      		TABLE_Supplier[nIdt].COL_Quantité = stNbligns.quantity
      		TABLE_Supplier[nIdt].COL_UoM_id = stNbligns.Udm_id
      		TABLE_Supplier[nIdt].COL_Unité_de_mesure = stNbligns.Udm
      		TABLE_Supplier[nIdt].COL_Prix = stNbligns.moPrix
      	FIN
      FIN
