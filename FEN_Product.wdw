     name : Available_quantity
     procedure_id : 2205344206308759747
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Available_quantity ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Available_quantity()
      
      
      SI COMBO_Product_type..ValeurAffichée = "Stockable" ALORS
      	// vérifier si l'article n a pas de quantité par defaut
      	sql_check_qt est une requête SQL = 
      	[
      		SELECT <§cs:6§>
      		IDArticle,<§cs:6§>
      		sum(Qantity) as quantité<§cs:6§>
      		FROM<§cs:6§>
      		Available_quantity<§cs:6§>
      		WHERE<§cs:6§>
      		IDArticle = {ArticleID} <§cs:6§>
      		and IDWarehouse ={id_Stock}<§cs:6§>
      		group by IDArticle<§cs:6§>
      	]
      	sql_check_qt.ArticleID = gnId_article
      	sql_check_qt.id_Stock  = gnID_Ware_House
      	soit nrc = HExécuteRequêteSQL(sql_check_qt)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	SINON SI HNbEnr(sql_check_qt) = 0 _et_ SEL_Suivi = 0
      		// ajout de qua quantité initial si l'article est stockable
      		REQ_Add_availlable_Qt.Param_IDArticle 	= gnId_article
      		REQ_Add_availlable_Qt.Param_IDLot		= 0
      		REQ_Add_availlable_Qt.Param_IDWarehouse	= gnID_Ware_House
      		REQ_Add_availlable_Qt.Param_Qantity		= 0	
      		nrc = HExécuteRequête(REQ_Add_availlable_Qt)
      		SI PAS nrc ALORS
      			HErreurInfo(hErrComplet)
      		FIN
      		HLibèreRequête(REQ_Add_availlable_Qt)
      		
      	sinon si HNbEnr(sql_check_qt) > 0
      		
      		xQt_P est un numérique = sql_check_qt.quantité
      		si SEL_Suivi = 0  ALORS
      			sql_lo_qt est une requête SQL = 
      [
      		select <§cs:6§>
      		IDArticle<§cs:6§>
      		FROM<§cs:6§>
      		Available_quantity<§cs:6§>
      		WHERE<§cs:6§>
      		IDArticle = {ArticleID} <§cs:6§>
      		AND IDWarehouse ={id_Stock}<§cs:6§>
      		and IDLot > 0<§cs:6§>
      ]
      			sql_lo_qt.ArticleID = gnId_article
      			sql_lo_qt.id_Stock  = gnID_Ware_House
      			nrc = HExécuteRequêteSQL(sql_lo_qt)
      		si pas nrc ALORS
      			HErreurInfo(hErrComplet)
      		fin
      				
      			SI HNbEnr(sql_lo_qt) > 0 ALORS
      				mettre_les_quantité_de_lot_0()
      				// ajout de qua quantité initial si l'article est stockable	
      				nrc = HExécuteRequête(REQ_Add_availlable_Qt,hVérifieIntégrité+hRequêteDéfaut,gnId_article,0,gnID_Ware_House,xQt_P)		
      				SI PAS nrc ALORS
      					HErreurInfo(hErrComplet)
      				FIN
      				HLibèreRequête(REQ_Add_availlable_Qt)
      			FIN
      			HLibèreRequête(sql_lo_qt)
      		sinon 
      			//on supprime tous le quantité sans lot on garde que ceux qui en a pour le suivi
      			sql_delete_qt est une requête SQL = 
      			[
      				delete from<§cs:6§>
      				Available_quantity<§cs:6§>
      				where<§cs:6§>
      				IDArticle = {ArticleID} <§cs:6§>
      				-- AND IDWarehouse = {id_Stock}<§cs:6§>
      				AND IDLot = 0	<§cs:6§>
      			]
      			sql_delete_qt.ArticleID	= gnId_article
      //			sql_delete_qt.id_Stock = gnID_Ware_House
      			nrc = HExécuteRequêteSQL(sql_delete_qt)
      			si pas nrc ALORS
      				HErreurInfo(hErrComplet)
      			FIN
      			HLibèreRequête(sql_delete_qt)
      		FIN
      	FIN
      	HLibèreRequête(sql_check_qt)
      sinon
      	//on supprime tous le quantité l'article n'est pas stockable
      	sql_delete_qt est une requête SQL = 
      	[
      				DELETE FROM<§cs:6§>
      				Available_quantity<§cs:6§>
      				WHERE<§cs:6§>
      				IDArticle = {ArticleID} <§cs:6§>
      				AND IDWarehouse = {id_Stock}	<§cs:6§>
      	]
      	sql_delete_qt.ArticleID	= gnId_article
      	sql_delete_qt.id_Stock = gnID_Ware_House
      	soit nrc = HExécuteRequêteSQL(sql_delete_qt)
      	SI PAS nrc ALORS
      		HErreurInfo(hErrComplet)
      	FIN
      	HLibèreRequête(sql_delete_qt)
      FIN
     type : 458752
