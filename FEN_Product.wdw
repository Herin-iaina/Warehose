      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE mettre_les_quantité_de_lot_0()
      
      sql_delete_Qte_lot est une requête SQL = 
      [
      	delete from 
      	Available_quantity
      	WHERE
      	Available_quantity.IDArticle = {ArticleID} 
      	AND Available_quantity.IDWarehouse ={id_Stock}
      	AND Available_quantity.IDLot > 0
      ]
      sql_delete_Qte_lot.ArticleID = gnId_article
      sql_delete_Qte_lot.id_Stock  = gnID_Ware_House
      //remettre à 0 tout les quantité de lot quant il y a parce que on a plus besoin l'article n'a pas de suivi de lot 
      soit nrc= HExécuteRequêteSQL(sql_delete_Qte_lot)
      SI PAS nrc ALORS
      	HErreurInfo(hErrComplet)
      FIN
      HLibèreRequête(sql_delete_Qte_lot)
